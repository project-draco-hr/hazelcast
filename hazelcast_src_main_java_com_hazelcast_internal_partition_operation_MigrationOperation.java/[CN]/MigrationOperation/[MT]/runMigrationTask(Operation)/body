{
  MigrationAwareService service=op.getService();
  PartitionMigrationEvent event=new PartitionMigrationEvent(MigrationEndpoint.DESTINATION,migrationInfo.getType(),migrationInfo.getPartitionId(),migrationInfo.getReplicaIndex(),migrationInfo.getKeepReplicaIndex());
  service.beforeMigration(event);
  op.beforeRun();
  op.run();
  op.afterRun();
}
