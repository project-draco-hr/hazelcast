{
  MigrationAwareService service=op.getService();
  PartitionMigrationEvent event=new PartitionMigrationEvent(MigrationEndpoint.DESTINATION,migrationInfo.getPartitionId(),migrationInfo.getDestinationCurrentReplicaIndex(),migrationInfo.getDestinationNewReplicaIndex());
  service.beforeMigration(event);
  op.beforeRun();
  op.run();
  op.afterRun();
}
