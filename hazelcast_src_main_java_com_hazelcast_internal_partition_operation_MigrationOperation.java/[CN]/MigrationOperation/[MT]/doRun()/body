{
  if (startMigration()) {
    try {
      migrate();
    }
 catch (    Throwable e) {
      success=false;
      failureReason=e;
      getLogger().severe("Error while processing " + migrationInfo,e);
    }
 finally {
      afterMigrate();
    }
  }
 else {
    success=false;
    logMigrationCancelled();
  }
}
