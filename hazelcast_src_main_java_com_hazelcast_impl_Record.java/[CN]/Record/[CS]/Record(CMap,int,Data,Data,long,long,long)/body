{
  super();
  this.recordEntry=new RecordEntry(this);
  this.cmap=cmap;
  this.blockId=blockId;
  this.setKey(key);
  this.setValue(value);
  this.setCreationTime(System.currentTimeMillis());
  this.setExpirationTime(ttl);
  this.maxIdleMillis=(maxIdleMillis == 0) ? Long.MAX_VALUE : maxIdleMillis;
  this.setVersion(0);
  this.id=id;
}
