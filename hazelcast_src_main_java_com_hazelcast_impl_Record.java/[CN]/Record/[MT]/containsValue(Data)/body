{
  if (this.getValue() != null) {
    return this.getValue().equals(value);
  }
 else   if (getMultiValues() != null) {
    int count=getMultiValues().size();
    for (int i=0; i < count; i++) {
      if (value.equals(getMultiValues().get(i))) {
        return true;
      }
    }
  }
  return false;
}
