{
  checkTimeout();
  try {
    if (state != ACTIVE) {
      throw new TransactionNotActiveException("Transaction is not active");
    }
    ClientMessage request=XATransactionPrepareParameters.encode(txnId);
    invoke(request);
    state=PREPARED;
  }
 catch (  Exception e) {
    state=ROLLING_BACK;
    throw ExceptionUtil.rethrow(e);
  }
}
