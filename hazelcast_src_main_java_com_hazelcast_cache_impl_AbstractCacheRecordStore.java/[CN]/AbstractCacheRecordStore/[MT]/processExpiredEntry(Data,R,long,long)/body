{
  final boolean isExpired=isExpiredAt(expiryTime,now);
  if (!isExpired) {
    return record;
  }
  if (isStatisticsEnabled()) {
    statistics.increaseCacheExpiries(1);
  }
  records.remove(key);
  if (isEventsEnabled) {
    publishEvent(CacheEventType.EXPIRED,key,null,toEventData(record),false,IGNORE_COMPLETION);
  }
  return null;
}
