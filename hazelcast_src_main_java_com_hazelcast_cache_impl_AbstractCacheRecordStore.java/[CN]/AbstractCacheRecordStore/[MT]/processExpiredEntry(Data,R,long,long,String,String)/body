{
  final boolean isExpired=isExpiredAt(expiryTime,now);
  if (!isExpired) {
    return record;
  }
  if (isStatisticsEnabled()) {
    statistics.increaseCacheExpiries(1);
  }
  doRemoveRecord(key,source);
  if (isEventsEnabled()) {
    publishEvent(createCacheExpiredEvent(toEventData(key),toEventData(record),CacheRecord.EXPIRATION_TIME_NOT_AVAILABLE,origin,IGNORE_COMPLETION));
  }
  return null;
}
