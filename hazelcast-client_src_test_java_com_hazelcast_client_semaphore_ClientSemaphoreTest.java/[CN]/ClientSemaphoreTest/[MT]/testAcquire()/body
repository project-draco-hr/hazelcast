{
  assertEquals(10,semaphore.drainPermits());
  final CountDownLatch latch=new CountDownLatch(1);
  new Thread(){
    public void run(){
      try {
        semaphore.acquire();
        latch.countDown();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
.start();
  Thread.sleep(1000);
  semaphore.release(2);
  assertTrue(latch.await(10,TimeUnit.SECONDS));
  assertEquals(1,semaphore.availablePermits());
}
