{
  Collection coll=getOrCreateCollection();
  List list=dataList;
  if (!isBinary()) {
    list=new ArrayList(dataList.size());
    for (    Data data : dataList) {
      list.add(toObject(data));
    }
  }
  if (retain) {
    response=coll.retainAll(list);
  }
 else {
    response=coll.removeAll(list);
  }
}
