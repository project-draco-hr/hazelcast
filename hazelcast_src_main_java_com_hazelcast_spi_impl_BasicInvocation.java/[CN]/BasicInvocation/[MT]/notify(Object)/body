{
  Object response=resolveResponse(obj);
  if (response == RETRY_RESPONSE) {
    handleRetryResponse();
    return;
  }
  if (response == WAIT_RESPONSE) {
    handleWaitResponse();
    return;
  }
  if (response instanceof NormalResponse) {
    handleNormalResponse((NormalResponse)response);
    return;
  }
  invocationFuture.set(response);
}
