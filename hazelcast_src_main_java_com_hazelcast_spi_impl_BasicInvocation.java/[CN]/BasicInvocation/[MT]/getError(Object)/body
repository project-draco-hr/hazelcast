{
  if (obj == null) {
    return null;
  }
  if (obj instanceof Throwable) {
    return (Throwable)obj;
  }
  if (!(obj instanceof Response)) {
    return null;
  }
  Response response=(Response)obj;
  if (!(response.response instanceof Throwable)) {
    return null;
  }
  return (Throwable)response.response;
}
