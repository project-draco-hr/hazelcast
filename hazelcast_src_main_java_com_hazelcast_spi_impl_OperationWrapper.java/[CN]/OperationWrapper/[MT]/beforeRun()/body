{
  final NodeEngine nodeEngine=getNodeEngine();
  operation=(Operation)nodeEngine.toObject(operationData);
  operation.setNodeEngine(nodeEngine).setCaller(getCaller()).setConnection(getConnection()).setServiceName(getServiceName()).setPartitionId(getPartitionId()).setReplicaIndex(getReplicaIndex()).setResponseHandler(getResponseHandler());
  OperationAccessor.setCallId(operation,getCallId());
}
