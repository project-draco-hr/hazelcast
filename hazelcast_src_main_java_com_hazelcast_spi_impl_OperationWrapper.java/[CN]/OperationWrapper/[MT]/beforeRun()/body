{
  final NodeService nodeService=getNodeService();
  operation=(Operation)nodeService.toObject(operationData);
  operation.setNodeService(nodeService).setCaller(getCaller()).setCallId(getCallId()).setConnection(getConnection()).setServiceName(getServiceName()).setPartitionId(getPartitionId()).setReplicaIndex(getReplicaIndex()).setResponseHandler(getResponseHandler());
}
