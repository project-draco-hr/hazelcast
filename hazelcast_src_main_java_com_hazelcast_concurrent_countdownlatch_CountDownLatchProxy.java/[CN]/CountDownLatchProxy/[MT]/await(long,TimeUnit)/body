{
  final NodeEngine nodeEngine=getNodeEngine();
  Future f=nodeEngine.getOperationService().invokeOnPartition(CountDownLatchService.SERVICE_NAME,new AwaitOperation(name,getTimeInMillis(timeout,unit)),partitionId);
  try {
    return (Boolean)nodeEngine.toObject(f.get());
  }
 catch (  ExecutionException e) {
    throw ExceptionUtil.rethrowAllowInterrupted(e);
  }
}
