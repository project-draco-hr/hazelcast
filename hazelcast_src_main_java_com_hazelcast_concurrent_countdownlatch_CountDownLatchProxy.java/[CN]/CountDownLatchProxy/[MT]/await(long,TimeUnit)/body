{
  final NodeEngine nodeEngine=getNodeEngine();
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(CountDownLatchService.SERVICE_NAME,new AwaitOperation(name,getTimeInMillis(timeout,unit)),partitionId).build();
  try {
    return (Boolean)nodeEngine.toObject(inv.invoke().get());
  }
 catch (  ExecutionException e) {
    throw ExceptionUtil.rethrowAllowInterrupted(e);
  }
}
