{
  final NodeEngine nodeEngine=getNodeEngine();
  CountDownOperation op=new CountDownOperation(name);
  Future f=nodeEngine.getOperationService().invokeOnPartition(CountDownLatchService.SERVICE_NAME,op,partitionId);
  try {
    f.get();
  }
 catch (  Exception e) {
    throw ExceptionUtil.rethrow(e);
  }
}
