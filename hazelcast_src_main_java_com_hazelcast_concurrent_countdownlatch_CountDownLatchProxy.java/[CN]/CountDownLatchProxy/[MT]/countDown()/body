{
  final NodeEngine nodeEngine=getNodeEngine();
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(CountDownLatchService.SERVICE_NAME,new CountDownOperation(name),partitionId).build();
  try {
    inv.invoke().get();
  }
 catch (  Exception e) {
    ExceptionUtil.rethrow(e);
  }
}
