{
  final NodeEngine nodeEngine=getNodeEngine();
  Future f=nodeEngine.getOperationService().invokeOnPartition(CountDownLatchService.SERVICE_NAME,new SetCountOperation(name,count),partitionId);
  try {
    return (Boolean)nodeEngine.toObject(f.get());
  }
 catch (  Exception e) {
    throw ExceptionUtil.rethrow(e);
  }
}
