{
  final NodeEngine nodeEngine=getNodeEngine();
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(CountDownLatchService.SERVICE_NAME,new GetCountOperation(name),partitionId).build();
  try {
    return (Integer)nodeEngine.toObject(inv.invoke().get());
  }
 catch (  Exception e) {
    return (Integer)ExceptionUtil.rethrow(e);
  }
}
