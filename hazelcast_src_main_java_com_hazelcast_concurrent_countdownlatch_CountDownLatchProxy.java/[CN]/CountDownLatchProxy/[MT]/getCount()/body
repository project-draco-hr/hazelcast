{
  final NodeEngine nodeEngine=getNodeEngine();
  GetCountOperation op=new GetCountOperation(name);
  Future<Integer> f=nodeEngine.getOperationService().invokeOnPartition(CountDownLatchService.SERVICE_NAME,op,partitionId);
  try {
    return f.get();
  }
 catch (  Exception e) {
    throw ExceptionUtil.rethrow(e);
  }
}
