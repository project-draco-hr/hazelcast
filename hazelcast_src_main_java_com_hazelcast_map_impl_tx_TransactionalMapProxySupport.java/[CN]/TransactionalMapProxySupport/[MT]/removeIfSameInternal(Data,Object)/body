{
  VersionedValue versionedValue=lockAndGet(key,tx.getTimeoutMillis());
  if (!getService().getMapServiceContext().compare(name,versionedValue.value,value)) {
    addUnlockTransactionRecord(key,versionedValue.version);
    return false;
  }
  tx.add(new MapTransactionRecord(name,key,new TxnDeleteOperation(name,key,versionedValue.version),versionedValue.version,tx.getOwnerUuid()));
  return true;
}
