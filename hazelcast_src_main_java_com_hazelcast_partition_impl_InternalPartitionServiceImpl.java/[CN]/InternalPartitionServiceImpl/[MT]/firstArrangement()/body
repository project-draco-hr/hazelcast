{
  if (node.getState() != NodeState.ACTIVE) {
    return;
  }
  if (!node.isMaster()) {
    notifyMasterToAssignPartitions();
    return;
  }
  if (initialized) {
    return;
  }
  lock.lock();
  try {
    if (initialized) {
      return;
    }
    if (initializePartitionAssignments()) {
      return;
    }
    publishPartitionRuntimeState();
  }
  finally {
    lock.unlock();
  }
}
