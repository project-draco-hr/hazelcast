{
  try {
    while (!isInterrupted()) {
      doRun();
    }
  }
 catch (  InterruptedException e) {
    if (logger.isFinestEnabled()) {
      logger.finest("MigrationThread is interrupted: " + e.getMessage());
    }
  }
catch (  OutOfMemoryError e) {
    OutOfMemoryErrorDispatcher.onOutOfMemory(e);
  }
 finally {
    migrationQueue.clear();
  }
}
