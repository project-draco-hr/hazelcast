{
  File config1=createConfigFile("hz1","xml");
  FileOutputStream os1=new FileOutputStream(config1);
  String config1Xml="<hazelcast-client xmlns=\"http://www.hazelcast.com/schema/client-config\">" + "    <import resource=\"file://" + config1.getAbsolutePath() + "\"/>\n"+ "</hazelcast-client>";
  writeStringToStreamAndClose(os1,"");
  buildConfig(config1Xml);
}
