{
  Map mm=new HashMap();
  for (int i=0; i < 100; i++) {
    mm.put(i,i);
  }
  map.putAll(mm);
  assertEquals(map.size(),100);
  for (int i=0; i < 100; i++) {
    assertEquals(map.get(i),i);
  }
  Set ss=new HashSet();
  ss.add(1);
  ss.add(3);
  Map m2=map.getAll(ss);
  assertEquals(m2.size(),2);
  assertEquals(m2.get(1),1);
  assertEquals(m2.get(3),3);
}
