{
  final IMap map=createMap();
  map.set("key1","value1");
  assertEquals("value1",map.get("key1"));
  map.set("key1","value2");
  assertEquals("value2",map.get("key1"));
  map.set("key1","value3",1,TimeUnit.SECONDS);
  assertEquals("value3",map.get("key1"));
  Thread.sleep(2000);
  assertNull(map.get("key1"));
}
