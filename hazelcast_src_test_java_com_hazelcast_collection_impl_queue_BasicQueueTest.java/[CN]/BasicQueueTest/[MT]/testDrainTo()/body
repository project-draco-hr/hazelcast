{
  IQueue<String> queue=newQueue();
  for (int i=0; i < 10; i++) {
    queue.offer("item" + i);
  }
  List list=new ArrayList(10);
  assertEquals(10,queue.drainTo(list));
  assertEquals(10,list.size());
  assertEquals("item0",list.get(0));
  assertEquals("item5",list.get(5));
  assertEquals(0,queue.size());
}
