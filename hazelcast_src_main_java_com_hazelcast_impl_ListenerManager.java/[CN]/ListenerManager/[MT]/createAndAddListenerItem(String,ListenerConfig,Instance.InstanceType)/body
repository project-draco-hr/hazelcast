{
  Object listener=lc.getImplementation();
  if (listener == null) {
    listener=ClassLoaderUtil.newInstance(lc.getClassName());
  }
  if (listener != null) {
    final ListenerItem listenerItem=new ListenerItem(name,null,listener,lc.isIncludeValue(),instanceType,lc.isLocal());
    getOrCreateListenerList(name).add(listenerItem);
  }
}
