{
  if (LITTLE_ENDIAN) {
    return MEM.getLong(address + i);
  }
 else {
    return ((MEM.getByte(address + i) & 0x00000000000000FFL)) | ((MEM.getByte(address + i + 1) & 0x00000000000000FFL) << 8) | ((MEM.getByte(address + i + 2) & 0x00000000000000FFL) << 16)| ((MEM.getByte(address + i + 3) & 0x00000000000000FFL) << 24)| ((MEM.getByte(address + i + 4) & 0x00000000000000FFL) << 32)| ((MEM.getByte(address + i + 5) & 0x00000000000000FFL) << 40)| ((MEM.getByte(address + i + 6) & 0x00000000000000FFL) << 48)| ((MEM.getByte(address + i + 7) & 0x00000000000000FFL) << 56);
  }
}
