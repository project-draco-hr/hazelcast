{
  Unsafe unsafe=UnsafeHelper.UNSAFE;
  if (LITTLE_ENDIAN) {
    return unsafe.getLong(address + i);
  }
 else {
    return ((unsafe.getByte(address + i) & 0x00000000000000FFL)) | ((unsafe.getByte(address + i + 1) & 0x00000000000000FFL) << 8) | ((unsafe.getByte(address + i + 2) & 0x00000000000000FFL) << 16)| ((unsafe.getByte(address + i + 3) & 0x00000000000000FFL) << 24)| ((unsafe.getByte(address + i + 4) & 0x00000000000000FFL) << 32)| ((unsafe.getByte(address + i + 5) & 0x00000000000000FFL) << 40)| ((unsafe.getByte(address + i + 6) & 0x00000000000000FFL) << 48)| ((unsafe.getByte(address + i + 7) & 0x00000000000000FFL) << 56);
  }
}
