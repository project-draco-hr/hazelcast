{
  StringBuilder sb=new StringBuilder("Member [");
  sb.append(address.getHost());
  sb.append(":");
  sb.append(address.getPort());
  sb.append("] ");
  if (localMember) {
    sb.append("this ");
  }
  if (Node.DEBUG && address.equals(Node.get().getMasterAddress())) {
    sb.append("* ");
  }
  return sb.toString();
}
