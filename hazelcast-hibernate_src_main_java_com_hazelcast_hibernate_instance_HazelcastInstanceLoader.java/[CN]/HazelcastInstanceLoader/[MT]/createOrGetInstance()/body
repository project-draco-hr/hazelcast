{
  if (staticInstance) {
    instance=Hazelcast.getDefaultInstance();
  }
 else {
    if (config == null) {
      config=new XmlConfigBuilder().build();
    }
    config.setInstanceName(instanceName);
    config.setLiteMember(useLiteMember);
    instance=Hazelcast.newHazelcastInstance(config);
  }
}
