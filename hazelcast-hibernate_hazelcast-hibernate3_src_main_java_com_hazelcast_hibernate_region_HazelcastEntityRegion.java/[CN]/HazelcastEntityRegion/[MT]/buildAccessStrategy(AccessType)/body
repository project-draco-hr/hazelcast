{
  if (AccessType.READ_ONLY.equals(accessType)) {
    return new EntityRegionAccessStrategyAdapter(new ReadOnlyAccessDelegate<HazelcastEntityRegion>(this,props));
  }
  if (AccessType.NONSTRICT_READ_WRITE.equals(accessType)) {
    return new EntityRegionAccessStrategyAdapter(new NonStrictReadWriteAccessDelegate<HazelcastEntityRegion>(this,props));
  }
  if (AccessType.READ_WRITE.equals(accessType)) {
    return new EntityRegionAccessStrategyAdapter(new ReadWriteAccessDelegate<HazelcastEntityRegion>(this,props));
  }
  throw new CacheException("AccessType \"" + accessType + "\" is not currently supported by Hazelcast.");
}
