{
  if (!isMemberNearCacheInvalidationEnabled(mapName)) {
    return;
  }
  NearCache nearCache=nearCacheProvider.getOrNullNearCache(mapName);
  if (nearCache != null) {
    if (key != null) {
      nearCache.remove(key);
    }
 else     if (!isEmpty(keys)) {
      for (      Data data : keys) {
        nearCache.remove(data);
      }
    }
  }
}
