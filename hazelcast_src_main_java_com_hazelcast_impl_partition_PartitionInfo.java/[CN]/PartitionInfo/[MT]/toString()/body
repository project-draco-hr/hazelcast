{
  StringBuilder sb=new StringBuilder("Partition [").append(partitionId).append("]{\n");
  for (int i=0; i < MAX_REPLICA_COUNT; i++) {
    Address address=addresses.get(i);
    if (address != null) {
      sb.append('\t');
      sb.append(i).append(":").append(address);
      sb.append("\n");
    }
  }
  sb.append("}");
  return sb.toString();
}
