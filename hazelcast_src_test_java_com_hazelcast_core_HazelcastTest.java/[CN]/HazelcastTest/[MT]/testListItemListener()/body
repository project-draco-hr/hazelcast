{
  IList<String> list=Hazelcast.getList("testListListener");
  list.addItemListener(new ItemListener<String>(){
    public void itemAdded(    String item){
      assertEquals("hello",item);
    }
    public void itemRemoved(    String item){
      assertEquals("hello",item);
    }
  }
,true);
  list.add("hello");
  list.remove("hello");
}
