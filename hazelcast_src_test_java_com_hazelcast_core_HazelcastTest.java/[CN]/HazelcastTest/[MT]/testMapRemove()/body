{
  IMap<String,String> map=Hazelcast.getMap("testMapRemove");
  map.put("Hello","World");
  assertEquals(1,map.size());
  assertEquals(1,map.keySet().size());
  map.remove("Hello");
  assertEquals(0,map.size());
  assertEquals(0,map.keySet().size());
  map.put("Hello","World");
  assertEquals(1,map.size());
  assertEquals(1,map.keySet().size());
}
