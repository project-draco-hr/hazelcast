{
  final IQueue q=Hazelcast.getQueue("testInterruption");
  Thread t=new Thread(new Runnable(){
    public void run(){
      try {
        q.take();
        fail();
      }
 catch (      InterruptedException e) {
      }
    }
  }
);
  t.start();
  Thread.sleep(2000);
  t.interrupt();
  q.offer("message");
  assertEquals(1,q.size());
}
