{
switch (method) {
case Byte:
    return ByteArray;
case Boolean:
  return BooleanArray;
case Char:
return CharArray;
case Short:
return ShortArray;
case Int:
return IntArray;
case Long:
return LongArray;
case Float:
return FloatArray;
case Double:
return DoubleArray;
case UTF:
return UTFArray;
default :
throw new RuntimeException("Unsupported method");
}
}
