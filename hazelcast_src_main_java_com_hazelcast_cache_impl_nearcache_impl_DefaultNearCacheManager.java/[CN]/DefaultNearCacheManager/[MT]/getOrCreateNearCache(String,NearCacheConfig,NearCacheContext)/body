{
  NearCache<K,V> nearCache=nearCacheMap.get(name);
  if (nearCache == null) {
synchronized (nearCacheMap) {
      nearCache=nearCacheMap.get(name);
      if (nearCache == null) {
        nearCache=createNearCache(name,nearCacheConfig,nearCacheContext);
        nearCacheMap.put(name,nearCache);
      }
    }
  }
  return nearCache;
}
