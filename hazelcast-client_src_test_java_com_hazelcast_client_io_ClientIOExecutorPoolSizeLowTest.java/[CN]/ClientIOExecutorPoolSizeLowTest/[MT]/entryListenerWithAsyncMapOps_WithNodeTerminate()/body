{
  for (int i=0; i < COUNT; i++) {
    map.putAsync(i,i);
  }
  assertTrueEventually(new AssertTask(){
    public void run(){
      assertEquals(COUNT,map.size());
    }
  }
);
  for (int i=0; i < COUNT; i++) {
    Future f=map.getAsync(i);
    assertEquals(i,f.get());
    if (i == 500) {
      server2.getLifecycleService().terminate();
    }
  }
}
