{
  final EntryCounter counter=new EntryCounter();
  map.addEntryListener(counter,true);
  for (int i=0; i < COUNT; i++) {
    map.putAsync(i,i);
  }
  assertTrueEventually(new AssertTask(){
    public void run(){
      assertEquals(COUNT,map.size());
    }
  }
);
  assertTrueEventually(new AssertTask(){
    public void run(){
      assertEquals(COUNT,counter.count.get());
    }
  }
);
}
