{
  final IMap<Object,Object> map=client.getMap("map");
  for (int i=0; i < 1000; i++) {
    map.put(i,i);
    if (i == 500) {
      server1.getLifecycleService().terminate();
    }
  }
  assertTrueEventually(new AssertTask(){
    public void run(){
      assertEquals(1000,map.size());
    }
  }
);
}
