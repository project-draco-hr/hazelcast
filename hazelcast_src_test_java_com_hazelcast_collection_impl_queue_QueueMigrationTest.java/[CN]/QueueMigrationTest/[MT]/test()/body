{
  List<Object> expectedItems=new LinkedList<Object>();
  for (int k=0; k < 100; k++) {
    queue.add(k);
    expectedItems.add(k);
  }
  remote1.shutdown();
  remote2.shutdown();
  assertEquals(expectedItems.size(),queue.size());
  List actualItems=Arrays.asList(queue.toArray());
  assertEquals(expectedItems,actualItems);
}
