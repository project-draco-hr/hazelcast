{
  SessionState sessionState=entry.getValue();
  if (sessionState == null) {
    return Boolean.FALSE;
  }
  sessionState.removeJvmId(jvmId);
  if (invalidate || sessionState.getJvmIds().isEmpty()) {
    entry.setValue(null);
    removed=true;
  }
 else {
    entry.setValue(sessionState);
  }
  return Boolean.TRUE;
}
