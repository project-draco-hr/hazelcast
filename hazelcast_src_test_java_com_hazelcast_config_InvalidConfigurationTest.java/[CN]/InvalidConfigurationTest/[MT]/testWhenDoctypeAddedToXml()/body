{
  expectInvalid("DOCTYPE is disallowed when the feature " + "\"http://apache.org/xml/features/disallow-doctype-decl\" set to true.");
  String xml="<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n" + "<!DOCTYPE data [\n" + "<!ENTITY e1 \"0123456789\" > ] >"+ "<hazelcast></hazelcast>";
  buildConfig(xml);
}
