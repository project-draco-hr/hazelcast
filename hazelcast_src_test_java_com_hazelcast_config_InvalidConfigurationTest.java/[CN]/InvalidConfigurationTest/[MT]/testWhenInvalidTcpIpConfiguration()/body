  expectInvalid("Duplicate required-member definition found in XML configuration.");
  buildConfig(HAZELCAST_START_TAG + "<network\n>" + "<join>\n"+ "<tcp-ip enabled=\"true\">\n"+ "<required-member>127.0.0.1</required-member>\n"+ "<required-member>128.0.0.1</required-member>\n"+ "</tcp-ip>\n"+ "</join>\n"+ "</network>\n"+ "</hazelcast>\n");
}
