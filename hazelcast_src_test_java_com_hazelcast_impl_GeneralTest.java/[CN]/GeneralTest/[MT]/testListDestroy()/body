{
  HazelcastInstance ins=Hazelcast.newHazelcastInstance();
  final ICollection<String> list=ins.getList("listTest");
  assertEquals(0,list.size());
  list.add("1");
  list.add("2");
  assertEquals(2,list.size());
  list.clear();
  assertEquals(0,list.size());
  list.add("1");
  list.add("2");
  assertEquals(2,list.size());
  list.destroy();
  assertEquals(0,list.size());
}
