{
  HazelcastClient hClient=getHazelcastClient();
  IMap map=hClient.getMap("putAllMany");
  int counter=100;
  for (int j=0; j < 4; j++, counter*=10) {
    Map tempMap=new HashMap();
    for (int i=0; i < counter; i++) {
      tempMap.put(i,i);
    }
    map.putAll(tempMap);
    assertEquals(1,map.get(1));
  }
  map.destroy();
}
