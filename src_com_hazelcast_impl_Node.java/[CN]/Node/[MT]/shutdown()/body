{
  try {
    ClusterService.get().stop();
    MulticastService.get().stop();
    ConnectionManager.get().shutdown();
    ExecutorManager.get().shutdown();
    InSelector.get().shutdown();
    OutSelector.get().shutdown();
    address=null;
    masterAddress=null;
    joined=false;
  }
 catch (  Throwable e) {
    logger.log(Level.FINEST,"shutdown exception",e);
  }
}
