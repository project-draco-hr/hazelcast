{
  if (address.indexOf('.') == -1) {
    return false;
  }
 else {
    final StringTokenizer st=new StringTokenizer(address,".");
    int tokenCount=0;
    while (st.hasMoreTokens()) {
      final String token=st.nextToken();
      tokenCount++;
      try {
        Integer.parseInt(token);
      }
 catch (      final Exception e) {
        return false;
      }
    }
    if (tokenCount != 4)     return false;
  }
  return true;
}
