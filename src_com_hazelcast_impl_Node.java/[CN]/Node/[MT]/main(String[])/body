{
  try {
    final int port=5701;
    final InetAddress addr=InetAddress.getByAddress(new byte[]{(byte)192,(byte)168,1,2});
    final SocketAddress sockaddr=new InetSocketAddress(addr,port);
    final Socket sock=new Socket();
    final int timeoutMs=2000;
    sock.connect(sockaddr,timeoutMs);
    System.in.read();
    sock.close();
  }
 catch (  final Exception e) {
    e.printStackTrace();
  }
}
