{
  try {
    int port=5701;
    InetAddress addr=InetAddress.getByAddress(new byte[]{(byte)192,(byte)168,1,2});
    SocketAddress sockaddr=new InetSocketAddress(addr,port);
    Socket sock=new Socket();
    int timeoutMs=2000;
    sock.connect(sockaddr,timeoutMs);
    System.in.read();
    sock.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
