{
  Object value=record.getCachedValue();
  if (value == Record.NOT_CACHED) {
    value=record.getValue();
    if (value != null && value instanceof Data) {
      value=ss.toObject(value);
    }
  }
 else {
    value=ss.toObject(record.getValue());
    record.setCachedValue(value);
  }
  return value;
}
