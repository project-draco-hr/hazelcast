{
  NodeEngine nodeEngine=getNodeEngine();
  Object[] responses=new Object[partitions.length];
  for (int i=0; i < partitions.length; i++) {
    ResponseQueue responseQueue=new ResponseQueue();
    responses[i]=responseQueue;
    Operation operation=operationFactory.createOperation();
    operation.setNodeEngine(nodeEngine).setPartitionId(partitions[i]).setReplicaIndex(getReplicaIndex()).setOperationResponseHandler(responseQueue).setServiceName(getServiceName()).setService(getService()).setCallerUuid(extractCallerUuid());
    OperationAccessor.setCallerAddress(operation,getCallerAddress());
    nodeEngine.getOperationService().execute(operation);
  }
  return responses;
}
