{
  if (!hasNode(address)) {
    throw new IllegalArgumentException(address + " is different from this node's " + this.address);
  }
  if (containsPartition(partitionId)) {
    throw new IllegalArgumentException("Partition[" + partitionId + "] is already owned by this node "+ address+ "! Duplicate!");
  }
  nodeTable.add(index,partitionId);
}
