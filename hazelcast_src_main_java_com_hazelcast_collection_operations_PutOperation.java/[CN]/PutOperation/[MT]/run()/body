{
  Object obj=isBinary() ? value : toObject(value);
  if (index == -1) {
    Collection coll=getOrCreateCollection();
    response=coll.add(obj);
  }
 else {
    List list=getOrCreateCollection();
    try {
      list.add(index,obj);
      response=true;
    }
 catch (    IndexOutOfBoundsException e) {
      response=e;
    }
  }
}
