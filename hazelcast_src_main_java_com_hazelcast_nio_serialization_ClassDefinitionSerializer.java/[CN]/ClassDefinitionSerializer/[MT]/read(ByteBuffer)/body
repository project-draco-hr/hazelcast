{
  if (!isStatusSet(ST_PREPARED)) {
    if (source.remaining() < INT_SIZE_IN_BYTES) {
      return false;
    }
    classDefCount=source.getInt();
    metadata=new byte[classDefCount * HEADER_ENTRY_LENGTH];
    ((MutableData)data).setHeader(metadata);
    setStatus(ST_PREPARED);
  }
  if (!readAll(source)) {
    return false;
  }
  return true;
}
