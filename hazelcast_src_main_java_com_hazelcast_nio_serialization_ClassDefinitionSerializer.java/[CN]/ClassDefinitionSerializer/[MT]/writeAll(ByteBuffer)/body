{
  for (; classDefIndex < classDefCount; classDefIndex++) {
    ClassDefinitionImpl cd=(ClassDefinitionImpl)classDefinitions[classDefIndex];
    if (!writeHeader(cd,destination)) {
      return false;
    }
    if (!writeData(cd,destination)) {
      return false;
    }
    clearStatus(ST_HEADER);
    clearStatus(ST_DATA);
  }
  return true;
}
