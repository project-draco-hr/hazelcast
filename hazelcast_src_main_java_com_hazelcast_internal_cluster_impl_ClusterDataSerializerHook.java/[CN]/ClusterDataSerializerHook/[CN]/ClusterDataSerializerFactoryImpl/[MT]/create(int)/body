{
switch (typeId) {
case ADDRESS:
    return new Address();
case MEMBER:
  return new MemberImpl();
case HEARTBEAT:
return new HeartbeatOperation();
case CONFIG_CHECK:
return new ConfigCheck();
case BIND_MESSAGE:
return new BindMessage();
case MEMBER_INFO_UPDATE:
return new MemberInfoUpdateOperation();
case FINALIZE_JOIN:
return new FinalizeJoinOperation();
default :
return null;
}
}
