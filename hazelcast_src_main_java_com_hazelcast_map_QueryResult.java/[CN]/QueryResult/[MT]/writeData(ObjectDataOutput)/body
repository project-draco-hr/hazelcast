{
  int psize=partitionIds.size();
  out.writeInt(psize);
  for (int i=0; i < psize; i++) {
    out.writeInt(partitionIds.get(i));
  }
  int rsize=result.size();
  out.writeInt(rsize);
  Iterator<QueryableEntry> iterator=result.iterator();
  for (int i=0; i < rsize; i++) {
    out.writeObject(iterator.next());
  }
}
