{
  ThreadContext.get().setCurrentFactory(node.factory);
  while (active) {
    Runnable r=null;
    try {
      r=q.take();
    }
 catch (    InterruptedException e) {
      return;
    }
    try {
      r.run();
    }
 catch (    OutOfMemoryError e) {
      OutOfMemoryErrorDispatcher.onOutOfMemory(e);
      throw e;
    }
catch (    Throwable ignored) {
    }
  }
}
