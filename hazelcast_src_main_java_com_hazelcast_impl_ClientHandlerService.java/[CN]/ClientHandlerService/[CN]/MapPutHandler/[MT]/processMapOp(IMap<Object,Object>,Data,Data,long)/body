{
  counter.incrementAndGet();
  MProxy mproxy=(MProxy)map;
  Object v=value;
  if (node.concurrentMapManager.isMapIndexed(mproxy.getLongName())) {
    v=toObject(value);
  }
  if (ttl <= 0) {
    Data result=(Data)map.put(key,v);
    return result;
  }
 else {
    return (Data)map.put(key,v,ttl,TimeUnit.MILLISECONDS);
  }
}
