{
  ByteBuffer bb=null;
  if (lsEmptyBuffers.size() == 0) {
    bufferq.drainTo(lsEmptyBuffers,listMaxSize);
  }
  if (lsEmptyBuffers.size() == 0) {
    bb=ByteBuffer.allocate(1024);
    bufferCounter.incrementAndGet();
  }
 else {
    bb=lsEmptyBuffers.remove(0);
    bb.clear();
  }
  return bb;
}
