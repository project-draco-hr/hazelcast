{
  Config config=new Config();
  config.setProperty(GroupProperties.PROP_PARTITION_TABLE_SEND_INTERVAL,"2");
  config.setProperty(GroupProperties.PROP_PARTITION_TABLE_SEND_INTERVAL,"2");
  final HazelcastInstance h1=Hazelcast.newHazelcastInstance(config);
  final HazelcastInstance h2=Hazelcast.newHazelcastInstance(config);
  RunAfterTester t=new RunAfterTester(new NoConnectionBehavior(h1,h2),new KeyCallBuilder(h1));
  t.run();
  t=new RunAfterTester(new NoConnectionBehavior(h2,h1),new KeyCallBuilder(h2));
  t.run();
}
