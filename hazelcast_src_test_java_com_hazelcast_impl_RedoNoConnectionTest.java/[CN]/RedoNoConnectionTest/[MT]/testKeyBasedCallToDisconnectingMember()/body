{
  Config config=new Config();
  final HazelcastInstance h1=Hazelcast.newHazelcastInstance(config);
  final HazelcastInstance h2=Hazelcast.newHazelcastInstance(config);
  Runnable t=new RunAfterTester(new DisconnectionBehavior(h1,h2),new KeyCallBuilder(h1));
  t.run();
  t=new RunAfterTester(new DisconnectionBehavior(h2,h1),new KeyCallBuilder(h2));
  t.run();
}
