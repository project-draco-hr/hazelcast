{
  callerNode.connectionManager.detachAndGetConnection(targetMember.getAddress());
  if (targetConn != null) {
    try {
      targetConn.close0();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  callerNode.clusterImpl.enqueueAndWait(new Processable(){
    public void process(){
      callerNode.clusterImpl.removeMember(targetMember);
    }
  }
,3);
}
