{
  callerNode.connectionManager.getIOHandler().removeEndpoint(targetMember.getAddress());
  if (targetConn != null) {
    try {
      targetConn.close0();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  callerNode.clusterManager.enqueueAndWait(new Processable(){
    public void process(){
      callerNode.clusterManager.removeMember(targetMember);
    }
  }
,3);
}
