{
  if (serviceThread) {
    checkThread();
  }
  final List<Data> lsKeysToInvalidate=getInvalidEntries(now);
  if (lsKeysToInvalidate != null && lsKeysToInvalidate.size() > 0) {
    if (serviceThread) {
      for (      Data key : lsKeysToInvalidate) {
        invalidate(key);
      }
    }
 else {
      cmap.concurrentMapManager.enqueueAndReturn(new Processable(){
        public void process(){
          for (          Data key : lsKeysToInvalidate) {
            invalidate(key);
          }
        }
      }
);
    }
  }
}
