{
  if (count < 0) {
    throw new IllegalArgumentException("count can't be negative");
  }
  ClientMessage request=CountDownLatchTrySetCountCodec.encodeRequest(getName(),count);
  CountDownLatchTrySetCountCodec.ResponseParameters resultParameters=CountDownLatchTrySetCountCodec.decodeResponse((ClientMessage)invoke(request));
  return resultParameters.response;
}
