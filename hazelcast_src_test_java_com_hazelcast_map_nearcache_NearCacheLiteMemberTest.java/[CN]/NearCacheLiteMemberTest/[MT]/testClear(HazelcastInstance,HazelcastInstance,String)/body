{
  final IMap<Object,Object> map=instance.getMap(mapName);
  final int count=100;
  for (int i=0; i < count; i++) {
    map.put(i,i);
  }
  final IMap<Object,Object> liteMap=lite.getMap(mapName);
  for (int i=0; i < count; i++) {
    liteMap.get(i);
  }
  map.clear();
  assertNearCacheIsEmptyEventually(lite,mapName);
}
