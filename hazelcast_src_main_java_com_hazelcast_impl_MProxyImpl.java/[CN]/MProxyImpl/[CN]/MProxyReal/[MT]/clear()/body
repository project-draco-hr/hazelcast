{
  if (concurrentMapManager.getMap(name).isClearQuick()) {
    mapOperationCounter.incrementOtherOperations();
    MClearQuick mClearQuick=concurrentMapManager.new MClearQuick(name);
    mClearQuick.call();
  }
 else {
    Set keys=keySet();
    for (    Object key : keys) {
      removeKey(key);
    }
  }
}
