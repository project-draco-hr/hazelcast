{
  while (inBuffer.hasRemaining()) {
    if (packet == null) {
      packet=new Packet(ioService.getPortableContext());
    }
    boolean complete=packet.readFrom(inBuffer);
    if (complete) {
      packet.setConn(connection);
      ioService.handleClientPacket(packet);
      packet=null;
    }
 else {
      break;
    }
  }
}
