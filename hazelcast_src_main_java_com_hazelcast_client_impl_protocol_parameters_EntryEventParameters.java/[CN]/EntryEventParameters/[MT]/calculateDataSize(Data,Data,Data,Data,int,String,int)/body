{
  return ClientMessage.HEADER_SIZE + ParameterUtil.calculateDataSize(key) + ParameterUtil.calculateDataSize(value)+ ParameterUtil.calculateDataSize(oldValue)+ ParameterUtil.calculateDataSize(mergingValue)+ Bits.INT_SIZE_IN_BYTES+ ParameterUtil.calculateStringDataSize(uuid)+ Bits.INT_SIZE_IN_BYTES;
}
