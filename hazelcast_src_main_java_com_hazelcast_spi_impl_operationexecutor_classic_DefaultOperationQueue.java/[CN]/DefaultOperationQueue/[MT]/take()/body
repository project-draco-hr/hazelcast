{
  ConcurrentLinkedQueue priorityQueue=this.priorityQueue;
  for (; ; ) {
    Object priorityItem=priorityQueue.poll();
    if (priorityItem != null) {
      return priorityItem;
    }
    Object normalItem=normalQueue.take();
    if (normalItem == TRIGGER_TASK) {
      continue;
    }
    return normalItem;
  }
}
