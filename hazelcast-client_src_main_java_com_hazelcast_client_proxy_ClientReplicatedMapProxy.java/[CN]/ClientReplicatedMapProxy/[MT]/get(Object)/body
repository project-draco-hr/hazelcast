{
  initNearCache();
  if (nearCache != null) {
    Object cached=nearCache.get(key);
    if (cached != null) {
      if (cached.equals(ClientNearCache.NULL_OBJECT)) {
        return null;
      }
      return (V)cached;
    }
  }
  ReplicatedMapGetResponse response=invoke(new ClientReplicatedMapGetRequest(getName(),key));
  return (V)response.getValue();
}
