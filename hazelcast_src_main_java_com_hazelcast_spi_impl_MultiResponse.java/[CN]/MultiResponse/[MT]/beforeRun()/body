{
  final NodeEngine nodeEngine=getNodeEngine();
  final int len=operationData.length;
  operations=new Operation[len];
  for (int i=0; i < len; i++) {
    final Operation op=(Operation)nodeEngine.toObject(operationData[i]);
    op.setNodeEngine(nodeEngine).setCaller(getCaller()).setCallId(getCallId()).setConnection(getConnection()).setResponseHandler(ResponseHandlerFactory.createEmptyResponseHandler());
    operations[i]=op;
  }
}
