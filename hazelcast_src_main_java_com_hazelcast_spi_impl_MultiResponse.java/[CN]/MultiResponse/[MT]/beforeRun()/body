{
  final NodeService nodeService=getNodeService();
  final int len=operationData.length;
  operations=new Operation[len];
  for (int i=0; i < len; i++) {
    final Operation op=(Operation)nodeService.toObject(operationData[i]);
    op.setNodeService(nodeService).setCaller(getCaller()).setCallId(getCallId()).setConnection(getConnection()).setResponseHandler(ResponseHandlerFactory.createEmptyResponseHandler());
    operations[i]=op;
  }
}
