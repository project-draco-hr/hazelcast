{
  if (selectorQueue != null) {
    selectorQueue.clear();
  }
  try {
    final CountDownLatch l=new CountDownLatch(1);
    addTask(new Runnable(){
      public void run(){
        live=false;
        l.countDown();
      }
    }
);
    l.await(3,TimeUnit.SECONDS);
  }
 catch (  InterruptedException ignored) {
  }
}
