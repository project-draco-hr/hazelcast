{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  StreamSerializerAdapter that=(StreamSerializerAdapter)o;
  if (serializer != null ? !serializer.equals(that.serializer) : that.serializer != null) {
    return false;
  }
  return true;
}
