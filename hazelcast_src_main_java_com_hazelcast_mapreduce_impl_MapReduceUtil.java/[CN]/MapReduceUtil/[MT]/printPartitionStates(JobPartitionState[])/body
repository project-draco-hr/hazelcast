{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < partitionStates.length; i++) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append("[").append(i).append("=>");
    sb.append(partitionStates[i] == null ? "null" : partitionStates[i].getState()).append("]");
  }
  return sb.toString();
}
