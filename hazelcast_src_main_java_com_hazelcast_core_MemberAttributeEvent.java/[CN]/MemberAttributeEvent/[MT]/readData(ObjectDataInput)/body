{
  key=in.readUTF();
  member=new MemberImpl();
  member.readData(in);
  int operation=in.readByte();
switch (operation) {
case DELTA_MEMBER_PROPERTIES_OP_PUT:
    operationType=MapOperationType.PUT;
  value=in.readObject();
break;
case DELTA_MEMBER_PROPERTIES_OP_REMOVE:
operationType=MapOperationType.REMOVE;
break;
}
this.source=member;
}
