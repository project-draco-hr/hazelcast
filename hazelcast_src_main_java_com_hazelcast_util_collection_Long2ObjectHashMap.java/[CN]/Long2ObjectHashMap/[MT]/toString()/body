{
  final StringBuilder sb=new StringBuilder();
  sb.append('{');
  for (  final Map.Entry<Long,V> entry : entrySet()) {
    sb.append(entry.getKey().longValue());
    sb.append('=');
    sb.append(entry.getValue());
    sb.append(", ");
  }
  if (sb.length() > 1) {
    sb.setLength(sb.length() - 2);
  }
  sb.append('}');
  return sb.toString();
}
