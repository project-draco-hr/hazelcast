{
  if (raw) {
    throw new HazelcastSerializationException("Cannot read Portable fields after getRawDataInput() is called!");
  }
  try {
    pathCursor.init(path);
    return PortablePositionNavigator.findPositionForReading(ctx,pathCursor);
  }
  finally {
    pathCursor.reset();
    ctx.reset();
  }
}
