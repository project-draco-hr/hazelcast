{
  beforeProcess();
  Operation op=prepareOperation();
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=nodeEngine.getOperationService().createInvocationBuilder(getServiceName(),op,getPartitionId()).setResultDeserialized(false);
  ICompletableFuture future=builder.invoke();
  future.andThen(this);
}
