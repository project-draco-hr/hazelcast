{
  String string=this.properties.get(name);
  if (string == null) {
    string=System.getProperty(name.getName());
  }
  if (string == null) {
    string=name.defaultValue;
  }
  if (string == null) {
    throw new IllegalStateException("property " + name.getName() + " is null");
  }
  return string;
}
