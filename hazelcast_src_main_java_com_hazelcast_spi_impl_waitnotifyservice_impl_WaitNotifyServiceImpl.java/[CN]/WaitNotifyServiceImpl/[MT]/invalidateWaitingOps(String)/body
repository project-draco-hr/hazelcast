{
  for (  Queue<WaitingOperation> q : mapWaitingOps.values()) {
    for (    WaitingOperation waitingOp : q) {
      if (waitingOp.isValid()) {
        Operation op=waitingOp.getOperation();
        if (callerUuid.equals(op.getCallerUuid())) {
          waitingOp.setValid(false);
        }
      }
    }
  }
}
