{
  System.out.println("==================================================================");
  System.out.println("Inserting data in map");
  System.out.println("==================================================================");
  for (int k=0; k < MAP_SIZE; k++) {
    map.put(k,0);
    if (k % 10000 == 0) {
      System.out.println("Inserted data: " + k);
    }
  }
  System.out.println("==================================================================");
  System.out.println("Completed with inserting data in map");
  System.out.println("==================================================================");
}
