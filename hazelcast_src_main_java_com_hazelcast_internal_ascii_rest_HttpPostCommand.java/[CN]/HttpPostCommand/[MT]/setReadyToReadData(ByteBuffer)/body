{
  while (!readyToReadData && cb.hasRemaining()) {
    byte b=cb.get();
    char c=(char)b;
    if (c == '\n') {
      processLine(toStringAndClear(line).toLowerCase());
      if (nextLine) {
        readyToReadData=true;
      }
      nextLine=true;
    }
 else     if (c != '\r') {
      nextLine=false;
      line.put(b);
    }
  }
}
