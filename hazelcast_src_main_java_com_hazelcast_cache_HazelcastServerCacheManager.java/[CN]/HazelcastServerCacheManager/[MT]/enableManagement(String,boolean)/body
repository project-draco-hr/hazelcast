{
  if (isClosed()) {
    throw new IllegalStateException();
  }
  if (cacheName == null) {
    throw new NullPointerException();
  }
synchronized (caches) {
    final String cacheNameWithPrefix=getCacheNameWithPrefix(cacheName);
    ICache<?,?> cache=caches.get(cacheNameWithPrefix);
    cache.enableManagement(enabled);
  }
}
