{
  try {
    if (state == NO_TXN || state == ROLLED_BACK) {
      throw new IllegalStateException("Transaction is not active");
    }
    checkThread();
    try {
      sendAndReceive(new RollbackTransactionRequest());
    }
 catch (    Exception e) {
    }
    state=ROLLED_BACK;
  }
  finally {
    closeConnection();
  }
}
