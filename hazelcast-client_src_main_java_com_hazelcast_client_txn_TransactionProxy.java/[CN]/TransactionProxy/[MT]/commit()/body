{
  try {
    if (state != ACTIVE) {
      throw new TransactionNotActiveException("Transaction is not active");
    }
    checkThread();
    checkTimeout();
    sendAndReceive(new CommitTransactionRequest());
    state=COMMITTED;
  }
 catch (  Exception e) {
    state=ROLLING_BACK;
    throw ExceptionUtil.rethrow(e);
  }
 finally {
    closeConnection();
  }
}
