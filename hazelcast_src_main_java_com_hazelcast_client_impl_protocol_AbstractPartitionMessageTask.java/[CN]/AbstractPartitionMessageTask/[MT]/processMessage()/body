{
  beforeProcess();
  Operation op=prepareOperation();
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=nodeEngine.getOperationService().createInvocationBuilder(getServiceName(),op,getPartitionId()).setReplicaIndex(getReplicaIndex()).setTryCount(TRY_COUNT).setResultDeserialized(false).setCallback(new CallbackImpl(endpoint));
  builder.invoke();
}
