{
  try {
    final ClientInvocation clientInvocation=new ClientInvocation((HazelcastClientInstanceImpl)clientContext.getHazelcastInstance(),req);
    final Future<SerializableCollection> future=clientInvocation.invoke();
    Object result=future.get();
    return toObject(result);
  }
 catch (  Exception e) {
    throw ExceptionUtil.rethrow(e);
  }
}
