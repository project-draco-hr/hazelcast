{
  FileObject file;
  try {
    final String fullName=className + ".cs";
    file=filer.createResource(StandardLocation.locationFor(StandardLocation.SOURCE_OUTPUT.name()),packageName,fullName,classElement);
    file.openWriter().append(content).close();
  }
 catch (  IOException e) {
    messager.printMessage(Diagnostic.Kind.ERROR,e.getMessage());
  }
}
