{
  QueueContainer container=getContainer();
  try {
    item=container.peek();
    if (item != null) {
      response=item.getData();
      if (container.isStoreEnabled() && !container.isStoreAsync()) {
        container.getStore().delete(item.getItemId());
      }
      container.pollBackup();
    }
  }
 catch (  Exception e) {
    throw new RetryableException(e);
  }
}
