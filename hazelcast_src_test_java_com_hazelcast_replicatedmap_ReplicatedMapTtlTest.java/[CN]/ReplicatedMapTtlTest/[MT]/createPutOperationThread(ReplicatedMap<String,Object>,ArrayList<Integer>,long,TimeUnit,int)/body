{
  return new Thread(new Runnable(){
    @Override public void run(){
      Random random=new Random();
      int size=keys.size();
      for (int i=0; i < operations; i++) {
        int index=i % size;
        String key="foo-" + keys.get(index);
        map.put(key,random.nextLong(),1 + random.nextInt((int)ttl),timeunit);
      }
    }
  }
);
}
