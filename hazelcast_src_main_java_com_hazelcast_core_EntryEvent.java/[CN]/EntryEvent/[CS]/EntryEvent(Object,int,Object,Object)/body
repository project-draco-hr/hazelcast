{
  this(source);
  this.key=key;
  this.value=value;
  if (eventType == TYPE_REMOVED) {
    entryEventType=EntryEventType.REMOVED;
  }
 else   if (eventType == TYPE_UPDATED) {
    entryEventType=EntryEventType.UPDATED;
  }
 else   if (eventType == TYPE_EVICTED) {
    entryEventType=EntryEventType.EVICTED;
  }
}
