{
  processedPackets.inc();
  try {
    currentRunner=getOperationRunner(packet.getPartitionId());
    currentRunner.run(packet);
  }
 catch (  Throwable e) {
    inspectOutputMemoryError(e);
    logger.severe("Failed to process packet: " + packet + " on "+ getName(),e);
  }
 finally {
    currentRunner=null;
  }
}
