{
  ensureOpen();
  validateNotNull(key);
  if (entryProcessor == null) {
    throw new NullPointerException("Entry Processor is null");
  }
  final Data keyData=toData(key);
  final CacheEntryProcessorRequest request=new CacheEntryProcessorRequest(nameWithPrefix,keyData,entryProcessor,cacheConfig.getInMemoryFormat(),arguments);
  try {
    final ICompletableFuture f=invoke(request,keyData,true);
    return (T)getSafely(f);
  }
 catch (  CacheException ce) {
    throw ce;
  }
catch (  Exception e) {
    throw new EntryProcessorException(e);
  }
}
