{
  Indexes indexes=mapContainer.getIndexes();
  if (indexes.hasIndex()) {
    Data key=record.getKey();
    Object value=Records.getValueOrCachedValue(record,serializationService);
    if (NATIVE.equals(inMemoryFormat)) {
      key=toData(key);
      value=toData(value);
    }
    indexes.removeEntryIndex(key,value);
  }
}
