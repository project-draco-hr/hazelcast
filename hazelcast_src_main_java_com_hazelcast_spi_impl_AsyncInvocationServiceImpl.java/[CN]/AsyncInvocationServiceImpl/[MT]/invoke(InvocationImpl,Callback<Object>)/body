{
  if (size.get() >= CAPACITY) {
    logger.log(Level.WARNING,"Capacity overloaded! Executing " + invocation + " in current thread, instead of invoking asynchronously.");
    invokeInCurrentThread(invocation,responseCallback);
  }
 else {
    size.incrementAndGet();
    executor.execute(new AsyncInvocation(invocation,responseCallback));
  }
}
