{
  if (proxy == null) {
    boolean interrupted=false;
synchronized (this) {
      while (proxy == null) {
        try {
          wait();
        }
 catch (        InterruptedException e) {
          interrupted=true;
        }
      }
    }
    if (interrupted) {
      Thread.currentThread().interrupt();
    }
  }
  return proxy;
}
