{
  long start=Clock.currentTimeMillis();
  startExecution(name,start - creationTime);
  Object result=null;
  try {
    super.run();
    if (!isCancelled()) {
      result=get();
    }
  }
 catch (  Exception e) {
    logException(e);
    result=e;
  }
 finally {
    if (uuid != null) {
      submittedTasks.remove(uuid);
    }
    if (!isCancelled()) {
      sendResponse(result);
      finishExecution(name,Clock.currentTimeMillis() - start);
    }
  }
}
