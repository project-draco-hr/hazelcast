{
  ByteBuffer out=ByteBuffer.allocate(4);
  try {
    for (int i=0; i < count; i++) {
      int data=prepareData(i);
      out.putInt(data);
      out.flip();
      while (out.hasRemaining()) {
        socketChannel.write(out);
      }
      out.clear();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    latch.countDown();
  }
}
