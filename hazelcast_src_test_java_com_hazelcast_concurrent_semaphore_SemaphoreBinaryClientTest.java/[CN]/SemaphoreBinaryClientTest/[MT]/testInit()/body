{
  ISemaphore s=hz.getSemaphore(name);
  c.send(new InitRequest(name,10));
  boolean result=(Boolean)c.receive();
  assertTrue(result);
  assertEquals(10,s.availablePermits());
  c.send(new InitRequest(name,20));
  result=(Boolean)c.receive();
  assertFalse(result);
  assertEquals(10,s.availablePermits());
}
