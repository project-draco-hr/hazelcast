{
  try {
    if (loadFail.get()) {
      throw new RuntimeException();
    }
 else {
      Map results=new HashMap();
      for (      Object key : keys) {
        Object value=db.get(key);
        if (value != null) {
          results.put(key,value);
        }
      }
      return results;
    }
  }
  finally {
    loadAlls.incrementAndGet();
    notifyListeners();
  }
}
