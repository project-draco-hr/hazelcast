{
  final PortableFactory factory=new PortableFactory(){
    public Portable create(    int classId){
switch (classId) {
case CREATE:
        return new CreateTransactionRequest();
case COMMIT:
      return new CommitTransactionRequest();
case ROLLBACK:
    return new RollbackTransactionRequest();
}
return null;
}
}
;
return factory;
}
