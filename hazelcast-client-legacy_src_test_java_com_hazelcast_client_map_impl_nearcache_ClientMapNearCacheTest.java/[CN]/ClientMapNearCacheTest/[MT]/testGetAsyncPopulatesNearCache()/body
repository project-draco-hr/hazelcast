{
  IMap<Integer,Integer> map=getNearCachedMapFromClient(newInvalidationEnabledNearCacheConfig());
  int size=1239;
  for (int i=0; i < size; i++) {
    map.put(i,i);
  }
  for (int i=0; i < size; i++) {
    Future async=map.getAsync(i);
    async.get();
  }
  for (int i=0; i < size; i++) {
    map.get(i);
  }
  assertThatOwnedEntryCountEquals(map,size);
}
