{
  if (isReferenceAligned(offset)) {
    return super.compareAndSwapObject(o,offset,expected,x);
  }
 else {
    throw new IllegalArgumentException("Unaligned memory accesses are not supported for CAS operations. " + "Offset must be " + OBJECT_REFERENCE_ALIGN + "-bytes "+ "aligned for object reference typed CAS, but it is "+ offset);
  }
}
