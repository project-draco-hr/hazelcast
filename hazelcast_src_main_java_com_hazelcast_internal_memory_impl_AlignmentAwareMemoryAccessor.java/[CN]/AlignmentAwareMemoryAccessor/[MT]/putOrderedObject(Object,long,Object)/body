{
  if (isReferenceAligned(offset)) {
    super.putOrderedObject(o,offset,x);
  }
 else {
    throw new IllegalArgumentException("Unaligned memory accesses are not supported for CAS operations. " + "Offset must be " + OBJECT_REFERENCE_ALIGN + "-bytes "+ "aligned for object reference typed ordered writes, but it is "+ offset);
  }
}
