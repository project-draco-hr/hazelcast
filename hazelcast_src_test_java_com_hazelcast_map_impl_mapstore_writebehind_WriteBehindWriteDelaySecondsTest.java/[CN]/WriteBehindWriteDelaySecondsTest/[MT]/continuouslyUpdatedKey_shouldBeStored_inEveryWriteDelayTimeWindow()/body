{
  final MapStoreWithCounter mapStore=new MapStoreWithCounter<Integer,String>();
  final IMap<Object,Object> map=TestMapUsingMapStoreBuilder.create().withMapStore(mapStore).withNodeCount(1).withNodeFactory(createHazelcastInstanceFactory(1)).withWriteDelaySeconds(6).withPartitionCount(1).build();
  for (int i=1; i <= 60; i++) {
    map.put(1,i);
    sleepMillis(500);
  }
  assertMinMaxStoreOperationsCount(2,mapStore);
  final int countStore=mapStore.countStore.get();
}
