{
  try {
    if (state == ACTIVE) {
      throw new IllegalStateException("Transaction is already active");
    }
    startTime=Clock.currentTimeMillis();
    txnId=invoke(new CreateXATransactionRequest(xid,timeout));
    state=ACTIVE;
  }
 catch (  Exception e) {
    throw ExceptionUtil.rethrow(e);
  }
}
