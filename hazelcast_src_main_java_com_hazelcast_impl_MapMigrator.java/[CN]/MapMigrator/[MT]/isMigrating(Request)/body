{
  if (blockMigrating != null) {
    if (!blocks[blockMigrating.getBlockId()].isMigrating()) {
      completeMigration();
    }
 else {
      if (req.key == null) {
        return true;
      }
 else       if (blockMigrating.getBlockId() == concurrentMapManager.getBlockId(req.key)) {
        return true;
      }
    }
  }
  return false;
}
