{
  if (req.key == null && req.blockId != -1 && concurrentMapManager.hashBlocks() != req.blockId) {
    logger.log(Level.FINEST,thisAddress + " blockHashes aren't the same:" + concurrentMapManager.hashBlocks()+ ", request.blockId:"+ req.blockId+ " caller: "+ req.caller);
    return true;
  }
  if (blockMigrating != null) {
    if (!blocks[blockMigrating.getBlockId()].isMigrating()) {
      completeMigration();
    }
 else {
      if (req.key == null) {
        return true;
      }
 else       if (blockMigrating.getBlockId() == concurrentMapManager.getBlockId(req.key)) {
        return true;
      }
    }
  }
  return false;
}
