{
  out.writeUTF(name);
  out.writeUTF(jobId);
  out.writeObject(keyValueSource);
  out.writeInt(keys == null ? 0 : keys.size());
  if (keys != null) {
    for (int i=0; i < keys.size(); i++) {
      out.writeObject(keys);
    }
  }
  out.writeObject(mapper);
  out.writeObject(combinerFactory);
  out.writeObject(reducerFactory);
}
