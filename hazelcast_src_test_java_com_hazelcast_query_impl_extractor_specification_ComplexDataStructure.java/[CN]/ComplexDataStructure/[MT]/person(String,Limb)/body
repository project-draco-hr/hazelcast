{
  Person person=new Person();
  person.name=name;
  if (limbs.length > 0) {
    person.limbs_list.addAll(Arrays.asList(limbs));
  }
 else {
    person.limbs_list=null;
  }
  if (limbs.length > 0) {
    person.firstLimb=limbs[0];
  }
  if (limbs.length > 1) {
    person.secondLimb=limbs[1];
  }
  person.limbs_array=limbs;
  return person;
}
