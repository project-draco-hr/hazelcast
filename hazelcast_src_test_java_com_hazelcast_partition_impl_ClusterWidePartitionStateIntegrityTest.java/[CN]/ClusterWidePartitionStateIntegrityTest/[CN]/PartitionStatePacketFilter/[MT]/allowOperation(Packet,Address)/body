{
  if (!blockedAddresses.contains(endpoint)) {
    return true;
  }
  try {
    String className=readOperationClassName(packet);
    if (PartitionStateOperation.class.getName().equals(className)) {
      return false;
    }
  }
 catch (  IOException e) {
    throw new HazelcastException(e);
  }
  return true;
}
