{
  try {
    for (; ; ) {
      Object task=globalExecutorPriorityQueue.poll();
      if (task == null) {
        break;
      }
      processor.process(task);
    }
    if (task != null) {
      processor.process(task);
    }
  }
 catch (  Throwable t) {
    inspectOutputMemoryError(t);
    logger.severe(t);
  }
}
