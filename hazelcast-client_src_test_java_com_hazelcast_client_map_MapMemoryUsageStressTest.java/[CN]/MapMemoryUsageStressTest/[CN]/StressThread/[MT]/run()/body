{
  try {
    for (; ; ) {
      int index=counter.decrementAndGet();
      if (index <= 0) {
        return;
      }
      IMap<Object,Object> map=client.getMap("juka" + index);
      map.set("aaaa","bbbb");
      map.clear();
      map.destroy();
      if (index % 1000 == 0) {
        System.out.println("At: " + index);
      }
    }
  }
 catch (  Throwable t) {
    errors.incrementAndGet();
    t.printStackTrace();
  }
}
