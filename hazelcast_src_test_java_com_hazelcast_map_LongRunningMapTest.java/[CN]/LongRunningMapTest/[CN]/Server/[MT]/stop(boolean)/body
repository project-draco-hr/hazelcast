{
  running=false;
  if (latch.await(100,TimeUnit.SECONDS)) {
    Thread.sleep(2 * 1000);
    if (shutdown) {
      ins.getLifecycleService().shutdown();
    }
    System.out.println("successfully stopped");
    return true;
  }
 else {
    System.out.println("latch not finished properly");
    return false;
  }
}
