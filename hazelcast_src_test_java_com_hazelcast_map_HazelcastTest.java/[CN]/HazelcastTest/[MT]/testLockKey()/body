{
  IMap<String,String> map=getInstance().getMap("testLockKey");
  map.lock("Hello");
  try {
    assertFalse(map.containsKey("Hello"));
  }
  finally {
    map.unlock("Hello");
  }
  map.put("Hello","World");
  map.lock("Hello");
  try {
    assertTrue(map.containsKey("Hello"));
  }
  finally {
    map.unlock("Hello");
  }
  map.remove("Hello");
  map.lock("Hello");
  try {
    assertFalse(map.containsKey("Hello"));
  }
  finally {
    map.unlock("Hello");
  }
}
