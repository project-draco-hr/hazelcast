{
  HazelcastConnection c=getConnection();
  IMap<Integer,String> m=c.getMap("testMap");
  Integer key1=1;
  String value1="value1";
  Integer key2=2;
  String value2="value2";
  m.put(key1,value1);
  m.put(key2,value2);
  assertEquals(m.get(key1),value1);
  assertEquals(m.get(key2),value2);
  c.close();
}
