{
  Invocation inv=obtainServiceInvocation();
  try {
    inv.set(name,(add) ? OP_LISTENER_ADD : OP_LISTENER_REMOVE,key,null);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  inv.longValue=(includeValue) ? 1 : 0;
  boolean sent=send(inv,toAddress);
  if (!sent) {
    inv.returnToContainer();
  }
}
