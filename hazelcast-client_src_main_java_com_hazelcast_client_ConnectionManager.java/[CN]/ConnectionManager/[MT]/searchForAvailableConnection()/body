{
  Connection connection=null;
  popAndPush(clusterMembers);
  int counter=clusterMembers.size();
  while (counter > 0) {
    try {
      connection=getNextConnection();
      break;
    }
 catch (    Exception e) {
      popAndPush(clusterMembers);
      counter--;
    }
  }
  return connection;
}
