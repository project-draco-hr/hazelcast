{
  NativeMemoryConfig nativeMemoryConfig=mapService.getMapServiceContext().getNodeEngine().getConfig().getNativeMemoryConfig();
  boolean clearPartitions=!onShutdown || (nativeMemoryConfig != null && nativeMemoryConfig.getAllocatorType() != MemoryAllocatorType.POOLED);
  if (clearPartitions) {
    for (    RecordStore recordStore : maps.values()) {
      recordStore.clearPartition();
    }
  }
  maps.clear();
}
