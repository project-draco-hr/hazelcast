{
  enqueueAndReturn(new Processable(){
    public void process(){
      MemberImpl member=getMember(connection.getEndPoint());
      if (member != null) {
        member.didRead();
      }
      if (!thisAddress.equals(connection.getEndPoint())) {
        sendProcessableTo(createInitialProcess(),connection);
      }
    }
  }
);
}
