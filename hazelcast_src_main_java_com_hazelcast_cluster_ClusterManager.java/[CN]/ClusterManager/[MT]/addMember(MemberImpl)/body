{
  logger.log(Level.FINEST,"ClusterManager adding " + member);
  if (lsMembers.contains(member)) {
    for (    MemberImpl m : lsMembers) {
      if (m.equals(member)) {
        member=m;
      }
    }
    mapMembers.put(member.getAddress(),member);
  }
 else {
    lsMembers.add(member);
    mapMembers.put(member.getAddress(),member);
  }
  return member;
}
