{
  if (toAddress == null) {
    toAddress=node.getMasterAddress();
  }
  JoinRequest joinRequest=node.createJoinInfo(withCredentials);
  Invocation inv=node.nodeService.createSingleInvocation(SERVICE_NAME,joinRequest,-1).setTarget(toAddress).setTryCount(1).build();
  inv.invoke();
  return true;
}
