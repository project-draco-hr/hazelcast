{
  List<Future> calls=new ArrayList<Future>();
  for (  MemberImpl member : getMembers()) {
    if (!member.localMember() && !member.isLiteMember()) {
      Invocation inv=node.nodeService.createSingleInvocation(SERVICE_NAME,new FinalizeJoin(),-1).setTarget(member.getAddress()).setTryCount(1).build();
      calls.add(inv.invoke());
    }
  }
}
