{
  return new PortableFactory(){
    public Portable create(    int classId){
switch (classId) {
case LOCK:
        return new LockRequest();
case UNLOCK:
      return new UnlockRequest();
case IS_LOCKED:
    return new IsLockedRequest();
case DESTROY:
  return new LockDestroyRequest();
}
return null;
}
}
;
}
