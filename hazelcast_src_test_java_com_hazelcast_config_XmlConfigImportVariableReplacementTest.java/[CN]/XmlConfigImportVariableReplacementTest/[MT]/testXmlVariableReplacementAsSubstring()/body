{
  String xml="<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "    <properties>\n" + "        <property name=\"${env}-with-suffix\">local-with-suffix</property>\n"+ "        <property name=\"with-prefix-${env}\">with-prefix-local</property>\n"+ "    </properties>\n"+ "</hazelcast>";
  Config config=buildConfig(xml,"env","local");
  assertEquals(config.getProperty("local-with-suffix"),"local-with-suffix");
  assertEquals(config.getProperty("with-prefix-local"),"with-prefix-local");
}
