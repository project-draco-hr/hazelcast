{
  for (int i=0; i < MAX_REPLICA_COUNT; i++) {
    if (deadAddress.equals(addresses[i])) {
      for (int a=i; a + 1 < MAX_REPLICA_COUNT; a++) {
        setReplicaAddress(a,addresses[a + 1]);
      }
      setReplicaAddress(MAX_REPLICA_COUNT - 1,null);
      return true;
    }
  }
  return false;
}
