{
  selectorQueue.clear();
  try {
    addTask(new Runnable(){
      @Override public void run(){
        live=false;
        shutdownLatch.countDown();
      }
    }
);
    interrupt();
  }
 catch (  Throwable t) {
    Logger.getLogger(AbstractIOSelector.class).finest("Exception while waiting for shutdown",t);
  }
}
