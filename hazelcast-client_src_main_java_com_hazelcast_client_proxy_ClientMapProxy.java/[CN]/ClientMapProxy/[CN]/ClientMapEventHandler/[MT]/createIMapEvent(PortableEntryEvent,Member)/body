{
  IMapEvent iMapEvent;
switch (event.getEventType()) {
case ADDED:
case REMOVED:
case UPDATED:
case EVICTED:
    iMapEvent=createEntryEvent(event,member);
  break;
case EVICT_ALL:
case CLEAR_ALL:
iMapEvent=createMapEvent(event,member);
break;
default :
throw new IllegalArgumentException("Not a known event type " + event.getEventType());
}
return iMapEvent;
}
