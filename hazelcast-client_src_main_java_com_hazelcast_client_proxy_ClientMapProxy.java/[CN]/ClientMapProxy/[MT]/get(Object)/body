{
  initNearCache();
  final Data keyData=toData(key);
  if (nearCache != null) {
    Object cached=nearCache.get(keyData);
    if (cached != null) {
      return (V)cached;
    }
  }
  MapGetRequest request=new MapGetRequest(name,keyData);
  final V result=invoke(request,keyData);
  if (nearCache != null) {
    nearCache.put(keyData,result);
  }
  return result;
}
