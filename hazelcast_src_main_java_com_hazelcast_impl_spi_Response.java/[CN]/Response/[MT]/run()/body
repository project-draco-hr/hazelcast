{
  if (opBeforeData != null) {
    Operation op=(Operation)toObject(opBeforeData);
    op.getOperationContext().setCallId(getOperationContext().getCallId()).setService(getOperationContext().getService()).setPartitionId(getOperationContext().getPartitionId()).setCaller(getOperationContext().getCaller()).setCallId(getOperationContext().getCallId()).setNodeService(getOperationContext().getNodeService());
    op.run();
  }
  long callId=getOperationContext().getCallId();
  getOperationContext().getNodeService().notifyCall(callId,Response.this);
}
