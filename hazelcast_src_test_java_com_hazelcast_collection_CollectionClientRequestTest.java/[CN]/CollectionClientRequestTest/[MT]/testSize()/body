{
  MultiMap<Object,Object> mm=getMultiMap();
  mm.put("key1","value1");
  mm.put("key2","value1");
  mm.put("key2","value2");
  mm.put("key3","value2");
  final SimpleClient client=getClient();
  client.send(new SizeRequest(name));
  int result=(Integer)client.receive();
  assertEquals(result,4);
  assertEquals(result,mm.size());
}
