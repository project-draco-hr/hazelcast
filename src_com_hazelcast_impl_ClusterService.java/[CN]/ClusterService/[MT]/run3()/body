{
  Object obj=null;
  while (running) {
    try {
      obj=queue.take();
      process(obj);
    }
 catch (    final InterruptedException e) {
      Node.get().handleInterruptedException(Thread.currentThread(),e);
    }
catch (    final Exception e) {
      if (DEBUG) {
        logger.log(Level.FINEST,e + ",  message: " + e.getMessage()+ "  obj="+ obj);
      }
      e.printStackTrace();
    }
  }
}
