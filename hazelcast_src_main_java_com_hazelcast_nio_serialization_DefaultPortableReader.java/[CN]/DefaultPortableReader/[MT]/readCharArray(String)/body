{
  final int currentPos=in.position();
  try {
    int pos=getPosition(fieldName);
    in.position(pos);
    final int len=in.readInt();
    final char[] values=new char[len];
    for (int i=0; i < len; i++) {
      values[i]=in.readChar();
    }
    return values;
  }
  finally {
    in.position(currentPos);
  }
}
