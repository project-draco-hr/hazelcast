{
  int unsignedShortV=Short.MAX_VALUE + 1;
  ByteArrayObjectDataOutput dataOutput=new ByteArrayObjectDataOutput(2,mockSerializationService,ByteOrder.BIG_ENDIAN);
  dataOutput.writeShort(unsignedShortV);
  ByteArrayObjectDataInput dataInput=new ByteArrayObjectDataInput(dataOutput.toByteArray(),mockSerializationService,ByteOrder.BIG_ENDIAN);
  int readShort=dataInput.readUnsignedShort();
  assertEquals(unsignedShortV,readShort);
}
