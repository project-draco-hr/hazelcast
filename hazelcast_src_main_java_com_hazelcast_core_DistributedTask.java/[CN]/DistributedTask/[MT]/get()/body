{
  if (!done || !passed) {
    inner.get();
    passed=true;
  }
  if (cancelled) {
    throw new CancellationException();
  }
 else   if (memberLeftException != null) {
    throw memberLeftException;
  }
 else   if (exception != null) {
    throw new ExecutionException(exception);
  }
  return result;
}
