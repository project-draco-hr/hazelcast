{
  if (backupOp != null) {
    try {
      if (backupOp.getNodeEngine() == null) {
        backupOp.setNodeEngine(getNodeEngine());
      }
      backupOp.onExecutionFailure(e);
    }
 catch (    Throwable t) {
      getLogger().warning("While calling operation.onFailure(). op: " + backupOp,t);
    }
  }
}
