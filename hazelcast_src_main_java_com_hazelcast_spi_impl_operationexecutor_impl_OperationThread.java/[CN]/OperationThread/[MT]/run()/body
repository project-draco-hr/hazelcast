{
  nodeExtension.onThreadStart(this);
  try {
    while (!shutdown) {
      Object task;
      try {
        task=queue.take(priority);
      }
 catch (      InterruptedException e) {
        continue;
      }
      process(task);
    }
  }
 catch (  Throwable t) {
    inspectOutputMemoryError(t);
    logger.severe(t);
  }
 finally {
    nodeExtension.onThreadStop(this);
  }
}
