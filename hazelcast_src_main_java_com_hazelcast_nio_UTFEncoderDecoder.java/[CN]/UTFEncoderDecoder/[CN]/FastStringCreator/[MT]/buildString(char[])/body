{
  try {
    if (useOldStringConstructor) {
      return constructor.newInstance(0,chars.length,chars);
    }
 else {
      Object[] args=NEW_ARGS_THREAD_LOCAL.get();
      args[0]=chars;
      try {
        return constructor.newInstance(args);
      }
  finally {
        args[0]=null;
      }
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
