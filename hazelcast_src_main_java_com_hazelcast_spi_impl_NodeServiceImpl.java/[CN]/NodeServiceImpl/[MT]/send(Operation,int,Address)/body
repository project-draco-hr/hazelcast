{
  if (target == null || getThisAddress().equals(target)) {
    System.out.println("tARGET >>> " + target + "  get this "+ getThisAddress());
    op.setNodeService(this);
    runOperation(op);
    return true;
  }
 else {
    return send(op,partitionId,node.getConnectionManager().getOrConnect(target));
  }
}
