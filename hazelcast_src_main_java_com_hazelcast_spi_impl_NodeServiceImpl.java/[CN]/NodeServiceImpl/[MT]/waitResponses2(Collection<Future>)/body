{
  while (futures.size() > 0) {
    final Iterator<Future> iter=futures.iterator();
    while (iter.hasNext()) {
      final Future f=iter.next();
      try {
        f.get(1,TimeUnit.MILLISECONDS);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
      if (f.isDone()) {
        iter.remove();
      }
    }
  }
}
