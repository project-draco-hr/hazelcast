{
  out.writeUTF(name);
  int size=scoreMap.size();
  out.writeInt(size);
  for (  User user : scoreMap.keySet()) {
    out.writeObject(user);
    List<Score> scores=scoreMap.get(user);
    int lsize=scores.size();
    out.writeInt(lsize);
    for (    Score score : scores) {
      out.writeObject(score);
    }
  }
}
