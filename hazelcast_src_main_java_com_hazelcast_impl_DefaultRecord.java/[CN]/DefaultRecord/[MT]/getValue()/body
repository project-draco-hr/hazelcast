{
  final Object currentValue=valueObject;
  if (currentValue != null) {
    return currentValue;
  }
synchronized (DefaultRecord.this) {
    if (valueObject != null) {
      return valueObject;
    }
    Object v=toObject(value);
    if (cmap.cacheValue) {
      valueObject=v;
    }
    return v;
  }
}
