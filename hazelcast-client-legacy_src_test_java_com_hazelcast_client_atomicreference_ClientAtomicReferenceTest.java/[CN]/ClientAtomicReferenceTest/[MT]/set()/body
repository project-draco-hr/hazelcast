{
  clientReference.set(null);
  assertTrue(serverReference.isNull());
  clientReference.set("foo");
  assertEquals("foo",serverReference.get());
  clientReference.set("foo");
  assertEquals("foo",serverReference.get());
  clientReference.set("bar");
  assertEquals("bar",serverReference.get());
  clientReference.set(null);
  assertTrue(serverReference.isNull());
}
