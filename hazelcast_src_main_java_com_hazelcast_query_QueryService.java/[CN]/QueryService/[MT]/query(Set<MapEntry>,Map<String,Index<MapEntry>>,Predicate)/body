{
  try {
    final BlockingQueue<Boolean> resultQ=new ArrayBlockingQueue<Boolean>(1);
    queryQ.put(new Runnable(){
      public void run(){
        resultQ.offer(doQuery(results,namedIndexes,predicate));
      }
    }
);
    return resultQ.take();
  }
 catch (  InterruptedException ignore) {
  }
  return false;
}
