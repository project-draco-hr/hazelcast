{
  Config config=Config.get();
  this.multicastSocket=multicastSocket;
  this.datagramPacketReceive=new DatagramPacket(new byte[bufferSize],bufferSize);
  this.datagramPacketSend=new DatagramPacket(new byte[bufferSize],bufferSize,InetAddress.getByName(config.join.multicastConfig.multicastGroup),config.join.multicastConfig.multicastPort);
  running=true;
}
