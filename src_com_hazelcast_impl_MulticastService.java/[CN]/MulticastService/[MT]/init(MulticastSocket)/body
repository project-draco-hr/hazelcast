{
  Config config=Config.get();
  this.multicastSocket=multicastSocket;
  this.datagramPacketReceive=new DatagramPacket(new byte[bufferSize],bufferSize);
  this.datagramPacketSend=new DatagramPacket(new byte[bufferSize],bufferSize,InetAddress.getByName(config.getJoin().getMulticastConfig().getMulticastGroup()),config.getJoin().getMulticastConfig().getMulticastPort());
  running=true;
}
