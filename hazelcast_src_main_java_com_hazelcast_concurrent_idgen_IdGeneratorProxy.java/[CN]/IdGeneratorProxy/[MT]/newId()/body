{
  int value=residue.getAndIncrement();
  if (value >= BLOCK_SIZE) {
synchronized (syncObject) {
      value=residue.get();
      if (value >= BLOCK_SIZE) {
        local.set(atomicNumber.getAndIncrement());
        residue.set(0);
      }
      return newId();
    }
  }
  return local.get() * BLOCK_SIZE + value;
}
