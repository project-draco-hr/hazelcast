{
  if (rawResponse == null) {
    throw new IllegalArgumentException("response can't be null: " + invocation);
  }
  if (rawResponse instanceof NormalResponse) {
    rawResponse=((NormalResponse)rawResponse).getValue();
  }
  if (rawResponse == null) {
    rawResponse=Invocation.NULL_RESPONSE;
  }
  return rawResponse;
}
