{
  if (partitionStateManager.isInitialized()) {
    return;
  }
  if (!node.isMaster()) {
    triggerMasterToAssignPartitions();
    return;
  }
  lock.lock();
  try {
    if (partitionStateManager.isInitialized()) {
      return;
    }
    if (!partitionStateManager.initializePartitionAssignments()) {
      return;
    }
    publishPartitionRuntimeState();
  }
  finally {
    lock.unlock();
  }
}
