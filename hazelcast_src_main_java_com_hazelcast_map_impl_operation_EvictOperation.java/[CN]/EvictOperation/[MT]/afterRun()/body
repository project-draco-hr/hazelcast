{
  if (evicted) {
    mapService.getMapServiceContext().interceptAfterRemove(name,dataValue);
    EntryEventType eventType=EntryEventType.EVICTED;
    mapService.getMapServiceContext().getMapEventPublisher().publishEvent(getCallerAddress(),name,eventType,dataKey,dataValue,null);
    invalidateNearCaches();
  }
}
