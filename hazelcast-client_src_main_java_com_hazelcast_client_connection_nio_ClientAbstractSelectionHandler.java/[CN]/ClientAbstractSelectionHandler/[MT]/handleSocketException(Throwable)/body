{
  if (sk != null) {
    sk.cancel();
  }
  connection.close(e);
  StringBuilder sb=new StringBuilder();
  sb.append(Thread.currentThread().getName());
  sb.append(" Closing socket to endpoint ");
  sb.append(connection.getEndPoint());
  sb.append(", Cause:").append(e);
  logger.warning(sb.toString());
}
