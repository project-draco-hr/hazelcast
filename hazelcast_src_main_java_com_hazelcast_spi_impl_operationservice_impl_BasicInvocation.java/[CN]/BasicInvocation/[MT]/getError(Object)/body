{
  if (obj == null) {
    return null;
  }
  if (obj instanceof Throwable) {
    return (Throwable)obj;
  }
  if (!(obj instanceof NormalResponse)) {
    return null;
  }
  NormalResponse response=(NormalResponse)obj;
  if (!(response.getValue() instanceof Throwable)) {
    return null;
  }
  return (Throwable)response.getValue();
}
