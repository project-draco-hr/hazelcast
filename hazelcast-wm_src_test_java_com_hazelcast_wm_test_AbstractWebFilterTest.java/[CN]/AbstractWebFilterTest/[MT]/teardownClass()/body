{
  for (  Map.Entry<Class<? extends AbstractWebFilterTest>,ContainerContext> ccEntry : CONTAINER_CONTEXT_MAP.entrySet()) {
    ContainerContext cc=ccEntry.getValue();
    try {
      cc.server1.stop();
      if (cc.server2 != null) {
        cc.server2.stop();
      }
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
  Hazelcast.shutdownAll();
}
