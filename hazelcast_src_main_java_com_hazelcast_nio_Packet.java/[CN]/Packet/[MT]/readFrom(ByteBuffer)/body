{
  if (!readVersion(source)) {
    return false;
  }
  if (!readHeader(source)) {
    return false;
  }
  if (!readPartition(source)) {
    return false;
  }
  if (!readSize(source)) {
    return false;
  }
  if (!readValue(source)) {
    return false;
  }
  setPersistStatus(PERSIST_COMPLETED);
  return true;
}
