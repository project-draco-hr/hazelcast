{
  if (!headComplete) {
    if (src.remaining() < HEAD_SIZE) {
      return false;
    }
    byte version=src.get();
    if (VERSION != version) {
      throw new IllegalArgumentException("Packet versions are not matching! Expected -> " + VERSION + ", Incoming -> "+ version);
    }
    header=src.getShort();
    partitionId=src.getInt();
    size=src.getInt();
    headComplete=true;
  }
  return readValue(src);
}
