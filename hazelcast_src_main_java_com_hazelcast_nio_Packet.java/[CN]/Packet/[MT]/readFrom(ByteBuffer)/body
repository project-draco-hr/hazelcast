{
  if (!isStatusSet(stHeader)) {
    if (source.remaining() < 2) {
      return false;
    }
    header=source.getShort();
    setStatus(stHeader);
  }
  if (!isStatusSet(stPartition)) {
    if (source.remaining() < 4) {
      return false;
    }
    partitionId=source.getInt();
    setStatus(stPartition);
  }
  return super.readFrom(source);
}
