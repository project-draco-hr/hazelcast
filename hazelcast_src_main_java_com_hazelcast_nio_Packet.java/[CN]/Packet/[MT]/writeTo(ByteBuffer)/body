{
  if (!writeVersion(destination)) {
    return false;
  }
  if (!writeHeader(destination)) {
    return false;
  }
  if (!writePartition(destination)) {
    return false;
  }
  if (!writeSize(destination)) {
    return false;
  }
  if (!writeValue(destination)) {
    return false;
  }
  setPersistStatus(PERSIST_COMPLETED);
  return true;
}
