{
  if (!writeVersion(dst)) {
    return false;
  }
  if (!writeHeader(dst)) {
    return false;
  }
  if (!writePartition(dst)) {
    return false;
  }
  if (!writeSize(dst)) {
    return false;
  }
  if (!writeValue(dst)) {
    return false;
  }
  setPersistStatus(PERSIST_COMPLETED);
  return true;
}
