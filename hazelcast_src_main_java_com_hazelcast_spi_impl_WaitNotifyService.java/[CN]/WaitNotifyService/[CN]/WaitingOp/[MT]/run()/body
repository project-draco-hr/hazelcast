{
  if (valid) {
    if (expired()) {
      queue.remove(this);
      waitSupport.onWaitExpire();
    }
 else     if (timedOut()) {
      queue.remove(this);
      Object response=new CallTimeoutException("Call timed out for " + op.getClass().getName());
      op.getResponseHandler().sendResponse(response);
    }
  }
}
