{
  if (logger.isLoggable(Level.FINE)) {
    logger.log(Level.FINE,"Received created notification {0} {1}",new String[]{event.getDistributedObject().getName(),event.getDistributedObject().toString()});
  }
  if (creationStats != null) {
    creationStats.addEvent();
  }
  registerInstance(event.getDistributedObject());
}
