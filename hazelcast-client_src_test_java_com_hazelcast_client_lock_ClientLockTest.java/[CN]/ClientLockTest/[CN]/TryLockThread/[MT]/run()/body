{
  for (int i=0; i < 1000 * 10; i++) {
    try {
      if (lock.tryLock(10,TimeUnit.MILLISECONDS)) {
        int delta=random.nextInt(1000);
        upTotal.addAndGet(delta);
        downTotal.addAndGet(-delta);
        lock.unlock();
      }
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
}
