{
  l.lock();
  final CountDownLatch latch=new CountDownLatch(1);
  new Thread(){
    public void run(){
      try {
        if (!l.tryLock(0,TimeUnit.SECONDS)) {
          latch.countDown();
        }
      }
 catch (      InterruptedException e) {
      }
    }
  }
.start();
  assertOpenEventually(latch);
  l.forceUnlock();
}
