{
  Index index=queryContext.getMapIndexes().get(first);
  if (index != null) {
    long[] longValues=new long[values.length];
    for (int i=0; i < values.length; i++) {
      longValues[i]=index.getLongValue(values[i]);
    }
    return index.getRecords(longValues);
  }
 else {
    return null;
  }
}
