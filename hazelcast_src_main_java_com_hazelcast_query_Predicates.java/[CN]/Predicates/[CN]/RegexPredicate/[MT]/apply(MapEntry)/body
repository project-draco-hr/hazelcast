{
  String firstVal=first.getValue(entry);
  if (firstVal == null) {
    return (regex == null);
  }
 else   if (regex == null) {
    return false;
  }
 else {
    if (pattern == null) {
      pattern=Pattern.compile(regex);
    }
    Matcher m=pattern.matcher(firstVal);
    return m.matches();
  }
}
