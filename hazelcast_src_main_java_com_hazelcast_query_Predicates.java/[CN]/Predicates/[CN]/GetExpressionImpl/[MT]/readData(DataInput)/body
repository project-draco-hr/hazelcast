{
  boolean inputString=in.readBoolean();
  if (inputString) {
    input=in.readUTF();
  }
 else {
    DataSerializable ds=null;
    try {
      ds=(DataSerializable)Class.forName(in.readUTF()).newInstance();
      ds.readData(in);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    input=ds;
  }
}
