{
  if (obj instanceof MapEntry) {
    obj=((MapEntry)obj).getValue();
  }
  try {
    if (input instanceof Method) {
      return ((Method)input).invoke(obj);
    }
 else {
      Method m=obj.getClass().getMethod((String)input,null);
      return m.invoke(obj);
    }
  }
 catch (  Throwable e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
}
