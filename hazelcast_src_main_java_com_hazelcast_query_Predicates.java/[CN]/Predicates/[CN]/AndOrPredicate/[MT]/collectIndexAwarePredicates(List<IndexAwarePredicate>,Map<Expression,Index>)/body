{
  boolean strong=and;
  if (!mapIndexes.isEmpty()) {
    lsIndexPredicates.add(this);
    if (strong) {
      final List<IndexAwarePredicate> indexPredicates=new ArrayList<IndexAwarePredicate>(predicates.length);
      for (      Predicate predicate : predicates) {
        if (predicate instanceof IndexAwarePredicate) {
          IndexAwarePredicate p=(IndexAwarePredicate)predicate;
          if (!p.collectIndexAwarePredicates(indexPredicates,mapIndexes)) {
            strong=false;
          }
        }
 else {
          strong=false;
        }
        if (!strong) {
          break;
        }
      }
    }
    return strong;
  }
  if (and) {
    for (    Predicate predicate : predicates) {
      if (predicate instanceof IndexAwarePredicate) {
        IndexAwarePredicate p=(IndexAwarePredicate)predicate;
        if (!p.collectIndexAwarePredicates(lsIndexPredicates,mapIndexes)) {
          strong=false;
        }
      }
 else {
        strong=false;
      }
    }
  }
  return strong;
}
