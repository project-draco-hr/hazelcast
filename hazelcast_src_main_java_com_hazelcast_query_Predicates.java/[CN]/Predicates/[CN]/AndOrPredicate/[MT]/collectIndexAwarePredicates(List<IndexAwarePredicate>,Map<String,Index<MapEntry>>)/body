{
  boolean strong=and;
  if (and) {
    for (    Predicate predicate : predicates) {
      if (predicate instanceof IndexAwarePredicate) {
        IndexAwarePredicate p=(IndexAwarePredicate)predicate;
        if (!p.collectIndexAwarePredicates(lsIndexPredicates,namedIndexes)) {
          strong=false;
        }
      }
 else {
        strong=false;
      }
    }
  }
  return strong;
}
