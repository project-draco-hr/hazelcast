{
  String firstVal=(String)readAttribute(entry,attribute);
  if (firstVal == null) {
    return (second == null);
  }
 else   if (second == null) {
    return false;
  }
 else {
    if (pattern == null) {
      pattern=Pattern.compile(second.replaceAll("%",".*").replaceAll("_","."));
    }
    Matcher m=pattern.matcher(firstVal);
    return m.matches();
  }
}
