{
  if (base == null) {
    String baseClassName=servletConfig.getInitParameter(HAZELCAST_BASE_SERVLET_CLASS);
    try {
      base=(GenericServlet)Class.forName(baseClassName).newInstance();
      debug("Will use base servlet class " + base);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
