{
  int timeout=-1;
  try {
    timeout=PropertiesHelper.getInt(LOCK_TIMEOUT,props,-1);
  }
 catch (  Exception ignored) {
  }
  if (timeout < 0) {
    try {
      timeout=PropertiesHelper.getInt(LOCK_TIMEOUT_SECONDS,props,-1) * 1000;
    }
 catch (    Exception ignored) {
    }
  }
  if (timeout < 0) {
    timeout=MAXIMUM_LOCK_TIMEOUT;
  }
  return timeout;
}
