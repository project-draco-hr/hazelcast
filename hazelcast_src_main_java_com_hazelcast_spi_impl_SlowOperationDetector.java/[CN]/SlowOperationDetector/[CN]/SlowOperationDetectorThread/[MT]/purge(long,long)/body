{
  if (nowNanos - lastLogPurge <= logPurgeIntervalNanos) {
    return false;
  }
  for (  SlowOperationLog log : slowOperationLogs.values()) {
    if (!running) {
      return false;
    }
    if (log.purgeInvocations(nowNanos,logRetentionNanos)) {
      slowOperationLogs.remove(log.getStackTrace().hashCode());
    }
  }
  return true;
}
