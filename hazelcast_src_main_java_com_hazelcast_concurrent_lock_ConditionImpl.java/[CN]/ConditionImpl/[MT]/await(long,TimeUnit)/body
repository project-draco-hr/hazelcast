{
  final NodeEngine nodeEngine=lockProxy.getNodeEngine();
  final Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(LockService.SERVICE_NAME,new AwaitOperation(lockProxy.namespace,lockProxy.key,ThreadContext.getThreadId()),partitionId).build();
  Future f=inv.invoke();
  try {
    return (Boolean)f.get(time,unit);
  }
 catch (  Throwable t) {
    return (Boolean)ExceptionUtil.rethrow(t);
  }
}
