{
  final NodeEngine nodeEngine=lockProxy.getNodeEngine();
  final Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(LockService.SERVICE_NAME,new SignalOperation(lockProxy.namespace,lockProxy.key,ThreadContext.getThreadId(),conditionId,all),partitionId).build();
  Future f=inv.invoke();
  try {
    f.get();
  }
 catch (  Throwable t) {
    ExceptionUtil.rethrow(t);
  }
}
