{
  boolean changed;
  if (oldAddress == null) {
    changed=newAddress != null;
  }
 else {
    changed=!oldAddress.equals(newAddress);
  }
  if (changed) {
    PartitionReplicaChangeEvent event=new PartitionReplicaChangeEvent(partitionId,replicaIndex,oldAddress,newAddress,reason);
    partitionListener.replicaChanged(event);
  }
}
