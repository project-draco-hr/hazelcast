{
  name=reader.readUTF("name");
  includeValue=reader.readBoolean("i");
  boolean hasKey=reader.readBoolean("key");
  if (reader.readBoolean("pre")) {
    predicate=reader.readUTF("p");
    final ObjectDataInput in=reader.getRawDataInput();
    if (hasKey) {
      key=new Data();
      key.readData(in);
    }
  }
 else   if (hasKey) {
    final ObjectDataInput in=reader.getRawDataInput();
    key=new Data();
    key.readData(in);
  }
}
