{
  checkThread();
  int evictionCount=(int)(cache.size() * 0.25);
  List<Data> lsRemoves=new ArrayList<Data>(evictionCount);
  int count=0;
  for (  Data key : sortedMap.keySet()) {
    lsRemoves.add(key);
    if (count++ >= evictionCount) {
      break;
    }
  }
  for (  Data key : lsRemoves) {
    invalidate(key);
  }
  lsRemoves.clear();
}
