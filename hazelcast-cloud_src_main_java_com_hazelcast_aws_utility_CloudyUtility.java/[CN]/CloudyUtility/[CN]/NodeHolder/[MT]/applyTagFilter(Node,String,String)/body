{
  if (nullOrEmpty(keyExpected)) {
    return true;
  }
 else {
    for (    NodeHolder group : new NodeHolder(node).getFirstSubNode("tagset").getSubNodes("item")) {
      if (keyEquals(keyExpected,group) && (nullOrEmpty(valueExpected) || valueEquals(valueExpected,group))) {
        return true;
      }
    }
    return false;
  }
}
