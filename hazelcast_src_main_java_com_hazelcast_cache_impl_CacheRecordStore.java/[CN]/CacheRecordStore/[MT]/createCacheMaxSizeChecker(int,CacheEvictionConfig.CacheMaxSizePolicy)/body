{
  if (maxSizePolicy == null) {
    throw new IllegalArgumentException("Max-Size policy cannot be null");
  }
  if (maxSizePolicy != CacheEvictionConfig.CacheMaxSizePolicy.ENTRY_COUNT) {
    throw new IllegalArgumentException("Invalid max-size policy " + "(" + maxSizePolicy + ") for "+ getClass().getName()+ " ! Only "+ CacheEvictionConfig.CacheMaxSizePolicy.ENTRY_COUNT+ " is supported.");
  }
 else {
    return super.createCacheMaxSizeChecker(size,maxSizePolicy);
  }
}
