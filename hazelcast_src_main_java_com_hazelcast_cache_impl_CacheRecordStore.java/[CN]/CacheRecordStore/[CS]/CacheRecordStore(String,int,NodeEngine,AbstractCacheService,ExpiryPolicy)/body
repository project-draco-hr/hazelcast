{
  super(name,partitionId,nodeEngine,cacheService,expiryPolicy);
  this.serializationService=nodeEngine.getSerializationService();
  this.cacheRecordFactory=createCacheRecordFactory();
  this.evictionTaskFuture=nodeEngine.getExecutionService().scheduleWithFixedDelay("hz:cache",new EvictionTask(),DEFAULT_EVICTION_TASK_INITIAL_DELAY,DEFAULT_EVICTION_TASK_PERIOD,TimeUnit.SECONDS);
}
