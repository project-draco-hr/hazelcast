{
  executedOperationsCount.incrementAndGet();
  RemoteCallKey callKey=null;
  try {
    if (timeout(op)) {
      return;
    }
    callKey=beforeCallExecution(op);
    ensureNoPartitionProblems(op);
    beforeRun(op);
    if (waitingNeeded(op)) {
      return;
    }
    op.run();
    handleResponse(op);
    afterRun(op);
  }
 catch (  Throwable e) {
    handleOperationError(op,e);
  }
 finally {
    afterCallExecution(op,callKey);
  }
}
