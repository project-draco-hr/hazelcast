{
  executedOperationsCount.incrementAndGet();
  try {
    if (timeout(op)) {
      return;
    }
    ensureNoPartitionProblems(op);
    op.beforeRun();
    if (waitingNeeded(op)) {
      return;
    }
    op.run();
    handleResponse(op);
    afterRun(op);
  }
 catch (  Throwable e) {
    handleOperationError(op,e);
  }
}
