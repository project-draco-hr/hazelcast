{
  if (target == null) {
    return new BasicPartitionInvocation(BasicOperationService.this,serviceName,op,partitionId,replicaIndex,tryCount,tryPauseMillis,callTimeout,callback,executorName,resultDeserialized).invoke();
  }
 else {
    return new BasicTargetInvocation(BasicOperationService.this,serviceName,op,target,tryCount,tryPauseMillis,callTimeout,callback,executorName,resultDeserialized).invoke();
  }
}
