{
  if (!isJoinOperation(op) && node.clusterService.getMember(op.getCallerAddress()) == null) {
    Exception error=new CallerNotMemberException(op.getCallerAddress(),op.getPartitionId(),op.getClass().getName(),op.getServiceName());
    operationProcessor.handleOperationError(op,error);
    return false;
  }
  return true;
}
