{
  out.writeUTF(engineName);
  out.writeUTF(script);
  if (bindings != null) {
    out.writeInt(bindings.size());
    Set<Entry<String,Object>> entries=bindings.entrySet();
    for (    Entry<String,Object> entry : entries) {
      out.writeUTF(entry.getKey());
      IOUtil.writeObject(out,entry.getValue());
    }
  }
 else {
    out.writeInt(0);
  }
}
