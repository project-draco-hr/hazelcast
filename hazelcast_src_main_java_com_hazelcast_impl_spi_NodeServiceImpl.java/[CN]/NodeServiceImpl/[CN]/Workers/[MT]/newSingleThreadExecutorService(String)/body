{
  return new ThreadPoolExecutor(1,1,0L,TimeUnit.MILLISECONDS,new LinkedBlockingQueue<Runnable>(),new ExecutorThreadFactory(partitionThreadGroup,node.hazelcastInstance,node.getThreadPoolNamePrefix(threadName),threadNumber,node.getConfig().getClassLoader()),new RejectedExecutionHandler(){
    public void rejectedExecution(    Runnable r,    ThreadPoolExecutor executor){
    }
  }
);
}
