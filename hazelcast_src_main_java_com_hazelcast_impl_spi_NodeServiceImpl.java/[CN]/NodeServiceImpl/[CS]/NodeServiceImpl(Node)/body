{
  this.node=node;
  logger=node.getLogger(NodeServiceImpl.class.getName());
  final ClassLoader classLoader=node.getConfig().getClassLoader();
  executorService=new ThreadPoolExecutor(2,Integer.MAX_VALUE,60L,TimeUnit.SECONDS,new SynchronousQueue(),new ExecutorThreadFactory(node,node.getThreadPoolNamePrefix("cached"),classLoader));
  scheduledExecutorService=new ScheduledThreadPoolExecutor(1,new ExecutorThreadFactory(node,node.getThreadPoolNamePrefix("scheduled"),classLoader));
  workers=new Workers("workers",2);
  partitionCount=node.groupProperties.CONCURRENT_MAP_PARTITION_COUNT.getInteger();
  maxBackupCount=MapConfig.MAX_BACKUP_COUNT;
}
