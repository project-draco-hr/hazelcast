{
  int counter=0;
  while (true) {
    counter++;
    System.out.println("Counter:" + counter);
    Packet response=null;
    try {
      response=reader.readPacket();
    }
 catch (    IOException e) {
      e.printStackTrace();
      return;
    }
    if (response != null)     System.out.println("Packet > " + response.getOperation());
    Call c=callMap.get(response.getCallId());
    if (c != null) {
synchronized (c) {
        c.setResponse(response);
        c.notify();
      }
    }
  }
}
