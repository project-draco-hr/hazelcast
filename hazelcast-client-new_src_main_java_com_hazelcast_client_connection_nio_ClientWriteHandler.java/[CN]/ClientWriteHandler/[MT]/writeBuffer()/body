{
  while (buffer.hasRemaining() && lastWritable != null) {
    boolean complete=lastWritable.writeTo(buffer);
    if (complete) {
      lastWritable=poll();
    }
 else {
      break;
    }
  }
  if (buffer.position() == 0) {
    return;
  }
  buffer.flip();
  socketChannel.write(buffer);
  if (buffer.hasRemaining()) {
    buffer.compact();
  }
 else {
    buffer.clear();
  }
}
