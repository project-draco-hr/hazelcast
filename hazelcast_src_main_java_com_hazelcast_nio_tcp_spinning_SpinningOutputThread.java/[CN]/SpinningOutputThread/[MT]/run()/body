{
  for (; ; ) {
    ConnectionHandlers handlers=connectionHandlers;
    if (handlers == SHUTDOWN) {
      return;
    }
    for (    SpinningWriteHandler handler : handlers.writeHandlers) {
      try {
        handler.write();
      }
 catch (      Throwable t) {
        handler.onFailure(t);
      }
    }
  }
}
