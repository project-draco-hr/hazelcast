{
  try {
    Object result=(timeout == -1) ? responseQueue.take() : responseQueue.poll(timeout,unit);
    if (result == null)     return null;
    if (result instanceof Data) {
      return ThreadContext.get().getObjectReaderWriter().readObject((Data)result);
    }
 else {
      return result;
    }
  }
 catch (  InterruptedException e) {
    finalizeTask();
    throw e;
  }
catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
