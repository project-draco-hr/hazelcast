{
  ClusterService.get().registerPacketProcessor(ClusterOperation.REMOTELY_EXECUTION,new PacketProcessor(){
    public void process(    Packet packet){
      handleRemoteExecution(packet);
    }
  }
);
  ClusterService.get().registerPacketProcessor(ClusterOperation.STREAM,new PacketProcessor(){
    public void process(    Packet packet){
      handleStream(packet);
    }
  }
);
}
