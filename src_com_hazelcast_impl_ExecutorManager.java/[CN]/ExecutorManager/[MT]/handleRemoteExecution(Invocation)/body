{
  if (DEBUG)   log("Remote Handling invocaiton %%%%5555... " + inv);
  Data callableData=inv.doTake(inv.data);
  RemoteExecutionId remoteExecutionId=new RemoteExecutionId(inv.conn.getEndPoint(),inv.longValue);
  SimpleExecution se=new SimpleExecution(remoteExecutionId,executor,null,callableData,null,false);
  mapRemoteExecutions.put(remoteExecutionId,se);
  executor.execute(se);
  inv.returnToContainer();
}
