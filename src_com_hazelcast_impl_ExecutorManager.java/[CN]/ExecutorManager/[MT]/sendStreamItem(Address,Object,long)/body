{
  try {
    final Invocation inv=ClusterManager.get().obtainServiceInvocation("exe",null,value,OP_STREAM,DEFAULT_TIMEOUT);
    inv.longValue=streamId;
    ClusterService.get().enqueueAndReturn(new Processable(){
      public void process(){
        send(inv,address);
      }
    }
);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
