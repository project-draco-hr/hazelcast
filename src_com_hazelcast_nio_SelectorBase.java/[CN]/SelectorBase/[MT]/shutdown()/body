{
  String msg="Selector shutting down  " + Thread.currentThread().getName();
  if (Build.DEBUG) {
    System.out.println(msg);
    ClusterManager.get().publishLog(msg);
  }
  live=false;
  selectorQueue.clear();
  try {
    selector.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
