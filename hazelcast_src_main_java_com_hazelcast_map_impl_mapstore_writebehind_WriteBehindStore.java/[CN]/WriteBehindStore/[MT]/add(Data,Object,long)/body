{
  if (InMemoryFormat.NATIVE.equals(inMemoryFormat)) {
    value=toData(value);
    key=toData(key);
  }
  if (InMemoryFormat.OBJECT.equals(inMemoryFormat)) {
    value=toData(value);
  }
  long writeDelay=this.writeDelayTime;
  long storeTime=now + writeDelay;
  DelayedEntry<Data,Object> delayedEntry=DelayedEntries.createDefault(key,value,storeTime,partitionId);
  add(delayedEntry);
  return value;
}
