{
  if (NATIVE == inMemoryFormat) {
    value=toData(value);
    key=toData(key);
  }
  if (!coalesce && OBJECT == inMemoryFormat) {
    value=toData(value);
  }
  long storeTime=now + writeDelayTime;
  DelayedEntry<Data,Object> delayedEntry=DelayedEntries.createDefault(key,value,storeTime,partitionId);
  add(delayedEntry);
  return value;
}
