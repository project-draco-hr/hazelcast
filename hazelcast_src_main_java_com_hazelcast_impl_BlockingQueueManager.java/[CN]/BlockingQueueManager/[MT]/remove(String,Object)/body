{
  Set<Long> keys=getValueKeys(name,toData(obj));
  if (keys != null) {
    for (    Long key : keys) {
      Data keyData=toData(key);
      if (removeKey(name,keyData)) {
        try {
          getStorageMap(name).tryRemove(keyData,0,TimeUnit.SECONDS);
        }
 catch (        TimeoutException ignored) {
        }
        return true;
      }
    }
  }
  return false;
}
