{
  Long key=generateKey(name,timeout);
  if (key != -1) {
    IMap imap=getStorageMap(name);
    final Data dataKey=toData(key);
    imap.put(dataKey,obj);
    if (addKeyAsync) {
      sendKeyToMaster(name,dataKey);
    }
 else {
      addKey(name,dataKey);
    }
    return true;
  }
  return false;
}
