{
  if (index < 0) {
    throw new IllegalArgumentException();
  }
  Data key=null;
  try {
    key=takeKey(name,index,0L);
  }
 catch (  InterruptedException ignored) {
  }
  if (key == null) {
    throw new IndexOutOfBoundsException();
  }
  IMap imap=getStorageMap(name);
  return imap.remove(key);
}
