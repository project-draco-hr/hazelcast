{
  enqueueAndReturn(new Processable(){
    public void process(){
      if (isMaster()) {
        doAddKey(queueName,key);
      }
 else {
        Packet packet=obtainPacket();
        packet.name=queueName;
        packet.setKey(key);
        packet.operation=ClusterOperation.BLOCKING_OFFER_KEY;
        boolean sent=send(packet,getMasterAddress());
      }
    }
  }
);
}
