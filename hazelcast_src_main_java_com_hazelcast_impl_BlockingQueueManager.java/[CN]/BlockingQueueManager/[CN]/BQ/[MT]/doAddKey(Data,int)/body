{
  if (keys.add(key)) {
    if (leases.size() > 0) {
      leases.remove(0);
    }
    if (index == Integer.MAX_VALUE || index >= queue.size()) {
      queue.offer(new QData(key));
    }
 else     if (index == 0) {
      queue.addFirst(new QData(key));
    }
 else {
      queue.add(index,new QData(key));
    }
    takeOne();
  }
}
