{
  MemberImpl nextMember=getNextMemberAfter(thisAddress,true,1);
  if (nextMember != null) {
    Address next=nextMember.getAddress();
    Packet packet=obtainPacket();
    packet.name=name;
    packet.operation=getOperation();
    packet.txnId=txnId;
    boolean sent=send(packet,next);
    if (!sent) {
      releasePacket(packet);
    }
  }
}
