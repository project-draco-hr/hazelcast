{
  IMap imap=hazelcast.getMap(name);
  Set localKeys=imap.localKeySet();
  if (localKeys != null) {
    Object itemObject=toObject(item);
    Keys keys=new Keys();
    for (    Object key : localKeys) {
      Object v=imap.get(key);
      if (v != null && v.equals(itemObject)) {
        keys.add(toData(key));
      }
    }
    return keys;
  }
  return null;
}
