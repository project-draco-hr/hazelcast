{
  int index=(int)packet.longValue;
  int blockId=packet.blockId;
  Block block=getBlock(blockId);
  packet.longValue=-1;
  if (block != null) {
    blockLoop:     for (int i=index; i < BLOCK_SIZE; i++) {
      Data data=block.get(i);
      if (data != null) {
        packet.value=data;
        packet.longValue=i;
        break blockLoop;
      }
    }
  }
  sendResponse(packet);
}
