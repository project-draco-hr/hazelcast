{
  Block block=getBlock(read.blockId);
  if (block == null) {
    read.setResponse(null,-1);
    return;
  }
  for (int i=read.index; i < BLOCK_SIZE; i++) {
    Data data=block.get(i);
    if (data != null) {
      Data value=doHardCopy(data);
      read.setResponse(value,i);
      return;
    }
  }
  read.setResponse(null,-1);
}
