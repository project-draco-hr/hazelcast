{
  if (zeroBackup)   return true;
  Block block=getBlock(blockId);
  if (block == null)   return false;
  if (thisAddress.equals(block.address)) {
    return false;
  }
  if (add) {
    boolean added=block.add(itemIndex,data);
    return true;
  }
 else {
    if (block.size() > 0) {
      block.remove(itemIndex);
    }
 else {
      return false;
    }
  }
  return true;
}
