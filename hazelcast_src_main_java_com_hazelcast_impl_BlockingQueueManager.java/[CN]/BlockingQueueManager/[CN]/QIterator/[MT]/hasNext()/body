{
  next=null;
  if (txnOffers != null) {
    boolean hasInTxn=txnOffers.hasNext();
    hasNextCalled=true;
    if (hasInTxn) {
      next=txnOffers.next();
      return true;
    }
 else {
      txnOffers=null;
    }
  }
  while (next == null) {
    boolean canRead=setNextBlock();
    if (!canRead)     return false;
    Read read=new Read();
    next=read.read(name,currentBlockId,currentIndex);
    if (next == null) {
      currentIndex=-1;
    }
 else {
      currentIndex=read.readIndex;
      currentIndex++;
    }
  }
  hasNextCalled=true;
  return true;
}
