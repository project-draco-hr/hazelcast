{
  Data data=next();
  if (data != null && data.size() != 0) {
    q.sendBackup(true,thisAddress,data,block.blockId,index);
    index++;
    if (index > indexUpto) {
      done=true;
    }
  }
 else {
    done=true;
  }
synchronized (BlockBackupSync.this) {
    BlockBackupSync.this.notify();
  }
}
