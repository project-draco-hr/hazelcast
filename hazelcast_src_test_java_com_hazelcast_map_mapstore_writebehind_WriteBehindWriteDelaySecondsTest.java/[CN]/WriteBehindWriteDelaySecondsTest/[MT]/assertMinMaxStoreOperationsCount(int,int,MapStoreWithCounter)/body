{
  assertTrueEventually(new AssertTask(){
    @Override public void run() throws Exception {
      final Object value=mapStore.store.get(1);
      final int countStore=mapStore.countStore.get();
      assertEquals(60,value);
      assertTrue("Minimum store operation count should be bigger than 1 but found = " + countStore,countStore >= minimumExpectedStoreOperationCount);
      assertTrue("Maximum store operation count should be smaller than " + maximumExpectedStoreOperationCount + " but found = "+ countStore,countStore <= maximumExpectedStoreOperationCount);
    }
  }
);
}
