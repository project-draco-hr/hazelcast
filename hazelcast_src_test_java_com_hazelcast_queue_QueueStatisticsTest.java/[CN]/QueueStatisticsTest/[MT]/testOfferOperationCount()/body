{
  final IQueue queue=newQueue();
  for (int i=0; i < 10; i++) {
    queue.offer("item" + i);
  }
  for (int i=0; i < 10; i++) {
    queue.add("item" + i);
  }
  for (int i=0; i < 10; i++) {
    queue.put("item" + i);
  }
  assertTrueEventually(new AssertTask(){
    @Override public void run() throws Exception {
      LocalQueueStats stats=queue.getLocalQueueStats();
      assertEquals(30,stats.getOfferOperationCount());
    }
  }
);
}
