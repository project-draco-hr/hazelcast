{
  IQueue queue=newQueue();
  for (int i=0; i < 30; i++) {
    queue.offer("item" + i);
  }
  for (int i=0; i < 10; i++) {
    queue.remove();
  }
  for (int i=0; i < 10; i++) {
    queue.take();
  }
  for (int i=0; i < 10; i++) {
    queue.poll();
  }
  LocalQueueStats stats=queue.getLocalQueueStats();
  assertEquals(30,stats.getPollOperationCount());
}
