{
  super(cachingProvider,uri,classLoader,properties);
  checkIfNotNull(hazelcastInstance,"hazelcastInstance cannot be null");
  this.hazelcastInstance=hazelcastInstance;
  final ClientCacheDistributedObject setupRef=hazelcastInstance.getDistributedObject(CacheService.SERVICE_NAME,"setupRef");
  this.clientContext=setupRef.getClientContext();
}
