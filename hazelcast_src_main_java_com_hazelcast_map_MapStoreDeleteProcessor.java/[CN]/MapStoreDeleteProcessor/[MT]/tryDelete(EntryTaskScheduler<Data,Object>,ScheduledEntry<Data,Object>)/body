{
  Exception exception=null;
  try {
    mapContainer.getStore().delete(mapService.toObject(entry.getKey()));
  }
 catch (  Exception e) {
    logger.warning(mapContainer.getStore().getMapStore().getClass() + " --> delete failed, " + "now Hazelcast reschedules this operation ",e);
    exception=e;
    scheduler.schedule(mapContainer.getWriteDelayMillis(),entry.getKey(),entry.getValue());
  }
  return exception;
}
