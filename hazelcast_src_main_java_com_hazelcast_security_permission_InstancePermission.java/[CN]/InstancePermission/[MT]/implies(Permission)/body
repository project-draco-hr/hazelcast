{
  if (this.getClass() != permission.getClass()) {
    return false;
  }
  InstancePermission that=(InstancePermission)permission;
  boolean maskTest=((this.mask & that.mask) == that.mask);
  if (!maskTest) {
    return false;
  }
  if (!SecurityUtil.nameMatches(that.getName(),this.getName())) {
    return false;
  }
  return true;
}
