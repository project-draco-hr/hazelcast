{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  InstancePermission other=(InstancePermission)obj;
  if (getName() == null && other.getName() != null) {
    return false;
  }
  if (!getName().equals(other.getName())) {
    return false;
  }
  if (mask != other.mask) {
    return false;
  }
  return true;
}
