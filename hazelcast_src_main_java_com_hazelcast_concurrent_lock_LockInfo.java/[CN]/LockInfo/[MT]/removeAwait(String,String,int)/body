{
  if (conditions != null) {
    final ConditionInfo condition=conditions.get(conditionId);
    if (condition != null) {
      final boolean ok=condition.removeWaiter(caller,threadId);
      if (condition.getAwaitCount() == 0) {
        conditions.remove(conditionId);
      }
      return ok;
    }
  }
  return false;
}
