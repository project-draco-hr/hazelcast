{
  try {
    out.writeBoolean(isRequest());
    address.writeData(out);
    out.writeUTF(groupName);
    out.writeUTF(groupPassword);
    out.writeByte(Packet.PACKET_VERSION);
    out.writeInt(buildNumber);
    out.writeInt(memberCount);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
