{
  boolean hasTo=in.readBoolean();
  if (hasTo) {
    to=new Address();
    to.readData(in);
  }
  address=new Address();
  address.readData(in);
  nodeType=NodeType.create(in.readInt());
  groupName=in.readUTF();
  groupPassword=in.readUTF();
  packetVersion=in.readByte();
  buildNumber=in.readInt();
}
