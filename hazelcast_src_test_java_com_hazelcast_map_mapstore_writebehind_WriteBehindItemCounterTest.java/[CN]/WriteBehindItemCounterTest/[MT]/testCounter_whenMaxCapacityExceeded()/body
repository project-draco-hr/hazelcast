{
  final int maxCapacityPerNode=100;
  final int nodeCount=1;
  final MapStoreWithCounter mapStore=new MapStoreWithCounter<Integer,String>();
  final TestMapUsingMapStoreBuilder builder=TestMapUsingMapStoreBuilder.create().withMapStore(mapStore).withNodeCount(nodeCount).withNodeFactory(createHazelcastInstanceFactory(1)).withBackupCount(0).withWriteCoalescing(false).withWriteBehindQueueCapacity(maxCapacityPerNode).withWriteDelaySeconds(100);
  final IMap<Object,Object> map=builder.build();
  populateMap(map,2 * maxCapacityPerNode);
}
