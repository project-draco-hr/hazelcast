{
  try {
    return tryLoadClass(factoryName).getClassLoader();
  }
 catch (  ClassNotFoundException cnfe) {
    logger.warning("Found ScriptEngineFactory candidate for " + factoryName + ", but cannot load class! -> "+ cnfe);
    if (logger.isFinestEnabled()) {
      logger.finest(cnfe);
    }
    return null;
  }
}
