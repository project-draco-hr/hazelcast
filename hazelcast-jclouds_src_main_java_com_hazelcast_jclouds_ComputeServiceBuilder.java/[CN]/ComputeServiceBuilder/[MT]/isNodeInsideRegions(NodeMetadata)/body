{
  Location location=metadata.getLocation();
  while (location != null) {
    String id=location.getId();
    if (location.getScope().equals(LocationScope.REGION)) {
      if (id != null && !regionsSet.isEmpty() && !regionsSet.contains(id)) {
        return false;
      }
    }
    location=location.getParent();
  }
  return true;
}
