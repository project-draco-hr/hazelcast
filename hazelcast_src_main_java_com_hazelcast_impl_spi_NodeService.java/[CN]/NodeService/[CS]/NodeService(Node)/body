{
  this.node=node;
  logger=node.getLogger(NodeService.class.getName());
  final ClassLoader classLoader=node.getConfig().getClassLoader();
  executorService=new ThreadPoolExecutor(3,Integer.MAX_VALUE,60L,TimeUnit.SECONDS,new SynchronousQueue(),new ExecutorThreadFactory(node.threadGroup,node.getThreadPoolNamePrefix("cached"),classLoader));
  scheduledExecutorService=new ScheduledThreadPoolExecutor(2,new ExecutorThreadFactory(node.threadGroup,node.getThreadPoolNamePrefix("scheduled"),classLoader));
  nonBlockingWorkers=new Workers("hz.NonBlocking",1);
  blockingWorkers=new Workers("hz.Blocking",8);
  partitionCount=node.groupProperties.CONCURRENT_MAP_PARTITION_COUNT.getInteger();
  maxBackupCount=MapConfig.MAX_BACKUP_COUNT;
}
