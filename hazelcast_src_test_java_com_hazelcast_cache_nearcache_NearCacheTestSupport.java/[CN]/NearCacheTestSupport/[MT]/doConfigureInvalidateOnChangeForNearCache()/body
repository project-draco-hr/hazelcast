{
  NearCacheConfig config1=createNearCacheConfig(DEFAULT_NEAR_CACHE_NAME + "-1",NearCacheConfig.DEFAULT_MEMORY_FORMAT);
  NearCacheConfig config2=createNearCacheConfig(DEFAULT_NEAR_CACHE_NAME + "-2",NearCacheConfig.DEFAULT_MEMORY_FORMAT);
  config1.setInvalidateOnChange(false);
  config2.setInvalidateOnChange(true);
  NearCache nearCache1=createNearCache(config1.getName(),config1,createManagedNearCacheRecordStore());
  NearCache nearCache2=createNearCache(config2.getName(),config2,createManagedNearCacheRecordStore());
  assertFalse(nearCache1.isInvalidateOnChange());
  assertTrue(nearCache2.isInvalidateOnChange());
}
