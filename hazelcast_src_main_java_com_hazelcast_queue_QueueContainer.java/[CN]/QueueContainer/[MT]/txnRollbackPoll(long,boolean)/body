{
  QueueItem item=txMap.remove(itemId);
  if (item == null) {
    System.err.println("something wrong!!!");
    return false;
  }
  if (!backup) {
    getItemQueue().offerFirst(item);
  }
  QueueItem replaced=itemMap.put(itemId,item);
  if (replaced != null) {
    System.err.println("something wrong!!!");
  }
  return true;
}
