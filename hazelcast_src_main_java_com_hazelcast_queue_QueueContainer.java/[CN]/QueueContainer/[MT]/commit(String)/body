{
  List<QueueItem> list=txOfferMap.remove(txId);
  if (list != null) {
    for (    QueueItem item : list) {
      item.setItemId(nextId());
      getItemQueue().offer(item);
    }
  }
  list=txPollMap.remove(txId);
  if (list != null) {
    long current=Clock.currentTimeMillis();
    for (    QueueItem item : list) {
      age(item,current);
    }
  }
}
