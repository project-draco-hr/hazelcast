{
  partitionId=in.readInt();
  int size=in.readInt();
  for (int j=0; j < size; j++) {
    QueueItem item=new QueueItem(this);
    item.readData(in);
    getItemQueue().offer(item);
    setId(item.getItemId());
  }
  int txSize=in.readInt();
  for (int j=0; j < txSize; j++) {
    QueueItem item=new QueueItem(this);
    item.readData(in);
    txMap.put(item.getItemId(),item);
    setId(item.getItemId());
  }
}
