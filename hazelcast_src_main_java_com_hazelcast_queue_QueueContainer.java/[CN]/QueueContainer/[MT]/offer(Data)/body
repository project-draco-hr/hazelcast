{
  QueueItem item=new QueueItem(this,idGen++);
  if (store.isEnabled()) {
    try {
      store.store(item.getItemId(),data);
    }
 catch (    Exception e) {
      idGen--;
      throw new RetryableHazelcastException(e);
    }
  }
  if (!store.isEnabled() || store.getMemoryLimit() > itemQueue.size()) {
    item.setData(data);
  }
  return itemQueue.offer(item);
}
