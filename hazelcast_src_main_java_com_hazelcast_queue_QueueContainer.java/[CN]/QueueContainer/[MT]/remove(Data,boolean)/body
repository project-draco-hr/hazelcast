{
  Iterator<QueueItem> iter=itemQueue.iterator();
  while (iter.hasNext()) {
    QueueItem item=iter.next();
    if (item.equals(data)) {
      if (!fromBackup && store.isEnabled()) {
        store.delete(item.getItemId());
      }
      iter.remove();
      return true;
    }
  }
  return false;
}
