{
  List<QueueItem> list=txPollMap.remove(txId);
  if (list != null) {
    ListIterator<QueueItem> iter=list.listIterator(list.size());
    while (iter.hasPrevious()) {
      QueueItem item=iter.previous();
      if (item.getItemId() != -1) {
        getItemQueue().offerFirst(item);
      }
    }
  }
  txOfferMap.remove(txId);
}
