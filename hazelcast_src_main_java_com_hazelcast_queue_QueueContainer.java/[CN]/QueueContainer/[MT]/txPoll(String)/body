{
  QueueItem item=getItemQueue().poll();
  if (item == null) {
    List<QueueItem> list=getTxList(txId,txOfferMap);
    if (list.size() > 0) {
      item=list.remove(0);
    }
  }
  if (item != null) {
    List<QueueItem> list=getTxList(txId,txPollMap);
    list.add(item);
    return item;
  }
  return null;
}
