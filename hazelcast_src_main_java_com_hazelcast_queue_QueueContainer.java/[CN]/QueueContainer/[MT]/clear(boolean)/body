{
  if (!fromBackup && store.isEnabled()) {
    Set<Long> keySet=new HashSet<Long>(itemQueue.size());
    Iterator<QueueItem> iter=itemQueue.iterator();
    while (iter.hasNext()) {
      QueueItem item=iter.next();
      keySet.add(item.getItemId());
    }
    store.deleteAll(keySet);
  }
  itemQueue.clear();
}
