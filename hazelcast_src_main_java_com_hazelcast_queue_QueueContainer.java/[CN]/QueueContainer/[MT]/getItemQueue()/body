{
  if (itemQueue == null) {
    itemQueue=new LinkedList<QueueItem>();
    if (backupMap != null && !backupMap.isEmpty()) {
      List<QueueItem> values=new ArrayList<QueueItem>(backupMap.values());
      Collections.sort(values);
      itemQueue.addAll(values);
      backupMap.clear();
      backupMap=null;
    }
  }
  return itemQueue;
}
