{
  if (conditions == null) {
    return false;
  }
  ConditionInfo condition=conditions.get(conditionId);
  if (condition == null) {
    return false;
  }
  boolean ok=condition.removeWaiter(caller,threadId);
  if (condition.getAwaitCount() == 0) {
    conditions.remove(conditionId);
  }
  return ok;
}
