{
  if (leaseTime < 0) {
    expirationTime=Long.MAX_VALUE;
    lockStore.cancelEviction(key);
  }
 else {
    expirationTime=Clock.currentTimeMillis() + leaseTime;
    if (expirationTime < 0) {
      expirationTime=Long.MAX_VALUE;
      lockStore.cancelEviction(key);
    }
 else {
      lockStore.scheduleEviction(key,leaseTime);
    }
  }
}
