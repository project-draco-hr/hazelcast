{
  if (!isClosed.compareAndSet(false,true)) {
    return;
  }
  for (  Future f : loadAllTasks) {
    try {
      f.get(TIMEOUT,TimeUnit.SECONDS);
    }
 catch (    Exception e) {
      throw new CacheException(e);
    }
  }
  loadAllTasks.clear();
  delegate.destroy();
  closeCacheLoader();
  deregisterCompletionListener();
}
