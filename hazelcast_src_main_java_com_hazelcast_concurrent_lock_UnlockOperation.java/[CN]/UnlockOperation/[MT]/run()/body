{
  final LockStoreImpl lockStore=getLockStore();
  if (force) {
    response=lockStore.forceUnlock(key);
  }
 else {
    final boolean unlocked=lockStore.unlock(key,getCallerUuid(),threadId);
    response=unlocked;
    if (!unlocked) {
      throw new IllegalMonitorStateException("Current thread is not owner of the lock! " + "-> Owner: " + lockStore.getOwnerInfo(key));
    }
  }
}
