{
  if (target == null) {
    return new BasicPartitionInvocation(nodeEngine,serviceName,op,partitionId,replicaIndex,tryCount,tryPauseMillis,callTimeout,callback).invoke();
  }
 else {
    return new BasicTargetInvocation(nodeEngine,serviceName,op,target,tryCount,tryPauseMillis,callTimeout,callback).invoke();
  }
}
