{
  final String id=UUID.randomUUID().toString();
  final StringBuilder sb=new StringBuilder("HZ");
  final char[] chars=id.toCharArray();
  for (  final char c : chars) {
    if (c != '-') {
      if (Character.isLetter(c)) {
        sb.append(Character.toUpperCase(c));
      }
 else {
        sb.append(c);
      }
    }
  }
  return sb.toString();
}
