{
  String id=UUID.randomUUID().toString();
  StringBuilder sb=new StringBuilder("HZ");
  char[] chars=id.toCharArray();
  for (  final char c : chars) {
    if (c != '-') {
      if (Character.isLetter(c)) {
        sb.append(Character.toUpperCase(c));
      }
 else {
        sb.append(c);
      }
    }
  }
  return sb.toString();
}
