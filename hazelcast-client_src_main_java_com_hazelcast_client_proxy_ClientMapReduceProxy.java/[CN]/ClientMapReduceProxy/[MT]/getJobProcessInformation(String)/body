{
  try {
    ClientContext context=getContext();
    ClientInvocationService cis=context.getInvocationService();
    Address runningMember=runningJobs.get(jobId);
    ICompletableFuture<JobProcessInformation> future=cis.invokeOnTarget(new ClientJobProcessInformationRequest(getName(),jobId),runningMember);
    return future.get();
  }
 catch (  Exception ignore) {
  }
  return null;
}
