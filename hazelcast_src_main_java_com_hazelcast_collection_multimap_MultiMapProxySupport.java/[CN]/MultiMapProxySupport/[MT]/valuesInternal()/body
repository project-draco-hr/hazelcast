{
  final NodeEngine nodeEngine=getNodeEngine();
  try {
    Map<Integer,Object> results=nodeEngine.getOperationService().invokeOnAllPartitions(CollectionService.SERVICE_NAME,new MultiMapOperationFactory(name,OperationFactoryType.VALUES));
    return results;
  }
 catch (  Throwable throwable) {
    throw ExceptionUtil.rethrow(throwable);
  }
}
