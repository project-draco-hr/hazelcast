{
  final NodeEngine nodeEngine=getNodeEngine();
  try {
    EntrySetOperation operation=new EntrySetOperation(proxyId);
    Map<Integer,Object> results=nodeEngine.getOperationService().invokeOnAllPartitions(CollectionService.SERVICE_NAME,new BinaryOperationFactory(operation,nodeEngine));
    return results;
  }
 catch (  Throwable throwable) {
    throw ExceptionUtil.rethrow(throwable);
  }
}
