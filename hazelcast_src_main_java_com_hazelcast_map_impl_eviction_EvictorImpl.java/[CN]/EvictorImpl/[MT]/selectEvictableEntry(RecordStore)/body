{
  Iterable<EntryView> samples=getSamples(recordStore);
  EntryView selected=null;
  for (  EntryView candidate : samples) {
    if (selected == null) {
      selected=candidate;
    }
 else     if (mapEvictionPolicy.compare(candidate,selected) < 0) {
      selected=candidate;
    }
  }
  return selected;
}
