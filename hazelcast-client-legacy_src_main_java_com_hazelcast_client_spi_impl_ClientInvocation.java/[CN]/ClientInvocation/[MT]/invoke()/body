{
  if (request == null) {
    throw new IllegalStateException("Request can not be null");
  }
  try {
    invokeOnSelection();
  }
 catch (  Exception e) {
    if (e instanceof HazelcastOverloadException) {
      throw (HazelcastOverloadException)e;
    }
    notify(e);
  }
  return clientInvocationFuture;
}
