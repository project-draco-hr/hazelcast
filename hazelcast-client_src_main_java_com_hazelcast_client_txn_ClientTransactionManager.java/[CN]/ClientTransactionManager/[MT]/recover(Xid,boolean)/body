{
  final SerializableXID sXid=new SerializableXID(xid.getFormatId(),xid.getGlobalTransactionId(),xid.getBranchQualifier());
  final ClientConnection connection=recoveredTransactions.remove(sXid);
  if (connection == null) {
    return false;
  }
  final ClientInvocationService invocationService=client.getInvocationService();
  final RecoverTransactionRequest request=new RecoverTransactionRequest(sXid,commit);
  try {
    final ICompletableFuture future=invocationService.invokeOnConnection(request,connection);
    future.get();
  }
 catch (  Exception e) {
    ExceptionUtil.rethrow(e);
  }
  return true;
}
