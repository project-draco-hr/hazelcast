{
  final MapStoreWithCounter mapStore=new MapStoreWithCounter<Integer,String>();
  final IMap<Object,Object> map=TestMapUsingMapStoreBuilder.create().withMapStore(mapStore).withNodeCount(1).withNodeFactory(createHazelcastInstanceFactory(1)).withBackupCount(0).withWriteDelaySeconds(100).withPartitionCount(1).build();
  final int numberOfItems=1000;
  populateMap(map,numberOfItems);
  evictMap(map,numberOfItems);
  assertFinalValueEqualsForEachEntry(map,numberOfItems);
}
