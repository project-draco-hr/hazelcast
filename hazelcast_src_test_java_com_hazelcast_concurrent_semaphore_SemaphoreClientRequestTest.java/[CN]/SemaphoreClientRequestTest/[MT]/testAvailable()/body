{
  getClient().send(new AvailableRequest(name));
  int result=(Integer)getClient().receive();
  assertEquals(0,result);
  ISemaphore s=getInstance().getSemaphore(name);
  s.release(5);
  getClient().send(new AvailableRequest(name));
  result=(Integer)getClient().receive();
  assertEquals(5,result);
}
