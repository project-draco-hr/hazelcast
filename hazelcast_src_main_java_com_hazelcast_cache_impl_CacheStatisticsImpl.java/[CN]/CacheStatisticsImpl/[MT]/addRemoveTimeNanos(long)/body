{
  for (; ; ) {
    long nanos=removeTimeTakenNanos;
    if (nanos <= Long.MAX_VALUE - duration) {
      if (REMOVALS_UPDATER.compareAndSet(this,nanos,nanos + duration)) {
        return;
      }
    }
 else {
      if (REMOVALS_UPDATER.compareAndSet(this,nanos,duration)) {
        clear();
        return;
      }
    }
  }
}
