{
  if (valueData != null && valueData.isPortable()) {
    FieldDefinition fd=valueData.cd.get(attributeName);
    if (fd == null)     throw new QueryException("Unknown Attribute: " + attributeName);
switch (fd.getType()) {
case FieldDefinition.TYPE_LONG:
      return TypeConverters.LONG_CONVERTER;
case FieldDefinition.TYPE_DOUBLE:
    return TypeConverters.DOUBLE_CONVERTER;
case FieldDefinition.TYPE_BOOLEAN:
  return TypeConverters.BOOLEAN_CONVERTER;
case FieldDefinition.TYPE_INT:
return TypeConverters.INTEGER_CONVERTER;
case FieldDefinition.TYPE_FLOAT:
return TypeConverters.FLOAT_CONVERTER;
case FieldDefinition.TYPE_UTF:
return TypeConverters.STRING_CONVERTER;
case FieldDefinition.TYPE_SHORT:
return TypeConverters.SHORT_CONVERTER;
default :
throw new QueryException("No type converter available for type: " + fd.getType());
}
}
return getAttributeTypeConverterViaReflection(attributeName);
}
