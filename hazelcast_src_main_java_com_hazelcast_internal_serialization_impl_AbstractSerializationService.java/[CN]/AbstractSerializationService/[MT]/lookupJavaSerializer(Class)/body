{
  if (Serializable.class.isAssignableFrom(type)) {
    if (!Throwable.class.isAssignableFrom(type)) {
      logger.warning("WARNING!!! Serialization service will use Java Serialization for : " + type.getName() + " . Please consider using a faster serialization option!!!");
    }
    safeRegister(type,javaSerializerAdapter);
    return javaSerializerAdapter;
  }
  return null;
}
