{
  IMap<String,String> map=getInstance().getMap("testGetPutAndSizeWhileStartShutdown");
  try {
    for (int i=1; i < 10000; i++) {
      map.put("key" + i,"value" + i);
      if (i == 100) {
        new Thread(new Runnable(){
          public void run(){
            newInstanceMany(2);
          }
        }
).start();
      }
      if (i == 600) {
        new Thread(new Runnable(){
          public void run(){
            removeInstance();
          }
        }
).start();
      }
      Thread.sleep(5);
    }
    Thread.sleep(3000);
    assertEquals(map.size(),10000);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
