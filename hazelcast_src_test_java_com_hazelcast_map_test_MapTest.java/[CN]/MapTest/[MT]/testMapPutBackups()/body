{
  IMap map=getInstance(0).getMap("testMapBackups");
  int size=1000;
  for (int i=0; i < size; i++) {
    map.put(i,i);
  }
  getInstance(1).getLifecycleService().shutdown();
  Thread.sleep(1000);
  assertEquals(map.size(),size);
  for (int i=0; i < size; i++) {
    assertEquals(i,map.get(i));
  }
}
