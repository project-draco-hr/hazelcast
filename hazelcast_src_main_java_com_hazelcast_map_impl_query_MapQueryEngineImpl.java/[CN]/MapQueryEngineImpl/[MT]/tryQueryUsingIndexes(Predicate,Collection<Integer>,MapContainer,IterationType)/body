{
  if (partitionService.hasOnGoingMigrationLocal()) {
    return null;
  }
  Set<QueryableEntry> entries=mapContainer.getIndexes().query(predicate);
  if (entries == null) {
    return null;
  }
  QueryResult result=newQueryResult(partitions.size(),iterationType);
  result.addAll(entries);
  return result;
}
