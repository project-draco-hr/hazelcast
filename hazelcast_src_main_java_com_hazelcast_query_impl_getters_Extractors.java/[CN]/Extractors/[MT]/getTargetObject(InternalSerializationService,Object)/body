{
  Data targetData;
  if (target instanceof Portable) {
    targetData=serializationService.toData(target);
    if (targetData.isPortable()) {
      return targetData;
    }
  }
  if (target instanceof Data) {
    targetData=(Data)target;
    if (targetData.isPortable()) {
      return targetData;
    }
  }
  if (target instanceof Data) {
    return serializationService.toObject(target);
  }
  return target;
}
