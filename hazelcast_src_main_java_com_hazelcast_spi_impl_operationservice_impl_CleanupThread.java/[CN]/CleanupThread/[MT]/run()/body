{
  try {
    while (!shutdown) {
      scanHandleOperationTimeout();
      if (!shutdown) {
        operationService.backPressureService.cleanup();
      }
      if (!shutdown) {
        sleep();
      }
    }
  }
 catch (  Throwable t) {
    inspectOutputMemoryError(t);
    operationService.logger.severe("Failed to run",t);
  }
}
