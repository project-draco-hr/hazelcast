{
  for (; ; ) {
    if (outputBuffer != null && !outputBuffer.hasRemaining()) {
      return;
    }
    if (currentFrame == null) {
      currentFrame=poll();
      if (currentFrame == null) {
        return;
      }
    }
    if (!writeHandler.onWrite(currentFrame,outputBuffer)) {
      return;
    }
    currentFrame=null;
  }
}
