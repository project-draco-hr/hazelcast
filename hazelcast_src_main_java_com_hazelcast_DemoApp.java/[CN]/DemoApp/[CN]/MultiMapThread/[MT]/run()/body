{
  int count=10000;
  for (int k=0; k < count; k++) {
    map.put(k,k);
  }
  Random random=new Random();
  for (; ; ) {
    try {
      Thread.sleep(100);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    int key=random.nextInt(count);
    map.put(key,random.nextInt(10));
    map.get(key);
  }
}
