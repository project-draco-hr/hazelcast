{
  int count=100000;
  for (int k=0; k < count; k++) {
    queue.offer(k);
  }
  Random random=new Random();
  for (; ; ) {
    try {
      Thread.sleep(100);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    queue.offer(random.nextInt());
    try {
      queue.take();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
}
