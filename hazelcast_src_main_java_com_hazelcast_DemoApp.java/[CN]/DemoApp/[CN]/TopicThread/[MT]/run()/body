{
  Random random=new Random();
  topic.addMessageListener(new MessageListener(){
    @Override public void onMessage(    Message message){
    }
  }
);
  for (; ; ) {
    try {
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    topic.publish(random.nextInt());
  }
}
