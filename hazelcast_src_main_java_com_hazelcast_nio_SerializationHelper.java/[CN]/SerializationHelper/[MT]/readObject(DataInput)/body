{
  try {
    int len=in.readInt();
    byte[] buf=new byte[len];
    in.readFully(buf);
    ObjectInputStream oin=new ObjectInputStream(new ByteArrayInputStream(buf));
    Object result=oin.readObject();
    oin.close();
    return result;
  }
 catch (  ClassNotFoundException e) {
    e.printStackTrace();
  }
  return null;
}
