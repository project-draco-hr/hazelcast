{
  if (value != null) {
    if (getMultiValues() == null) {
      setMultiValues(new ConcurrentHashSet<ValueHolder>(){
        @Override public boolean add(        ValueHolder e){
          return e != null && super.add(e);
        }
      }
);
    }
    getMultiValues().add(new ValueHolder(value));
  }
}
