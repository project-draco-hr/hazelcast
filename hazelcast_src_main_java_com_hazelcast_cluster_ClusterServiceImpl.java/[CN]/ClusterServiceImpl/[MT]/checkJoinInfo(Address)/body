{
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(SERVICE_NAME,new JoinCheckOperation(node.createJoinRequest()),target).setTryCount(1).build();
  try {
    return (JoinRequest)nodeEngine.toObject(inv.invoke().get());
  }
 catch (  Exception e) {
    logger.warning("Error during join check!",e);
  }
  return null;
}
