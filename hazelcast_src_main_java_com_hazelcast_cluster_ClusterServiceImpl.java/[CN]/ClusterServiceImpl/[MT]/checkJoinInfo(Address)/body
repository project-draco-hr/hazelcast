{
  Future f=nodeEngine.getOperationService().createInvocationBuilder(SERVICE_NAME,new JoinCheckOperation(node.createJoinRequest()),target).setTryCount(1).invoke();
  try {
    return (JoinRequest)nodeEngine.toObject(f.get());
  }
 catch (  Exception e) {
    logger.warning("Error during join check!",e);
  }
  return null;
}
