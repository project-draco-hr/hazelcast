{
  Context context=Context.getOrCreate();
  if (context.getConnection() == null) {
    try {
      Connection connection=cp.takeConnection(key);
      context.setConnection(connection);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
catch (    UnknownHostException e) {
      e.printStackTrace();
    }
  }
  return context;
}
