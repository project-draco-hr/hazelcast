{
  if (!backPressureEnabled) {
    return false;
  }
  if (op.isUrgent()) {
    return false;
  }
  AtomicInteger syncDelay=getSyncDelay(op);
  int currentSyncDelay=syncDelay.get();
  if (currentSyncDelay > 0) {
    syncDelay.decrementAndGet();
    return false;
  }
  syncDelay.set(calcSyncDelay());
  return true;
}
