{
  String value=getString(groupProperty);
  for (  Enum e : enumClazz.getEnumConstants()) {
    if (e.name().equalsIgnoreCase(value)) {
      return (E)e;
    }
  }
  throw new IllegalArgumentException(format("value '%s' for property '%s' is not a valid %s value",value,groupProperty.getName(),enumClazz.getName()));
}
