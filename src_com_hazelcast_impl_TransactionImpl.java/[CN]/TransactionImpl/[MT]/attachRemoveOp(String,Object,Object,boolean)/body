{
  TxnRecord rec=findTxnRecord(name,key);
  Object oldValue=null;
  if (rec == null) {
    rec=new TxnRecord(name,key,value,newRecord);
    rec.removed=true;
    lsTxnRecords.add(rec);
    return null;
  }
 else {
    oldValue=rec.value;
    rec.value=value;
  }
  rec.removed=true;
  return oldValue;
}
