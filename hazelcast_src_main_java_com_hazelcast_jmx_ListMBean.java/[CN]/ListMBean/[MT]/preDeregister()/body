{
  if (listener != null) {
    getManagedObject().removeItemListener(listener);
    listener=null;
  }
  if (receivedStats != null) {
    receivedStats.destroy();
    receivedStats=null;
  }
  if (servedStats != null) {
    servedStats.destroy();
    servedStats=null;
  }
  super.preDeregister();
}
