{
  String name=protocol.args[0];
  Data dKey=protocol.buffers[0];
  Data dEntryProcessor=protocol.buffers[1];
  EntryProcessor ep=(EntryProcessor)node.serializationService.toObject(dEntryProcessor);
  DataMapProxy dataMapProxy=getMapProxy(name);
  dataMapProxy.executeOnKey(dKey,ep);
  return protocol.success();
}
