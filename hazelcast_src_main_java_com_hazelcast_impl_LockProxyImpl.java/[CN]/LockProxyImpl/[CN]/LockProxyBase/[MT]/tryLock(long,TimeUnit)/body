{
  try {
    if (factory.locksMapProxy.tryLock(key,time,unit)) {
      lockOperationsCounter.incrementLocks();
      return true;
    }
  }
 catch (  RuntimeInterruptedException e) {
    lockOperationsCounter.incrementFailedLocks();
    throw new InterruptedException();
  }
  lockOperationsCounter.incrementFailedLocks();
  return false;
}
