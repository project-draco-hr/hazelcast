{
  IMap<Integer,Integer> map=Hazelcast.getMap("def");
  Hazelcast.getTransaction().begin();
  Map<Integer,Integer> localMap=new HashMap<Integer,Integer>();
  for (int i=0; i < 10; i++) {
    localMap.put(i,i);
  }
  map.putAll(localMap);
  Hazelcast.getTransaction().rollback();
  assertEquals(0,map.size());
}
