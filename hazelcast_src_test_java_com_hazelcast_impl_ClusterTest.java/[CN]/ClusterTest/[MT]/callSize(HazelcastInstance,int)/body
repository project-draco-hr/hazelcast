{
  Random r=new Random();
  Map<Integer,Integer> map=h.getMap("testMap");
  try {
    sleep(5000);
  }
 catch (  InterruptedException ignored) {
  }
  int size=0;
  for (int i=0; i < numberOfIterations; i++) {
    if (i % 1000 == 0) {
      int sizeNow=map.size();
      if (sizeNow < size)       throw new RuntimeException("CurrentSize cannot be smaller. " + sizeNow + ", was "+ size);
      size=sizeNow;
    }
    map.put(r.nextInt(200000),i);
  }
  h.shutdown();
}
