{
  if (collection == null || collection.isEmpty()) {
    return;
  }
  final int currentSize=size();
  final int desiredSize=currentSize + collection.size();
  if (hasReachedMaxSize(desiredSize)) {
    throw new ReachedMaxSizeException("Remaining empty slots are not appropriate for this collection." + " Remaining = [" + (maxSize - currentSize) + "]");
  }
  super.addFront(collection);
}
