{
  HazelcastInstance instance=createHazelcastInstance();
  final ICountDownLatch latch=instance.getCountDownLatch(randomString());
  latch.trySetCount(1);
  TestThread thread=new TestThread(){
    public void doRun(){
      latch.countDown();
    }
  }
;
  thread.start();
  assertOpenEventually(latch);
}
