{
  final HazelcastInstance instance=createHazelcastInstance();
  final ICountDownLatch latch=instance.getCountDownLatch(randomString());
  latch.trySetCount(1);
  Thread thread=(new Thread(){
    public void run(){
      latch.countDown();
    }
  }
);
  thread.start();
  assertOpenEventually(latch);
}
