{
  final StringBuffer sb=new StringBuffer();
  sb.append("Employee");
  sb.append("{name='").append(name).append('\'');
  sb.append(", city=").append(city);
  sb.append(", age=").append(age);
  sb.append(", active=").append(active);
  sb.append(", salary=").append(salary);
  sb.append('}');
  return sb.toString();
}
