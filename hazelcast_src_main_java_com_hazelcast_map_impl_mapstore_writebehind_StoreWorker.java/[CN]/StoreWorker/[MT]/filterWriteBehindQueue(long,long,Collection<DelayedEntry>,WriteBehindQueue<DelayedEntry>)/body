{
  if (sequence > 0) {
    queue.filter(new IPredicate<DelayedEntry>(){
      @Override public boolean test(      DelayedEntry delayedEntry){
        return delayedEntry.getSequence() <= sequence;
      }
    }
,collection);
  }
 else {
    queue.filter(new IPredicate<DelayedEntry>(){
      @Override public boolean test(      DelayedEntry delayedEntry){
        return delayedEntry.getStoreTime() <= now;
      }
    }
,collection);
  }
}
