{
  if (booleanCall(threadMxBean,ThreadMXBean_isObjectMonitorUsageSupported) && booleanCall(threadMxBean,ThreadMXBean_isSynchronizerUsageSupported)) {
    return parameterizedObjectCall(threadMxBean,ThreadMXBean_dumpAllThreads,new Class[]{boolean.class,boolean.class},new Object[]{true,true});
  }
 else {
    return super.getAllThreads();
  }
}
