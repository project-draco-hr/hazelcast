{
  try {
    Runnable r;
    do {
      r=taskQ.poll(1,TimeUnit.MILLISECONDS);
      if (r != null) {
        r.run();
        EXECUTED_COUNT_UPDATER.incrementAndGet(CachedExecutorServiceDelegate.this);
      }
    }
 while (r != null);
  }
 catch (  InterruptedException ignored) {
  }
 finally {
    exit();
  }
}
