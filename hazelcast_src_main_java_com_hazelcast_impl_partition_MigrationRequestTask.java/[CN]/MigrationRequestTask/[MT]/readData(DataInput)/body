{
  partitionId=in.readInt();
  replicaIndex=in.readInt();
  migration=in.readBoolean();
  diffOnly=in.readBoolean();
  selfCopyReplicaIndex=in.readInt();
  boolean hasFrom=in.readBoolean();
  if (hasFrom) {
    from=new Address();
    from.readData(in);
  }
  to=new Address();
  to.readData(in);
}
