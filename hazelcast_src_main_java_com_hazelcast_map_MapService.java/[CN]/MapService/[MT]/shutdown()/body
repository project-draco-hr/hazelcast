{
  final PartitionContainer[] containers=partitionContainers;
  for (int i=0; i < containers.length; i++) {
    PartitionContainer container=containers[i];
    if (container != null) {
      container.destroy();
    }
    containers[i]=null;
  }
  for (  NearCache nearCache : nearCacheMap.values()) {
    nearCache.destroy();
  }
  nearCacheMap.clear();
  mapContainers.clear();
  eventRegistrations.clear();
}
