{
  flushMaps();
  destroyMapStores();
  final PartitionContainer[] containers=partitionContainers;
  for (  PartitionContainer container : containers) {
    if (container != null) {
      container.clear();
    }
  }
  for (  NearCache nearCache : nearCacheMap.values()) {
    nearCache.clear();
  }
  nearCacheMap.clear();
  mapContainers.clear();
}
