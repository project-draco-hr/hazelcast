{
  if (!terminate) {
    flushMapsBeforeShutdown();
    destroyMapStores();
    final PartitionContainer[] containers=partitionContainers;
    for (    PartitionContainer container : containers) {
      if (container != null) {
        container.clear();
      }
    }
    nearCacheProvider.clear();
    mapContainers.clear();
  }
}
