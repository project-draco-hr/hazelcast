{
  ensureOpen();
  final NodeEngine engine=getNodeEngine();
  if (key == null) {
    throw new NullPointerException(NULL_KEY_IS_NOT_ALLOWED);
  }
  final Operation op=new CacheRemoveOperation(nameWithPrefix,key,null);
  final int partitionId=getPartitionId(engine,key);
  final InternalCompletableFuture<Boolean> f=engine.getOperationService().invokeOnPartition(getServiceName(),op,partitionId);
  return f.getSafely();
}
