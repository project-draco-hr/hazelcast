{
  ensureOpen();
  if (key == null) {
    throw new NullPointerException(NULL_KEY_IS_NOT_ALLOWED);
  }
  final Operation op=new CacheRemoveOperation(getDistributedObjectName(),key,null,-1);
  final int partitionId=getPartitionId(nodeEngine,key);
  final InternalCompletableFuture<Boolean> f=nodeEngine.getOperationService().invokeOnPartition(getServiceName(),op,partitionId);
  return f.getSafely();
}
