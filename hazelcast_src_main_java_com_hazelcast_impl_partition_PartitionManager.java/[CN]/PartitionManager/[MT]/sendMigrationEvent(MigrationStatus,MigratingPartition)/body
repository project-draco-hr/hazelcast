{
  final Collection<MemberImpl> members=node.clusterImpl.getMemberList();
  for (  MemberImpl member : members) {
    if (!member.localMember()) {
      nodeService.createSingleInvocation(PARTITION_SERVICE_NAME,new MigrationNotification(status,migratingPartition),-1).setTarget(member.getAddress()).setTryCount(1).build().invoke();
    }
  }
  fireMigrationEvent(status,migratingPartition);
}
