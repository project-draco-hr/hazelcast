{
  isNotNull(callback,"callback");
  isNotNull(executor,"executor");
  if (isDoneInternal()) {
    runAsynchronous(callback,executor);
    return;
  }
synchronized (completionLock) {
    if (isDoneInternal()) {
      runAsynchronous(callback,executor);
      return;
    }
    this.callbackHead=new ExecutionCallbackNode<V>(callback,executor,callbackHead);
  }
}
