{
  DataInputStream in=null;
  try {
    ByteArrayInputStream bais=new ByteArrayInputStream(bytesRecordSet);
    in=new DataInputStream(new InflaterInputStream(bais));
    int size=in.readInt();
    tasks=new ArrayList<Operation>(size);
    for (int i=0; i < size; i++) {
      Operation task=IOUtil.readObject(in);
      tasks.add(task);
    }
    if (taskCount != tasks.size()) {
      getLogger().log(Level.SEVERE,"Migration task count mismatch! => " + "expected-count: " + size + ", actual-count: "+ tasks.size()+ "\nfrom: "+ from+ ", partition: "+ getPartitionId()+ ", replica: "+ getReplicaIndex());
    }
    success=runMigrationTasks();
  }
 catch (  Throwable e) {
    Level level=Level.WARNING;
    if (e instanceof IllegalStateException) {
      level=Level.FINEST;
    }
    getLogger().log(level,e.getMessage(),e);
    success=false;
  }
 finally {
    IOUtil.closeResource(in);
  }
}
