{
  callerUuid=in.readUTF();
  txnId=in.readUTF();
  partitionId=in.readInt();
  state=Transaction.State.valueOf(in.readUTF());
  int len=in.readInt();
  if (len > 0) {
    for (int i=0; i < len; i++) {
      TransactionalOperation operation=in.readObject();
      operation.setState(state);
      addOperationRecord(operation);
    }
  }
}
