{
  final Address senderAddress=conn != null ? conn.getEndPoint() : null;
  final boolean accept=conn == null || (senderAddress != null && senderAddress.equals(node.getMasterAddress()));
  if (accept) {
    node.getClusterImpl().setMasterTime(masterTime);
    node.clusterManager.updateMembers(getMemberInfos());
    return Boolean.TRUE;
  }
 else {
    node.getLogger(MembersUpdateCall.class.getName()).log(Level.WARNING,"Received MembersUpdateCall from " + senderAddress + ", but current master is "+ node.getMasterAddress());
  }
  return Boolean.FALSE;
}
