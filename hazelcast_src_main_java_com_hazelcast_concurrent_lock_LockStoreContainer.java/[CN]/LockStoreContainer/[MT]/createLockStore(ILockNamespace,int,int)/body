{
  final LockStoreImpl ls=new LockStoreImpl(namespace,backupCount,asyncBackupCount,lockService);
  final LockStoreImpl current;
  if ((current=lockStores.putIfAbsent(namespace,ls)) != null) {
    if (current.getBackupCount() != ls.getBackupCount() || current.getAsyncBackupCount() != ls.getAsyncBackupCount()) {
      throw new IllegalStateException("LockStore for namespace[" + namespace + "] is already created! "+ current+ " - VS - "+ ls);
    }
    return current;
  }
  return ls;
}
