{
  final Config config=new Config();
  final ServiceConfig serviceConfig=new ServiceConfig().setEnabled(true).setName(MigrationEventCollectingService.SERVICE_NAME).setClassName(MigrationEventCollectingService.class.getName());
  config.getServicesConfig().addServiceConfig(serviceConfig);
  config.setProperty(GroupProperty.PARTITION_MAX_PARALLEL_REPLICATIONS,"0");
  config.setProperty(GroupProperty.PARTITION_COUNT,String.valueOf(PARTITION_COUNT));
  return config;
}
