{
  if (locallyOwnedMap != null) {
    locallyOwnedMap.reset();
  }
  if (mapNearCache != null) {
    mapNearCache.reset();
  }
  mapRecords.clear();
  mapIndexService.clear();
  if (store != null && store instanceof MapStoreWrapper) {
    try {
      ((MapStoreWrapper)store).destroy();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
