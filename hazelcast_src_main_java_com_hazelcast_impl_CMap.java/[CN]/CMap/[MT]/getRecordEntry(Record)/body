{
  RecordEntry recordEntry=mapRecordEntries.get(record.getId());
  if (recordEntry == null) {
    recordEntry=new RecordEntry(record);
    RecordEntry found=mapRecordEntries.putIfAbsent(record.getId(),recordEntry);
    if (found != null) {
      recordEntry=found;
    }
  }
  return recordEntry;
}
