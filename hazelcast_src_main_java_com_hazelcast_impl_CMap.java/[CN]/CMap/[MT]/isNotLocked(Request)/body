{
  boolean result=(lockEntireMap == null || !lockEntireMap.isLocked() || lockEntireMap.isLockedBy(request.lockAddress,request.lockThreadId));
  if (!result) {
    System.out.println(thisAddress + " and caller is " + request.caller);
    System.out.println(lockEntireMap + " LOCKED !! " + request.operation);
  }
  return result;
}
