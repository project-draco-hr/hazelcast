{
  sbState.append("\nCMap [");
  sbState.append(name);
  sbState.append("] r:");
  sbState.append(mapRecords.size());
  if (locallyOwnedMap != null) {
    locallyOwnedMap.appendState(sbState);
  }
  if (mapNearCache != null) {
    mapNearCache.appendState(sbState);
  }
  mapIndexService.appendState(sbState);
}
