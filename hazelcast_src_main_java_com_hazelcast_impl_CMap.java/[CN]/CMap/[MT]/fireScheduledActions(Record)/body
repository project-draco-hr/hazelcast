{
  concurrentMapManager.checkServiceThread();
  if (record.getLockCount() == 0) {
    record.clearLock();
    while (record.hasScheduledAction()) {
      ScheduledAction sa=record.getScheduledActions().remove(0);
      node.clusterManager.deregisterScheduledAction(sa);
      if (!sa.expired()) {
        sa.consume();
        if (record.isLocked()) {
          return;
        }
      }
 else {
        sa.onExpire();
      }
    }
  }
}
