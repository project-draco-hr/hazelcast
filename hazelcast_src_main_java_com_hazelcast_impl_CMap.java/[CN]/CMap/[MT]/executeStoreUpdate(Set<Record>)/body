{
  if (dirtyRecords.size() > 0) {
    concurrentMapManager.executeLocally(new Runnable(){
      public void run(){
        try {
          runStoreUpdate(dirtyRecords);
        }
 catch (        Throwable e) {
          for (          Record dirtyRecord : dirtyRecords) {
            dirtyRecord.setDirty(true);
          }
        }
      }
    }
);
  }
}
