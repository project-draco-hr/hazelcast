{
  DefaultSerializationServiceBuilder defaultSerializationServiceBuilder=new DefaultSerializationServiceBuilder();
  serializationService=defaultSerializationServiceBuilder.setVersion(InternalSerializationService.VERSION_1).addPortableFactory(TestSerializationConstants.PORTABLE_FACTORY_ID,new PortableTest.TestPortableFactory()).setAllowUnsafe(false).setUseNativeByteOrder(false).build();
}
