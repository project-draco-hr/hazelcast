{
  dead=false;
  rollback:   while (writeHandlerQueue.size() > 0) {
    Invocation inv=(Invocation)writeHandlerQueue.poll();
    if (inv == null)     break rollback;
    if (inv.local) {
      ClusterService.get().rollbackInvocation(inv);
    }
  }
  writeHandlerQueue.clear();
  lsWrittenLocals.clear();
}
