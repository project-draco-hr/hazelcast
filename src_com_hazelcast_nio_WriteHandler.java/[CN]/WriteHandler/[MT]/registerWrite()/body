{
  try {
    if (!connection.live())     return;
    if (sk == null) {
      sk=socketChannel.register(outSelector.selector,SelectionKey.OP_WRITE,this);
    }
 else {
      sk.interestOps(SelectionKey.OP_WRITE);
    }
    alreadyRegistered.set(true);
  }
 catch (  Exception e) {
    handleSocketException(e);
  }
}
