{
  if (result == null) {
synchronized (this) {
      if (result == null) {
        Packet packet;
        if (l < 0) {
          packet=queue.take();
        }
 else {
          packet=queue.poll(l,timeUnit);
        }
        try {
          this.result=handleResult(packet);
        }
 catch (        ExecutionException e) {
          throw new RuntimeException(e);
        }
      }
      if (it == null) {
        it=result.iterator();
      }
    }
  }
  if (it.hasNext()) {
    return it.next();
  }
 else {
    return OBJECT_DONE;
  }
}
