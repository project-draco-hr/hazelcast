{
  final HazelcastInstance h=Hazelcast.newHazelcastInstance(config);
  client=TestUtility.newHazelcastClient(h);
  new Thread(new Runnable(){
    public void run(){
      try {
        Thread.sleep(10);
      }
 catch (      InterruptedException ignore) {
      }
      h.shutdown();
    }
  }
).start();
  for (int i=0; i < 10; i++) {
    client.getCluster().getMembers();
    Thread.sleep(2);
  }
  fail();
}
