{
  active=false;
  for (  SerializerAdapter serializer : typeMap.values()) {
    serializer.destroy();
  }
  typeMap.clear();
  idMap.clear();
  global.set(null);
  constantTypesMap.clear();
  for (  BufferObjectDataOutput output : outputPool) {
    IOUtil.closeResource(output);
  }
  outputPool.clear();
}
