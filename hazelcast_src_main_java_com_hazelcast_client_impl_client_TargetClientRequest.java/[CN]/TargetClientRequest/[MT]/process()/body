{
  final ClientEndpoint endpoint=getEndpoint();
  Operation op=prepareOperation();
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=operationService.createInvocationBuilder(getServiceName(),op,getTarget()).setTryCount(TRY_COUNT).setResultDeserialized(false).setCallback(new Callback<Object>(){
    public void notify(    Object object){
      endpoint.sendResponse(filter(object),getCallId());
    }
  }
);
  builder.invoke();
}
