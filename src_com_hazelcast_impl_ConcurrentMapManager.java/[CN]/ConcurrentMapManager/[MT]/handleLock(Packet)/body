{
  if (rightRemoteTarget(packet)) {
    remoteReq.setPacket(packet);
    doLock(remoteReq);
    if (!remoteReq.scheduled) {
      if (remoteReq.response == Boolean.TRUE) {
        sendResponse(packet);
      }
 else {
        sendResponseFailure(packet);
      }
    }
 else {
      packet.returnToContainer();
    }
    remoteReq.reset();
  }
}
