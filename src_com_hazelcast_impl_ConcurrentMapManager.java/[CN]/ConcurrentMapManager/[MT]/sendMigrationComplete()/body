{
  for (  MemberImpl member : lsMembers) {
    if (!member.localMember()) {
      Invocation inv=obtainServiceInvocation();
      inv.name="cmap";
      inv.operation=OP_CMAP_MIGRATION_COMPLETE;
      boolean sent=send(inv,member.getAddress());
      if (!sent) {
        inv.returnToContainer();
      }
    }
  }
}
