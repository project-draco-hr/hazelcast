{
  if (DEBUG)   log("=========================================");
  for (int i=0; i < BLOCK_COUNT; i++) {
    if (DEBUG)     log(mapBlocks.get(i));
  }
  Collection<CMap> cmaps=maps.values();
  for (  CMap cmap : cmaps) {
    if (DEBUG)     log(cmap);
  }
  if (DEBUG)   log("=========================================");
}
