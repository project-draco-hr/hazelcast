{
  if (rightRemoteTarget(inv)) {
    remoteReq.setInvocation(inv);
    doPutMulti(remoteReq);
    if (!remoteReq.scheduled) {
      if (remoteReq.response == Boolean.TRUE) {
        sendResponse(inv);
      }
 else {
        sendResponseFailure(inv);
      }
    }
 else {
      inv.returnToContainer();
    }
    remoteReq.reset();
  }
}
