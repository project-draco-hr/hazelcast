{
  if (rightRemoteTarget(inv)) {
    remoteReq.setInvocation(inv);
    doPut(remoteReq);
    if (!remoteReq.scheduled) {
      Data oldValue=(Data)remoteReq.response;
      if (oldValue != null && oldValue.size() > 0) {
        doSet(oldValue,inv.value);
      }
      sendResponse(inv);
    }
 else {
      inv.returnToContainer();
    }
    remoteReq.reset();
  }
}
