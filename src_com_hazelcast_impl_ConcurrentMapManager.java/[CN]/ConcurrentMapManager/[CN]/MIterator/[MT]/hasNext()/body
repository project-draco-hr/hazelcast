{
  if (next != null) {
    if (next.copyCount-- <= 0) {
      next=null;
    }
  }
  while (next == null) {
    boolean canRead=setNextBlock();
    if (!canRead)     return false;
    next=read.read(name,currentBlockId,currentIndex);
    if (next == null) {
      currentIndex=-1;
    }
 else {
      currentIndex=read.lastReadRecordIndex;
      currentIndex++;
    }
  }
  hasNextCalled=true;
  return true;
}
