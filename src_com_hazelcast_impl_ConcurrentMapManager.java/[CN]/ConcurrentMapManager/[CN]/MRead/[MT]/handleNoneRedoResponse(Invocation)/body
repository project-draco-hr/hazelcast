{
  removeCall(getId());
  lastReadRecordId=inv.recordId;
  Data key=inv.doTake(inv.key);
  if (key == null) {
    setResult(null);
  }
 else {
    Data value=null;
    if (containsValue) {
      value=inv.doTake(inv.data);
    }
    setResult(new SimpleDataEntry(request.name,request.blockId,key,value,(int)inv.longValue));
  }
}
