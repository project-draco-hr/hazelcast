{
  if (backupOps != null) {
    if (backupOps.size() > 0) {
      Iterator<VersionedBackupOp> it=backupOps.iterator();
      while (it.hasNext()) {
        VersionedBackupOp bo=it.next();
        if (bo.getVersion() < version + 1) {
          it.remove();
        }
 else         if (bo.getVersion() == version + 1) {
          bo.run();
          version=bo.getVersion();
          it.remove();
        }
 else {
          return;
        }
      }
    }
  }
}
