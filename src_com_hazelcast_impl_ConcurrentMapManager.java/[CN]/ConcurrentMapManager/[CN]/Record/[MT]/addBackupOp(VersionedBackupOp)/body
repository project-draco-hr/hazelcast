{
  if (backupOps == null) {
    backupOps=new TreeSet<VersionedBackupOp>();
  }
  backupOps.add(bo);
  if (backupOps.size() > 5) {
    logger.log(Level.FINEST,id + " Forcing backup.run version " + version);
    Iterator<VersionedBackupOp> it=backupOps.iterator();
    while (it.hasNext()) {
      VersionedBackupOp v=it.next();
      v.run();
      version=v.getVersion();
      v.request.reset();
      it.remove();
    }
  }
}
