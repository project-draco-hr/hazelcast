{
  CMap map=getMap(request.name);
  backupInfo.reset();
  int backupCount=map.getNumberOfBackups();
  backupCount=Math.min(backupCount,lsMembers.size());
  if (backupCount > 0) {
    backupInfo.set(request.name,backupCount,target,doHardCopy(request.key),doHardCopy(request.value));
  }
  super.process();
}
