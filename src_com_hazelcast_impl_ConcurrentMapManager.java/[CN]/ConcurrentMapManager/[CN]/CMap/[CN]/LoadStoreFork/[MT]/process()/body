{
  processing.set(true);
  for (int i=0; i < 5; i++) {
    final Request request=qRequests.poll();
    if (request != null) {
      execute(request);
      offerSize.decrementAndGet();
      qResponses.offer(request);
      if (!processing.get()) {
        enqueueAndReturn(LoadStoreFork.this);
      }
    }
 else {
      processing.set(false);
      return;
    }
  }
}
