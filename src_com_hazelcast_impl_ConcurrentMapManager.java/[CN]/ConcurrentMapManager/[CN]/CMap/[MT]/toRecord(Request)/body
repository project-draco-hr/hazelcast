{
  Record record=getRecord(req.key);
  if (record == null) {
    record=createNewRecord(req.key,req.value);
  }
 else {
    if (req.value != null) {
      record.setValue(req.value);
    }
  }
  record.lockAddress=req.lockAddress;
  record.lockThreadId=req.lockThreadId;
  record.lockCount=req.lockCount;
  return record;
}
