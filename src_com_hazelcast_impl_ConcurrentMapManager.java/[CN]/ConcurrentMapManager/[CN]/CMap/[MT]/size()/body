{
  int size=0;
  Collection<Record> records=mapRecords.values();
  for (  Record record : records) {
    if (record.value != null) {
      Block block=mapBlocks.get(record.blockId);
      if (thisAddress.equals(block.owner)) {
        size++;
        size+=record.getCopyCount();
      }
    }
  }
  return size;
}
