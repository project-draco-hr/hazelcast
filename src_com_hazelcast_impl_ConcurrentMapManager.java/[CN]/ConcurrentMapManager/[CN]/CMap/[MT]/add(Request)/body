{
  Record record=getRecord(req.key);
  if (record == null) {
    record=createNewRecord(req.key,null);
    req.key=null;
  }
 else {
    if (req.operation == OP_CMAP_ADD_TO_SET) {
      return false;
    }
  }
  record.version++;
  record.incrementCopyCount();
  fireMapEvent(mapListeners,name,EntryEvent.TYPE_ADDED,record);
  return true;
}
