{
  if (isMigrating()) {
    packet.responseType=RESPONSE_REDO;
    sendResponse(packet);
  }
 else   if (!rightRemoteTarget(packet)) {
  }
 else {
    Request remoteReq=new Request();
    remoteReq.setFromPacket(packet);
    remoteReq.local=false;
    packet.returnToContainer();
    handle(remoteReq);
  }
}
