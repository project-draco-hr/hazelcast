{
  while (src.hasRemaining()) {
    if (!connectionTypeSet) {
      if (!setConnectionType(src)) {
        return;
      }
      connectionTypeSet=true;
    }
    if (packet == null) {
      packet=new Packet();
    }
    boolean complete=packet.readFrom(src);
    if (complete) {
      packet.setConn(connection);
      ioService.handleClientPacket(packet);
      packet=null;
    }
 else {
      break;
    }
  }
}
