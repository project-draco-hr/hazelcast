{
  final NodeEngine nodeEngine=lockProxy.getNodeEngine();
  Future f=nodeEngine.getOperationService().invokeOnPartition(SERVICE_NAME,new SignalOperation(namespace,lockProxy.key,ThreadUtil.getThreadId(),conditionId,all),partitionId);
  try {
    f.get();
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
