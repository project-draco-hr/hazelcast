{
  fireCleanup();
  if (evictionPolicy == MapConfig.EvictionPolicy.NONE && cache.size() >= maxSize) {
    return;
  }
  if (evictionPolicy != MapConfig.EvictionPolicy.NONE && cache.size() >= maxSize) {
    fireEvictCache();
  }
  if (value == null)   cache.put(key,NULLOBJECT);
 else   cache.put(key,new CacheRecord(key,value));
}
