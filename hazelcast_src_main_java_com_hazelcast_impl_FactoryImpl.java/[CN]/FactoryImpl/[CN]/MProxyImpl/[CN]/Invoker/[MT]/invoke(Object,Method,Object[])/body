{
  beforeCall();
  try {
    return method.invoke(mproxyReal,args);
  }
 catch (  Throwable e) {
    if (factory.restarted) {
      return invoke(proxy,method,args);
    }
 else     if (e instanceof RuntimeException) {
      throw (RuntimeException)e;
    }
 else {
      throw new RuntimeException(e);
    }
  }
 finally {
    afterCall();
  }
}
