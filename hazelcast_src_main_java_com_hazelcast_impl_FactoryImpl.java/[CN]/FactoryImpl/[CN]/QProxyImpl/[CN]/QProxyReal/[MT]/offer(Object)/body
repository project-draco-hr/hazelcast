{
  check(obj);
  Offer offer=blockingQueueManager.new Offer();
  try {
    boolean result=offer.offer(name,obj,0);
    if (!result) {
      operationsCounter.incrementRejectedOffers();
    }
    operationsCounter.incrementOffers();
    return result;
  }
 catch (  InterruptedException e) {
    return false;
  }
}
