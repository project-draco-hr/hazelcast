{
  FactoryImpl factory=getFactory(factoryName);
  MProxy map=factory.idGeneratorMapProxy;
  map.lock(name);
  try {
    Long max=(Long)map.get(name);
    if (max == null) {
      max=0L;
      map.put(name,0L);
      return max;
    }
 else {
      Long newMax=max + 1;
      map.put(name,newMax);
      return newMax;
    }
  }
  finally {
    map.unlock(name);
  }
}
