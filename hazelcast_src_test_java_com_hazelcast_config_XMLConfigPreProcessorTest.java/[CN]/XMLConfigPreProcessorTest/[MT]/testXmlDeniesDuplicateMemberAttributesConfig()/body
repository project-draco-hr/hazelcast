{
  String xml="<hazelcast>\n" + "    <member-attributes>\n" + "        <attribute name=\"attribute.float\" type=\"float\">1234.5678</attribute>\n"+ "    </member-attributes>\n"+ "    <member-attributes>\n"+ "        <attribute name=\"attribute.float\" type=\"float\">1234.5678</attribute>\n"+ "    </member-attributes>\n"+ "</hazelcast>";
  ByteArrayInputStream bis=new ByteArrayInputStream(xml.getBytes());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder(bis);
  configBuilder.build();
}
