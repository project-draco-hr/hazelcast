{
  String xml="<hazelcast>\n" + "    <properties>\n" + "        <property>foo</property>\n"+ "    </properties>"+ "    <properties>\n"+ "        <property>bar</property>\n"+ "    </properties>"+ "</hazelcast>";
  ByteArrayInputStream bis=new ByteArrayInputStream(xml.getBytes());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder(bis);
  configBuilder.build();
}
