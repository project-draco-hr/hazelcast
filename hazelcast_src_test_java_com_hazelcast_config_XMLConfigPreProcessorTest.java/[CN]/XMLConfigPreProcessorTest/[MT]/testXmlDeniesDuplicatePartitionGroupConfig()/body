{
  String xml="<hazelcast>\n" + "   <partition-group>\n" + "      <member-group>\n"+ "          <interface>foo</interface>\n"+ "      </member-group>\n"+ "   </partition-group>\n"+ "   <partition-group>\n"+ "      <member-group>\n"+ "          <interface>bar</interface>\n"+ "      </member-group>\n"+ "   </partition-group>\n"+ "</hazelcast>";
  ByteArrayInputStream bis=new ByteArrayInputStream(xml.getBytes());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder(bis);
  configBuilder.build();
}
