{
  String xml="<hazelcast>\n" + "   <listeners>" + "        <listener>foo</listener>\n\n"+ "   </listeners>\n"+ "   <listeners>"+ "        <listener>bar</listener>\n\n"+ "   </listeners>\n"+ "</hazelcast>";
  ByteArrayInputStream bis=new ByteArrayInputStream(xml.getBytes());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder(bis);
  configBuilder.build();
}
