{
  reset();
  set(false,packet.operation,packet.name,doTake(packet.key),doTake(packet.value),packet.blockId,packet.timeout,packet.txnId,packet.callId,packet.threadId,packet.lockAddress,packet.lockCount,packet.conn.getEndPoint(),packet.longValue,packet.recordId,packet.version);
  indexCount=packet.indexCount;
  for (int i=0; i < indexCount; i++) {
    indexes[i]=packet.indexes[i];
  }
}
