{
  reset();
  set(req.local,req.operation,req.name,null,null,req.blockId,req.timeout,req.txnId,req.eventId,req.lockThreadId,req.lockAddress,req.lockCount,req.caller,req.longValue,req.recordId,req.version);
  if (hardCopy) {
    key=doHardCopy(req.key);
    value=doHardCopy(req.value);
  }
 else {
    key=req.key;
    value=req.value;
    req.key=null;
    req.value=null;
  }
  attachment=req.attachment;
  response=req.response;
  scheduled=req.scheduled;
}
