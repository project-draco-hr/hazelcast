{
  reset();
  set(req.local,req.operation,req.name,null,null,req.blockId,req.timeout,req.ttl,req.txnId,req.callId,req.lockThreadId,req.lockAddress,req.lockCount,req.caller,req.longValue,req.version);
  key=req.key;
  value=req.value;
  if (!hardCopy) {
    req.key=null;
    req.value=null;
  }
  attachment=req.attachment;
  response=req.response;
  scheduled=req.scheduled;
  indexes=req.indexes;
  indexTypes=req.indexTypes;
}
