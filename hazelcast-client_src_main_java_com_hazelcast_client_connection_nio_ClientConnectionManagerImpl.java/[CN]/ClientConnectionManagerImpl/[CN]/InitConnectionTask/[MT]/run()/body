{
  final Object lock=getLock(target);
synchronized (lock) {
    ClientConnection connection=connections.get(target);
    if (connection != null) {
      return;
    }
    try {
      initializeConnection(remoteAddress,authenticator);
    }
 catch (    Exception e) {
      logger.finest(e);
    }
 finally {
      connectionsInProgress.remove(target);
    }
  }
}
