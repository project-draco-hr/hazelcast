{
  Authenticator authenticator=new ClusterAuthenticator();
  int count=0;
  IOException lastError=null;
  while (count < RETRY_COUNT) {
    try {
      return getOrConnect(target,authenticator);
    }
 catch (    IOException e) {
      lastError=e;
    }
    count++;
    sleep();
  }
  throw lastError;
}
