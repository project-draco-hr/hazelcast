{
  BlockingCallback blockingCallback=new BlockingCallback();
  Connection connection=getOrTriggerConnectInternal(address,asOwner,blockingCallback);
  if (connection != null) {
    return connection;
  }
  try {
    return blockingCallback.get();
  }
 catch (  Throwable e) {
    throw ExceptionUtil.rethrow(e);
  }
}
