{
  inputThread=new NonBlockingIOThread(client.getThreadGroup(),client.getName() + ".ClientInSelector",Logger.getLogger(NonBlockingIOThread.class),OUT_OF_MEMORY_HANDLER);
  outSelector=new ClientNonBlockingOutputThread(client.getThreadGroup(),client.getName() + ".ClientOutSelector",Logger.getLogger(ClientNonBlockingOutputThread.class),OUT_OF_MEMORY_HANDLER);
}
