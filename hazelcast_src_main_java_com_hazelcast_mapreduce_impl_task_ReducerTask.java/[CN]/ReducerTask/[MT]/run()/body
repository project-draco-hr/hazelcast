{
  try {
    ReducerChunk<Key,Chunk> reducerChunk;
    while ((reducerChunk=reducerQueue.poll()) != null) {
      reduceChunk(reducerChunk.chunk);
      processProcessedState(reducerChunk);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    active.compareAndSet(true,false);
  }
}
