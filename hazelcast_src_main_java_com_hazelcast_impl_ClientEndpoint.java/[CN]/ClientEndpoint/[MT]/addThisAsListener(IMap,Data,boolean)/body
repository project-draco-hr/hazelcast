{
  Object oKey=toObject(key);
  if (!listeningMaps.contains(map) && !(listeningKeyExist(map,key))) {
    if (key == null)     map.addEntryListener(this,includeValue);
 else     map.addEntryListener(this,oKey,includeValue);
  }
  if (key == null)   listeningMaps.add(map);
 else   listeningKeysOfMaps.add(new Entry(map,oKey));
}
