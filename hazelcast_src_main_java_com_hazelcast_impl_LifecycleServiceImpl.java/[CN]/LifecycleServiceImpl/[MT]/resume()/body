{
synchronized (lifecycleLock) {
    if (paused.get()) {
      fireLifecycleEvent(new LifecycleEvent(LifecycleEvent.LifecycleState.RESUMING));
    }
 else {
      return false;
    }
    paused.set(false);
    fireLifecycleEvent(new LifecycleEvent(LifecycleEvent.LifecycleState.RESUMED));
    return true;
  }
}
