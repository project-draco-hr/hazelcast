{
synchronized (lifecycleLock) {
    if (paused.get()) {
      fireLifecycleEvent(new LifecycleEvent(LifecycleEvent.LifecycleState.RESTARTING));
    }
 else {
      return false;
    }
    paused.set(false);
    fireLifecycleEvent(new LifecycleEvent(LifecycleEvent.LifecycleState.RESTARTED));
    return true;
  }
}
