{
  ResponseHandler responseHandler=getResponseHandler();
  MapService mapService=(MapService)getService();
  MapPartition mapPartition=mapService.getMapPartition(getPartitionId(),name);
  Object val=mapService.getNodeService().toObject(dataValue);
  Collection<Record> records=mapPartition.records.values();
  for (  Record record : records) {
    if (record.getValue().equals(val)) {
      responseHandler.sendResponse(true);
      return;
    }
  }
  responseHandler.sendResponse(false);
}
