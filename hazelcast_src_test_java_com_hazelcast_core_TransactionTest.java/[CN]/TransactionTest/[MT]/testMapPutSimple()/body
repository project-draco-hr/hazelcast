{
  TransactionalMap txnMap=newTransactionalMapProxy("testMapPutSimple");
  txnMap.put("1","value");
  assertEquals(1,txnMap.size());
  txnMap.begin();
  assertEquals(1,txnMap.size());
  assertNull(txnMap.put("2","value"));
  assertEquals(2,txnMap.size());
  txnMap.commit();
  assertEquals(2,txnMap.size());
}
