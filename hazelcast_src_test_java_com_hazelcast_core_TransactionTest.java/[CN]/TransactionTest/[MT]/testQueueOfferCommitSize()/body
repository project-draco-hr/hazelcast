{
  TransactionalQueue txnq=newTransactionalQueueProxy("test");
  TransactionalQueue txnq2=newTransactionalQueueProxy("test");
  txnq.begin();
  txnq.offer("item");
  assertEquals(1,txnq.size());
  assertEquals(0,txnq2.size());
  txnq.commit();
  assertEquals(1,txnq.size());
  assertEquals(1,txnq2.size());
}
