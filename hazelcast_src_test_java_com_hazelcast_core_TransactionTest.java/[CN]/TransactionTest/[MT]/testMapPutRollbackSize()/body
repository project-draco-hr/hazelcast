{
  TransactionalMap txnMap=newTransactionalMapProxy("testMapPutRollbackSize");
  IMap imap=newMapProxy("testMapPutRollbackSize");
  txnMap.put("1","item");
  assertEquals(1,txnMap.size());
  assertEquals(1,imap.size());
  txnMap.begin();
  txnMap.put(2,"newone");
  assertEquals(2,txnMap.size());
  assertEquals(1,imap.size());
  txnMap.rollback();
  assertEquals(1,txnMap.size());
  assertEquals(1,imap.size());
}
