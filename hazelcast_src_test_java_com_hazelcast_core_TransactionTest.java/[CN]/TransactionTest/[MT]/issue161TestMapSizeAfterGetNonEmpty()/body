{
  IMap<String,String> map=Hazelcast.getMap("issue161TestMapSizeAfterGet");
  map.put("key","value");
  Transaction txn=Hazelcast.getTransaction();
  txn.begin();
  map.get("key");
  txn.commit();
  assertFalse(map.isLocked("key"));
  assertEquals(1,map.size());
  assertEquals("value",map.get("key"));
}
