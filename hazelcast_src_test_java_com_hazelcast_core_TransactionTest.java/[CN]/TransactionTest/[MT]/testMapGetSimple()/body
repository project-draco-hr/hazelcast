{
  TransactionalMap txnMap=newTransactionalMapProxy("testMapGetSimple");
  txnMap.put("1","value");
  txnMap.begin();
  assertEquals("value",txnMap.get("1"));
  assertEquals("value",txnMap.get("1"));
  assertEquals("value",txnMap.get("1"));
  assertEquals(1,txnMap.size());
  txnMap.commit();
  assertEquals(1,txnMap.size());
}
