{
  TransactionalMap txnMap=newTransactionalMapProxy("testMapContainsValue");
  txnMap.put("1","value");
  txnMap.put("2","value");
  Assert.assertTrue(txnMap.containsValue("value"));
  txnMap.begin();
  Assert.assertTrue(txnMap.containsValue("value"));
  txnMap.commit();
  assertEquals(2,txnMap.size());
}
