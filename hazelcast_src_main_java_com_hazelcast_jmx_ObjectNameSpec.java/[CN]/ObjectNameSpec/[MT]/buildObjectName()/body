{
  StringBuffer sb=new StringBuffer(NAME_DOMAIN);
  if (type != null) {
    sb.append("type=").append(type);
  }
  if (cluster != null) {
    if (type != null) {
      sb.append(',');
    }
    sb.append("Cluster=").append(cluster);
  }
  if (name != null) {
    if (type != null || cluster != null) {
      sb.append(',');
    }
    sb.append("name=").append(name);
  }
  return new ObjectName(sb.toString());
}
