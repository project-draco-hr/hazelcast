{
  check(listener);
  Protocol request=proxyHelper.createProtocol(Command.SLISTEN,new String[]{getName(),String.valueOf(includeValue)},null);
  ListenerThread thread=proxyHelper.createAListenerThread("hz.client.setListener.",client,request,new ItemEventLRH<E>(listener,includeValue,this));
  listenerMap.put(listener,thread);
  thread.start();
}
