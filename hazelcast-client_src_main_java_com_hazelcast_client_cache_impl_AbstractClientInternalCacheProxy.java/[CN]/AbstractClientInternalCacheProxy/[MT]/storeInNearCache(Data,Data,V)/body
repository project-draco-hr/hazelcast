{
  if (nearCache != null) {
    final Object valueToStore;
    if (nearCache.getInMemoryFormat() == InMemoryFormat.OBJECT) {
      valueToStore=value != null ? value : valueData;
    }
 else {
      valueToStore=valueData != null ? valueData : value;
    }
    nearCache.put(key,valueToStore);
  }
}
