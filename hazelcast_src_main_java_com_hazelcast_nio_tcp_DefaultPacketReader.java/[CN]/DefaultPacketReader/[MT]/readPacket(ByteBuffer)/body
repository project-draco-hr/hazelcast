{
  while (inBuffer.hasRemaining()) {
    if (packet == null) {
      packet=obtainPacket();
    }
    boolean complete=packet.readFrom(inBuffer);
    if (complete) {
      packet.setConn(connection);
      ioService.handleMemberPacket(packet);
      packet=null;
    }
 else {
      break;
    }
  }
}
