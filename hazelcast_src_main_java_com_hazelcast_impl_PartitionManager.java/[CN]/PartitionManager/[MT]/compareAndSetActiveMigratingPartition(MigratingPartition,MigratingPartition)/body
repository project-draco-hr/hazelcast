{
  lock.lock();
  try {
    if (expectedMigratingPartition == null) {
      if (migratingPartition == null) {
        migratingPartition=newMigratingPartition;
      }
    }
 else     if (expectedMigratingPartition.equals(migratingPartition)) {
      migratingPartition=newMigratingPartition;
    }
  }
  finally {
    lock.unlock();
  }
}
