{
  StringBuilder sb=new StringBuilder("PartitionManager[" + version + "] {\n");
  sb.append("migratingPartition: " + migratingPartition);
  sb.append("\n");
  sb.append("immediateQ:" + immediateTasksQueue.size());
  sb.append(", scheduledQ:" + scheduledTasksQueue.size());
  sb.append("\n}");
  return sb.toString();
}
