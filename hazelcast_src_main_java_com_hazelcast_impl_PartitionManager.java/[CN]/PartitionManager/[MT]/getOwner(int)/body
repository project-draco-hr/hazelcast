{
  if (!initialized) {
    firstArrangement();
  }
  Address owner=partitions[partitionId].getOwner();
  if (owner == null && !node.isMaster()) {
    nodeService.createSingleInvocation(PARTITION_SERVICE_NAME,new AssignPartitions(),-1).setTarget(node.getMasterAddress()).setTryCount(1).build().invoke();
    return partitions[partitionId].getOwner();
  }
  return owner;
}
