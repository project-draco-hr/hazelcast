{
  if (req.key == null && req.blockId != -1 && hashBlocks() != req.blockId) {
    logger.log(Level.FINEST,thisAddress + " blockHashes aren't the same:" + hashBlocks()+ ", request.blockId:"+ req.blockId+ " caller: "+ req.caller);
    return true;
  }
  if (req.key != null) {
    Block block=concurrentMapManager.getOrCreateBlock(req.key);
    return block.isMigrating();
  }
 else {
    return containsMigratingBlock();
  }
}
