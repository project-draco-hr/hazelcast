{
  for (int i=0; i < owners.length; i++) {
    Address owner=owners[i];
    if (owner != null && owner.equals(address)) {
      removeAddresses(i);
    }
  }
  for (int i=0; i < targets.length; i++) {
    Address target=targets[i];
    if (target != null && target.equals(address)) {
      removeAddresses(i);
    }
  }
}
