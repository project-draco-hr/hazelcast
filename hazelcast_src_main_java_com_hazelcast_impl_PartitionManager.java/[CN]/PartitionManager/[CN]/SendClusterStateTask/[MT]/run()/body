{
  if (concurrentMapManager.isMaster()) {
    logger.log(Level.INFO,"Remaining migration tasks in queue => Immediate-Tasks: " + immediateTasksQueue.size() + ", Scheduled-Tasks: "+ scheduledTasksQueue.size());
    final Node node=concurrentMapManager.node;
    concurrentMapManager.enqueueAndReturn(new Processable(){
      public void process(){
        if (!node.isActive())         return;
        sendClusterRuntimeState();
      }
    }
);
  }
}
