{
  HazelcastInstance hazelcastInstance=createHazelcastInstanceFactory(1).newHazelcastInstance();
  IAtomicReference<String> ref=hazelcastInstance.getAtomicReference("clear");
  ref.clear();
  assertNull(ref.get());
  ref.set("foo");
  ref.clear();
  assertNull(ref.get());
  ref.set(null);
  assertNull(ref.get());
}
