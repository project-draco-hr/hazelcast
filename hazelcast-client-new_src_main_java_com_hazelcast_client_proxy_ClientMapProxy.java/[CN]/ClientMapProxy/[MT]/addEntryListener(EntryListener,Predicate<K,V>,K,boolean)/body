{
  final Data keyData=toData(key);
  final Data predicateData=toData(predicate);
  ClientMessage request=MapAddEntryListenerToKeyWithPredicateCodec.encodeRequest(name,keyData,predicateData,includeValue);
  EventHandler<ClientMessage> handler=createHandler(listener,includeValue);
