{
  checkNotNull(key,NULL_KEY_IS_NOT_ALLOWED);
  checkNotNull(value,NULL_VALUE_IS_NOT_ALLOWED);
  final Data keyData=toData(key);
  final Data valueData=toData(value);
  invalidateNearCache(keyData);
  ClientMessage request=MapPutParameters.encode(name,keyData,valueData,ThreadUtil.getThreadId(),getTimeInMillis(ttl,timeunit));
  ClientMessage response=invoke(request,keyData);
  GenericResultParameters resultParameters=GenericResultParameters.decode(response);
  return toObject(resultParameters.result);
}
