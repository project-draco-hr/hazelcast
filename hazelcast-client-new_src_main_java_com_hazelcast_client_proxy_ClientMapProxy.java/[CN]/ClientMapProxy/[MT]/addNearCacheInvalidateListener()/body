{
  try {
    ClientMessage request=MapAddNearCacheEntryListenerCodec.encodeRequest(name,false);
    EventHandler handler=new ClientMapAddNearCacheEventHandler();
    String registrationId=getContext().getListenerService().startListening(request,null,handler,new ClientMessageDecoder(){
      @Override public <T>T decodeClientMessage(      ClientMessage clientMessage){
        return (T)MapAddNearCacheEntryListenerCodec.decodeResponse(clientMessage).response;
      }
    }
);
    nearCache.setId(registrationId);
  }
 catch (  Exception e) {
    Logger.getLogger(ClientHeapNearCache.class).severe("-----------------\n Near Cache is not initialized!!! \n-----------------",e);
  }
}
