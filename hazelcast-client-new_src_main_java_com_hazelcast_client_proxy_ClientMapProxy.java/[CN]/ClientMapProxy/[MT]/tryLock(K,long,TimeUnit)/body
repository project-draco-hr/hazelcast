{
  checkNotNull(key,NULL_KEY_IS_NOT_ALLOWED);
  final Data keyData=toData(key);
  ClientMessage request=MapTryLockWithTimeoutParameters.encode(name,keyData,ThreadUtil.getThreadId(),getTimeInMillis(time,timeunit));
  ClientMessage response=invoke(request,keyData);
  BooleanResultParameters resultParameters=BooleanResultParameters.decode(response);
  return resultParameters.result;
}
