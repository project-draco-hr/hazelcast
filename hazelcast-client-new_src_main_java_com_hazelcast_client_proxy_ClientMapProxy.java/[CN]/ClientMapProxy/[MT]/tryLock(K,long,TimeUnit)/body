{
  checkNotNull(key,NULL_KEY_IS_NOT_ALLOWED);
  final Data keyData=toData(key);
  ClientMessage request=MapTryLockCodec.encodeRequest(name,keyData,ThreadUtil.getThreadId(),getTimeInMillis(time,timeunit));
  ClientMessage response=invoke(request,keyData);
  MapTryLockCodec.ResponseParameters resultParameters=MapTryLockCodec.decodeResponse(response);
  return resultParameters.response;
}
