{
  EntryEventType entryEventType=EntryEventType.getByType(eventType);
switch (entryEventType) {
case ADDED:
case REMOVED:
case UPDATED:
case MERGED:
case EVICTED:
case EXPIRED:
    nearCache.remove(key);
  break;
case CLEAR_ALL:
case EVICT_ALL:
nearCache.clear();
break;
default :
throw new IllegalArgumentException("Not a known event type " + entryEventType);
}
}
