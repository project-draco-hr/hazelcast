{
  for (int i=0; i < COUNT; i++) {
    DelegatingFuture f=(DelegatingFuture)map.putAsync(i,i);
    if (i == COUNT / 2) {
      server1.getLifecycleService().terminate();
    }
  }
  try {
    assertSizeEventually(COUNT,map);
  }
  finally {
    System.out.println("-----------------------------");
  }
}
