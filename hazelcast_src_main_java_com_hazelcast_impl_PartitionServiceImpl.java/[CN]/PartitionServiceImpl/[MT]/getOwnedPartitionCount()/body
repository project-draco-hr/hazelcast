{
  int currentCount=ownedPartitionCount;
  if (currentCount > 0) {
    return currentCount;
  }
 else {
    currentCount=0;
    for (    Partition partition : partitions) {
      if (partition.getOwner() == null || partition.getOwner().localMember()) {
        currentCount++;
      }
    }
    ownedPartitionCount=currentCount;
    return currentCount;
  }
}
