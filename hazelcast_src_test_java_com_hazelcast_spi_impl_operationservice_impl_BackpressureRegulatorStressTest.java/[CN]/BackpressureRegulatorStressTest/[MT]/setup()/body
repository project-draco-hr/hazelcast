{
  Config config=new Config().setProperty(PROP_OPERATION_BACKUP_TIMEOUT_MILLIS,"60000").setProperty(PROP_BACKPRESSURE_ENABLED,"true").setProperty(PROP_BACKPRESSURE_SYNCWINDOW,"10").setProperty(PROP_BACKPRESSURE_MAX_CONCURRENT_INVOCATIONS_PER_PARTITION,"2");
  HazelcastInstance[] cluster=createHazelcastInstanceFactory(2).newInstances(config);
  local=cluster[0];
  remote=cluster[1];
  localOperationService=(OperationServiceImpl)getOperationService(local);
}
