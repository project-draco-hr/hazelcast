{
  checkNotNull(key,NULL_KEY_IS_NOT_ALLOWED);
  Data keyData=toData(key);
  Data predicateData=toData(predicate);
  ClientMessage request=ReplicatedMapAddEntryListenerToKeyWithPredicateCodec.encodeRequest(name,keyData,predicateData);
  EventHandler<ClientMessage> handler=createHandler(listener);
  ClientMessageDecoder responseDecoder=new ClientMessageDecoder(){
    @Override public <T>T decodeClientMessage(    ClientMessage clientMessage){
      return (T)ReplicatedMapAddEntryListenerToKeyWithPredicateCodec.decodeResponse(clientMessage).response;
    }
  }
;
  return listenOnPartition(request,getOrInitTargetPartitionId(),handler,responseDecoder);
}
