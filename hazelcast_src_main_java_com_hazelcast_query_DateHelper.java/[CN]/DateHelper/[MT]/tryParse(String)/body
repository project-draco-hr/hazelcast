{
  try {
    return getUtilDateFormat().parse(value);
  }
 catch (  Exception ignored) {
  }
  try {
    return getTimestampFormat().parse(value);
  }
 catch (  Exception ignored) {
  }
  try {
    return getSqlDateFormat().parse(value);
  }
 catch (  Exception ignored) {
  }
  return throwRuntimeParseException(value,null);
}
