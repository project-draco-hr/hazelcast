{
  response=false;
  QueueContainer container=getContainer();
  itemList=container.addAll(dataList);
  if (itemList.size() > 0) {
    try {
      storeAll(false);
    }
 catch (    Exception e) {
      for (int i=0; i < itemList.size(); i++) {
        container.pollBackup();
      }
      throw new RetryableException(e);
    }
    response=true;
  }
}
