{
  if (resultSets.size() == 0)   return false;
  if (currentIterator != null && currentIterator.hasNext()) {
    return true;
  }
  while (currentIndex < resultSets.size()) {
    currentIterator=resultSets.get(currentIndex++).values().iterator();
    if (currentIterator.hasNext()) {
      return true;
    }
  }
  return false;
}
