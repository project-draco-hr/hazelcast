{
  if (connection.getEndPoint().equals(ownerConnectionAddress)) {
    ClientConnection clientConnection=(ClientConnection)connection;
    Exception ex=newTargetDisconnectedExceptionCausedByHeartBeat(clientConnection.getRemoteEndpoint(),clientConnection.getLastHeartbeatMillis(),clientConnection.getCloseCause());
    connectionManager.destroyConnection(connection,ex);
  }
}
