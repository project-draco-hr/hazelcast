{
  Address address=partitionService.getPartition(partitionId).getOwner();
  if (address == null) {
    try {
      address=partitionService.getPartitionOwner(partitionId);
    }
 catch (    Exception ignored) {
    }
  }
  if (address != null) {
    return partitionService.getMember(address);
  }
  return null;
}
