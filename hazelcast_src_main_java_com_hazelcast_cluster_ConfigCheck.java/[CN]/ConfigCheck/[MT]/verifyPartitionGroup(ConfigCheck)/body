{
  if (!partitionGroupEnabled && other.partitionGroupEnabled || partitionGroupEnabled && !other.partitionGroupEnabled) {
    throw new ConfigMismatchException("Incompatible partition groups! " + "this: " + (partitionGroupEnabled ? "enabled" : "disabled") + " / "+ memberGroupType+ ", other: "+ (other.partitionGroupEnabled ? "enabled" : "disabled")+ " / "+ other.memberGroupType);
  }
  if (partitionGroupEnabled && memberGroupType != other.memberGroupType) {
    throw new ConfigMismatchException("Incompatible partition groups! this: " + memberGroupType + ", other: "+ other.memberGroupType);
  }
}
