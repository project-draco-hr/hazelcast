{
  if (data == null)   return null;
  int newSize=0;
  int len=data.length;
  int i=0;
  for (; i < len; i++) {
    if (!isWhiteSpace(data[i]))     newSize++;
  }
  if (newSize == len)   return data;
  byte[] arrayWithoutSpaces=new byte[newSize];
  int j=0;
  for (i=0; i < len; i++) {
    if (isWhiteSpace(data[i]))     continue;
 else     arrayWithoutSpaces[j++]=data[i];
  }
  return arrayWithoutSpaces;
}
