{
  final IMap map=getMap();
  map.lock(1);
  final SimpleClient client=getClient();
  client.send(new MapIsLockedRequest(mapName,TestUtil.toData(1)));
  assertEquals(true,client.receive());
  map.unlock(1);
  client.send(new MapIsLockedRequest(mapName,TestUtil.toData(1)));
  assertEquals(false,client.receive());
}
