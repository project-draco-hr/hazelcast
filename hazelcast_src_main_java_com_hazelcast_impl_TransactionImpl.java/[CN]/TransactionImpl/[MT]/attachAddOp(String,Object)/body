{
  TransactionRecord rec=findTransactionRecord(name,key);
  if (rec == null) {
    rec=new TransactionRecord(name,key,1,true);
    transactionRecords.add(rec);
    return null;
  }
 else {
    final Object old=rec.value;
    rec.value=((Integer)rec.value) + 1;
    rec.removed=false;
    return old;
  }
}
