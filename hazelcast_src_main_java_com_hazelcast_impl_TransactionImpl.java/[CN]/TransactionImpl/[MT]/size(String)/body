{
  int size=0;
  for (  final TransactionRecord transactionRecord : transactionRecords) {
    if (transactionRecord.name.equals(name)) {
      if (transactionRecord.removed) {
        if (!transactionRecord.newRecord) {
          if (transactionRecord.map) {
            size--;
          }
        }
      }
 else {
        size++;
      }
    }
  }
  return size;
}
