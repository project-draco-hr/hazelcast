{
  TransactionRecord rec=findTransactionRecord(name,key);
  Object oldValue=null;
  if (rec == null) {
    rec=new TransactionRecord(name,key,value,newRecord);
    rec.removed=true;
    transactionRecords.add(rec);
  }
 else {
    oldValue=rec.value;
    rec.value=value;
  }
  rec.removed=true;
  return oldValue;
}
