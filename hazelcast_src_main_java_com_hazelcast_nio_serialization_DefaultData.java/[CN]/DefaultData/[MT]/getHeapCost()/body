{
  final int integerSizeInBytes=4;
  final int arrayHeaderSizeInBytes=16;
  int total=0;
  total+=integerSizeInBytes;
  if (header != null) {
    total+=arrayHeaderSizeInBytes;
    total+=header.length;
  }
 else {
    total+=integerSizeInBytes;
  }
  if (data != null) {
    total+=arrayHeaderSizeInBytes;
    total+=data.length;
  }
 else {
    total+=integerSizeInBytes;
  }
  total+=integerSizeInBytes;
  return total;
}
