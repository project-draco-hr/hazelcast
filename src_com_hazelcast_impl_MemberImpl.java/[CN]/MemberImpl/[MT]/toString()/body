{
  StringBuilder sb=new StringBuilder("Member [");
  sb.append(address.getHost());
  sb.append(":");
  sb.append(address.getPort());
  sb.append("] ");
  if (thisMember) {
    sb.append("this ");
  }
  if (Node.DEBUG && Node.get().getMasterAddress().equals(address)) {
    sb.append("* ");
  }
  return sb.toString();
}
