{
  AssertionError error=null;
  long iterations=timeoutSeconds * 5;
  int sleepMillis=200;
  for (int k=0; k < iterations; k++) {
    try {
      task.run();
      return;
    }
 catch (    AssertionError e) {
      error=e;
    }
    sleepMillis(sleepMillis);
  }
  printAllStackTraces();
  throw error;
}
