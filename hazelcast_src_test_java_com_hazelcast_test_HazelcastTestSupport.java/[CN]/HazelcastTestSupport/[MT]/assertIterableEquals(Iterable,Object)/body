{
  int counter=0;
  for (  Object o : iter) {
    if (values.length < counter + 1) {
      throw new AssertionError("Iterator and values sizes are not equal");
    }
    assertEquals(values[counter],o);
    counter++;
  }
  if (values.length != counter) {
    throw new AssertionError("Iterator and values sizes are not equal");
  }
}
