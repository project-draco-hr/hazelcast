{
  managedConnection.log(Level.FINEST,"XA start: " + xid);
switch (flags) {
case TMNOFLAGS:
    validateInner(false);
  setInner();
inner.start(xid,flags);
break;
case TMRESUME:
case TMJOIN:
validateInner(true);
inner.start(xid,flags);
break;
default :
throw new XAException(XAException.XAER_INVAL);
}
}
