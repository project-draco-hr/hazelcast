{
  if (selector != null) {
    try {
      if (logger.isFinestEnabled()) {
        logger.finest("Closing selector " + Thread.currentThread().getName());
      }
      selector.close();
    }
 catch (    final Exception e) {
      Logger.getLogger(SocketAcceptor.class).finest("Exception while closing selector",e);
    }
  }
}
