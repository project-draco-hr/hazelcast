{
  final NodeEngine nodeEngine=mapServiceContext.getNodeEngine();
  final Collection<EventRegistration> registrations=nodeEngine.getEventService().getRegistrations(mapServiceContext.serviceName(),mapName);
  if (registrations.isEmpty()) {
    return;
  }
  final String source=nodeEngine.getThisAddress().toString();
  final MapEventData mapEventData=new MapEventData(source,mapName,caller,eventType.getType(),numberOfEntriesAffected);
  nodeEngine.getEventService().publishEvent(mapServiceContext.serviceName(),registrations,mapEventData,mapName.hashCode());
}
