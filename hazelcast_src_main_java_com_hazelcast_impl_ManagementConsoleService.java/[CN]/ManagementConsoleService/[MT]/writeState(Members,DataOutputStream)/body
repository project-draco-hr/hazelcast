{
  dos.writeInt(members.socketAddresses.length);
  for (  SocketAddress socketAddress : members.socketAddresses) {
    MemberState memberState=members.getMemberState(socketAddress);
    if (memberState != null) {
      dos.write((byte)1);
      memberState.writeData(dos);
    }
 else {
      dos.write((byte)0);
    }
  }
}
