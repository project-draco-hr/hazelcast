{
  if (members != null) {
    for (    SocketAddress address : members.socketAddresses) {
      try {
        updateState();
        bbState.clear();
        state.writeData(dos);
        packet.setData(bbState.array(),0,bbState.position());
        packet.setSocketAddress(address);
        socket.send(packet);
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
}
