{
  return new DataSerializableFactory(){
    public IdentifiedDataSerializable create(    int typeId){
switch (typeId) {
case GET_PARTITIONS:
        return new GetPartitionsRequest();
case PARTITIONS:
      return new PartitionsResponse();
case ADD_LISTENER:
    return new AddMigrationListenerRequest();
}
return null;
}
}
;
}
