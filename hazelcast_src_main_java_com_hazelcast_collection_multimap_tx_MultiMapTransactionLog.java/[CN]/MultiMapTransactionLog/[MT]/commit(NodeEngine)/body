{
  try {
    int partitionId=nodeEngine.getPartitionService().getPartitionId(key);
    Invocation invocation=nodeEngine.getOperationService().createInvocationBuilder(CollectionService.SERVICE_NAME,op,partitionId).build();
    return invocation.invoke();
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
