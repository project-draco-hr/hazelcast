{
  NodeEngineImpl nodeEngine=(NodeEngineImpl)getNodeEngine();
  for (  MigrationAwareService service : nodeEngine.getServices(MigrationAwareService.class)) {
    try {
      service.beforeMigration(event);
      service.commitMigration(event);
    }
 catch (    Exception e) {
      logMigrationError(e);
    }
  }
}
