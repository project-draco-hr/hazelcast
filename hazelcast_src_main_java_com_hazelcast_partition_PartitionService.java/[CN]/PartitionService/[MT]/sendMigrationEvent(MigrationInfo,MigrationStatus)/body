{
  final Collection<MemberImpl> members=node.clusterService.getMemberList();
  for (  MemberImpl member : members) {
    if (!member.localMember()) {
      nodeService.getInvocationService().createInvocationBuilder(SERVICE_NAME,new MigrationEventOperation(status,migrationInfo),member.getAddress()).setTryCount(1).build().invoke();
    }
  }
  nodeService.getEventExecutor().execute(new Runnable(){
    public void run(){
      fireMigrationEvent(migrationInfo,status);
    }
  }
);
}
