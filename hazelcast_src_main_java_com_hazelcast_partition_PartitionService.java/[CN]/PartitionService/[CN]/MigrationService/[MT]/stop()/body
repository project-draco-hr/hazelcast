{
  clearTaskQueues();
  try {
    final CountDownLatch stopLatch=new CountDownLatch(1);
    immediateTasksQueue.offer(new Runnable(){
      public void run(){
        running=false;
        stopLatch.countDown();
      }
    }
);
    stopLatch.await(1,TimeUnit.SECONDS);
  }
 catch (  Throwable ignore) {
  }
}
