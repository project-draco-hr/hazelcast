{
  while (true) {
    final Runnable runnable=poll();
    if (runnable != null) {
      runnable.run();
      long left=size.decrementAndGet();
      if (left < 0) {
        throw new RuntimeException("Cannot be negative.");
      }
 else       if (left == 0) {
        return;
      }
    }
 else {
      return;
    }
  }
}
