{
  try {
    Map<Integer,Object> results=executeOnPartitions(name,new MapSizeExecutor());
    int total=0;
    for (    Object result : results.values()) {
      Integer size=(Integer)result;
      System.out.println(">> " + size);
      total+=size;
    }
    return total;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return 0;
}
