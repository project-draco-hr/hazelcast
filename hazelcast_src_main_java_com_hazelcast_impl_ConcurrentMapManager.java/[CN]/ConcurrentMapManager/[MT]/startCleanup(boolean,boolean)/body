{
  if (now) {
    for (    CMap cMap : maps.values()) {
      cMap.startCleanup(force);
    }
  }
 else {
    node.executorManager.executeNow(new Runnable(){
      public void run(){
        for (        CMap cMap : maps.values()) {
          cMap.startCleanup(force);
        }
      }
    }
);
  }
}
