{
  for (  CMap cmap : maps.values()) {
    try {
      logger.log(Level.FINEST,"Destroying CMap[" + cmap.name + "]");
      flush(cmap.name);
      cmap.destroy();
    }
 catch (    Throwable e) {
      if (node.isActive()) {
        logger.log(Level.SEVERE,e.getMessage(),e);
      }
    }
  }
  reset();
  partitionManager.shutdown();
}
