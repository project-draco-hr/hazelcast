{
  int blockId=getBlockId(key);
  Block block=blocks[blockId];
  if (block == null) {
    if (isMaster() && !isSuperClient()) {
      block=getOrCreateBlock(blockId);
      block.setOwner(thisAddress);
    }
 else     return null;
  }
  if (block.isMigrating()) {
    return null;
  }
  return block.getOwner();
}
