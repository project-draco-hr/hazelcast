{
  Pairs pairs=new Pairs(entries.size());
  for (  Object key : entries.keySet()) {
    Object value=entries.get(key);
    pairs.addKeyValue(new KeyValue(toData(key),toData(value)));
  }
  while (true) {
    node.checkNodeState();
    try {
      doPutAll(name,pairs);
      return;
    }
 catch (    Throwable e) {
      try {
        Thread.sleep(500);
      }
 catch (      InterruptedException e1) {
      }
    }
  }
}
