{
  while (true) {
    try {
      return doGetAll(name,keys);
    }
 catch (    Throwable e) {
      if (e instanceof InterruptedException) {
        handleInterruptedException();
      }
      try {
        Thread.sleep(redoWaitMillis);
      }
 catch (      InterruptedException e1) {
        handleInterruptedException();
      }
    }
  }
}
