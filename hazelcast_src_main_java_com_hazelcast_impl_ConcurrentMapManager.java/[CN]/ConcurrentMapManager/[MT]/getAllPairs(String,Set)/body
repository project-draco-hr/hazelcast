{
  while (true) {
    node.checkNodeState();
    try {
      return doGetAll(name,keys);
    }
 catch (    Throwable e) {
      TransactionImpl txn=ThreadContext.get().getCallContext().getTransaction();
      if (txn != null && txn.getStatus() == Transaction.TXN_STATUS_ACTIVE) {
        throw new RuntimeException(e);
      }
    }
  }
}
