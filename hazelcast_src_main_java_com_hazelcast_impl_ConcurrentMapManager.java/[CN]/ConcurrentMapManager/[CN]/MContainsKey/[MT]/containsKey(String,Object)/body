{
  this.key=key;
  this.nearCache=mapCaches.get(name);
  Data dataKey=toData(key);
  if (nearCache != null) {
    if (nearCache.containsKey(key)) {
      return true;
    }
 else     if (nearCache.getMaxSize() == Integer.MAX_VALUE) {
      return false;
    }
  }
  return booleanCall(CONCURRENT_MAP_CONTAINS,name,dataKey,null,0,-1);
}
