{
  if (backupCount > 0) {
    for (int i=0; i < backupCount; i++) {
      int distance=i + 1;
      MBackup backupOp=backupOps[i];
      if (backupOp == null) {
        backupOp=new MBackup();
        backupOps[i]=backupOp;
      }
      backupOp.sendBackup(operation,target,true,distance,reqBackup);
    }
    for (int i=0; i < backupCount; i++) {
      MBackup backupOp=backupOps[i];
      backupOp.getResultAsBoolean();
      backupOp.reset();
    }
  }
}
