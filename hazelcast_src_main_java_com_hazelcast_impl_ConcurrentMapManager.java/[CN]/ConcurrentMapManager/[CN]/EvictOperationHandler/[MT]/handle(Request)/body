{
  CMap cmap=getOrCreateMap(request.name);
  if (cmap.isNotLocked(request)) {
    Record record=cmap.getRecord(request.key);
    if (record != null && record.isActive() && cmap.loader != null && cmap.writeDelayMillis > 0 && record.isValid() && record.isDirty()) {
      record.setDirty(false);
      request.value=record.getValue();
      executeAsync(request);
    }
 else {
      doOperation(request);
      returnResponse(request);
    }
  }
 else {
    request.response=OBJECT_REDO;
    returnResponse(request);
  }
}
