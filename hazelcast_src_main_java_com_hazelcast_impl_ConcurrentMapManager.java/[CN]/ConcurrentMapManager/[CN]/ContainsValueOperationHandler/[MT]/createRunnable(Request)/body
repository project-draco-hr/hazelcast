{
  final CMap cmap=getOrCreateMap(request.name);
  return new Runnable(){
    public void run(){
      request.response=cmap.getMapIndexService().containsValue(request.value);
      enqueueAndReturn(new Processable(){
        public void process(){
          returnResponse(request);
        }
      }
);
    }
  }
;
}
