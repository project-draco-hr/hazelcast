{
  final CMap cmap=getMap(request.name);
  return new Runnable(){
    public void run(){
      request.response=node.queryService.containsValue(cmap.name,request.value);
      enqueueAndReturn(new Processable(){
        public void process(){
          returnResponse(request);
        }
      }
);
    }
  }
;
}
