{
  if (record.isActive()) {
    node.queryService.updateIndex(name,null,null,record,record.value.hashCode());
    record.markRemoved();
    setRemovedRecords.add(record);
    Block ownerBlock=blocks[record.blockId];
    if (thisAddress.equals(ownerBlock.getRealOwner())) {
      ownedEntryCount--;
    }
  }
}
