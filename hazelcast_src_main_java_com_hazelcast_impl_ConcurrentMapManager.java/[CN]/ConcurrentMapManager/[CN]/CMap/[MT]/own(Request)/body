{
  if (req.value == null) {
    req.value=new Data();
  }
  Record record=getRecord(req.key);
  if (record == null) {
    record=toRecord(req);
    updateIndexes(true,req,record);
  }
  record.setVersion(req.version);
  logger.log(Level.FINEST,req.name + " own lockCount:" + record.getLockCount()+ ", threadId:"+ record.getLockThreadId());
}
