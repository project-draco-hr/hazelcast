{
  threadNamePrefix=s;
  this.threadCount=threadCount;
  workers=new Worker[threadCount];
  for (int i=0; i < threadCount; i++) {
    workers[i]=new Worker();
  }
  for (int i=0; i < threadCount; i++) {
    Worker worker=workers[i];
    Thread t=new Thread(worker,threadNamePrefix + "_" + (i + 1));
    t.start();
  }
}
