{
  try {
    DistributedTask task=new DistributedTask(callable);
    factory.getExecutorService().execute(task);
    try {
      return task.get(1,TimeUnit.SECONDS);
    }
 catch (    Throwable e) {
      logger.log(Level.FINEST,e.getMessage(),e);
      return null;
    }
  }
 catch (  Throwable e) {
    return null;
  }
}
