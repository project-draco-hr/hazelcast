{
  StringBuilder sb=new StringBuilder();
  sb.append("SlowOperationLog{operation='").append(operation).append("', stackTrace='").append(stackTrace).append("', totalInvocations=").append(totalInvocations).append(", invocations='");
  String prefix="";
  for (  Invocation log : invocations.values()) {
    sb.append(prefix).append(log.toString());
    prefix=", ";
  }
  sb.append("'}");
  return sb.toString();
}
