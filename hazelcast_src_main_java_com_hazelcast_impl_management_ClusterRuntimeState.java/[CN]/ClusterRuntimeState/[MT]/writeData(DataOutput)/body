{
  super.writeData(out);
  out.writeInt(localMemberIndex);
  out.writeInt(lockTotalNum);
  for (  ConnectionInfo info : connectionInfos) {
    info.writeData(out);
  }
  int lockSize=lockInfos.size();
  out.writeInt(lockSize);
  for (  LockInfo lockInfo : lockInfos) {
    lockInfo.writeData(out);
  }
}
