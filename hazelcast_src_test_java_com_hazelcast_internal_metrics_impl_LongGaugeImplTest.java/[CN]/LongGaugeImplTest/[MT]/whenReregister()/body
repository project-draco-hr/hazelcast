{
  metricsRegistry.register(this,"foo",new LongProbeFunction(){
    @Override public long get(    Object o) throws Exception {
      return 10;
    }
  }
);
  LongGauge gauge=metricsRegistry.newLongGauge("foo");
  gauge.read();
  metricsRegistry.register(this,"foo",new LongProbeFunction(){
    @Override public long get(    Object o) throws Exception {
      return 11;
    }
  }
);
  assertEquals(11,gauge.read());
}
