{
  if (itemMap == null) {
    if (itemList != null && !itemList.isEmpty()) {
      itemMap=new HashMap<Long,CollectionItem>(itemList.size());
      for (      CollectionItem item : itemList) {
        itemMap.put(item.getItemId(),item);
      }
    }
 else {
      itemMap=new HashMap<Long,CollectionItem>(1000);
    }
  }
  return itemMap;
}
