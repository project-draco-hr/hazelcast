{
  ensureOpen();
  try {
    CacheSizeRequest request=new CacheSizeRequest(nameWithPrefix,cacheConfig.getInMemoryFormat());
    Integer result=invoke(request);
    if (result == null) {
      return 0;
    }
    return result;
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrowAllowedTypeFirst(t,CacheException.class);
  }
}
