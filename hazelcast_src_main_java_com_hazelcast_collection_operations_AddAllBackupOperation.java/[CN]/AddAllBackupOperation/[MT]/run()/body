{
  Collection<CollectionRecord> coll=getOrCreateCollectionWrapper().getCollection();
  if (index == -1) {
    response=coll.addAll(recordList);
  }
 else {
    List list=(List)coll;
    try {
      response=list.addAll(index,recordList);
    }
 catch (    IndexOutOfBoundsException e) {
      response=e;
    }
  }
}
