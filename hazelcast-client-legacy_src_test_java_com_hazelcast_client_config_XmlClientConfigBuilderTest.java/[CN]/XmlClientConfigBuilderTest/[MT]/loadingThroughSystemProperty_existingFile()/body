{
  String xml="<hazelcast-client xmlns=\"http://www.hazelcast.com/schema/client-config\">\n" + "    <group>\n" + "        <name>foobar</name>\n"+ "        <password>dev-pass</password>\n"+ "    </group>"+ "</hazelcast-client>";
  File file=File.createTempFile("foo","bar");
  file.deleteOnExit();
  PrintWriter writer=new PrintWriter(file,"UTF-8");
  writer.println(xml);
  writer.close();
  System.setProperty("hazelcast.client.config",file.getAbsolutePath());
  XmlClientConfigBuilder configBuilder=new XmlClientConfigBuilder();
  ClientConfig config=configBuilder.build();
  assertEquals("foobar",config.getGroupConfig().getName());
}
