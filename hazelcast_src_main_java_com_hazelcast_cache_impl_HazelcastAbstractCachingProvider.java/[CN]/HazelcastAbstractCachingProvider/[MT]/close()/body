{
synchronized (cacheManagers) {
    for (    Map<URI,CacheManager> cacheManagersByURI : cacheManagers.values()) {
      for (      CacheManager cacheManager : cacheManagersByURI.values()) {
        cacheManager.close();
      }
    }
  }
  this.cacheManagers.clear();
  shutdownHazelcastInstance();
}
