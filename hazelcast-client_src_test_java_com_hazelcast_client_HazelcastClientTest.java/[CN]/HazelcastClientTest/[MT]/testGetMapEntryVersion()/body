{
  IMap<String,String> map=getHazelcastClient().getMap("testGetMapEntryVersion");
  map.put("Hello","World");
  MapEntry me=map.getMapEntry("Hello");
  assertEquals(0,me.getVersion());
  map.put("Hello","1");
  map.put("Hello","2");
  map.put("Hello","3");
  me=map.getMapEntry("Hello");
  assertEquals(3,me.getVersion());
}
