{
  HazelcastInstance h=Hazelcast.newHazelcastInstance(null);
  hClient=getHazelcastClient(h);
  IMap<Integer,Integer> map=hClient.getMap("default");
  for (int i=0; i < 100; i++) {
    assertNull(map.put(i,i));
    assertEquals(i,map.get(i));
  }
  map.clear();
  for (int i=0; i < 100; i++) {
    assertNull(map.get(i));
  }
}
