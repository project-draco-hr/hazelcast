{
  IMap<String,String> map=getHazelcastClient().getMap("testGetMapEntryHits");
  map.put("Hello","World");
  MapEntry me=map.getMapEntry("Hello");
  assertEquals(0,me.getHits());
  map.get("Hello");
  map.get("Hello");
  map.get("Hello");
  me=map.getMapEntry("Hello");
  assertEquals(3,me.getHits());
}
