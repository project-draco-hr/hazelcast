{
  final NodeEngine nodeEngine=getNodeEngine();
  int partitionId=nodeEngine.getPartitionService().getPartitionId(key);
  RemoveOperation operation=new RemoveOperation(name,key);
  operation.setThreadId(ThreadContext.getThreadId());
  try {
    Invocation invocation=nodeEngine.getOperationService().createInvocationBuilder(SERVICE_NAME,operation,partitionId).setAsync(true).build();
    return invocation.invoke();
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
