{
  final NodeEngine nodeEngine=getNodeEngine();
  try {
    Map<Integer,Object> results=nodeEngine.getOperationService().invokeOnAllPartitions(SERVICE_NAME,new BinaryOperationFactory(new MapSizeOperation(name),nodeEngine));
    int total=0;
    for (    Object result : results.values()) {
      Integer size=(Integer)getService().toObject(result);
      total+=size;
    }
    return total;
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
