{
  final NodeEngine nodeEngine=getNodeEngine();
  try {
    Map<Integer,Object> results=nodeEngine.getOperationService().invokeOnAllPartitions(SERVICE_NAME,new IsEmptyOperationFactory(name));
    for (    Object result : results.values()) {
      if (!(Boolean)getService().toObject(result)) {
        return false;
      }
    }
    return true;
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
