{
  try {
    MapIsEmptyOperation mapIsEmptyOperation=new MapIsEmptyOperation(name);
    Map<Integer,Object> results=nodeEngine.getOperationService().invokeOnAllPartitions(MAP_SERVICE_NAME,mapIsEmptyOperation,false);
    for (    Object result : results.values()) {
      if (!(Boolean)nodeEngine.toObject(result))       return false;
    }
    return true;
  }
 catch (  Throwable throwable) {
    throw new HazelcastException(throwable);
  }
}
