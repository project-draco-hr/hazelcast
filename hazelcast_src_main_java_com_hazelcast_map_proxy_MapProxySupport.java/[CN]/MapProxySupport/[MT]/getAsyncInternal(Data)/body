{
  int partitionId=nodeEngine.getPartitionId(key);
  GetOperation operation=new GetOperation(name,key);
  operation.setThreadId(ThreadContext.get().getThreadId());
  try {
    Invocation invocation=nodeEngine.getOperationService().createInvocationBuilder(MAP_SERVICE_NAME,operation,partitionId).build();
    Future invoke=invocation.invoke();
    return invoke;
  }
 catch (  Throwable throwable) {
    throw new HazelcastException(throwable);
  }
}
