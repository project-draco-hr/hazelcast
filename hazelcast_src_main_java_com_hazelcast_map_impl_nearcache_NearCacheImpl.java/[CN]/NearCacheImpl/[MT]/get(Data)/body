{
  fireCacheExpiration();
  NearCacheRecord record=cache.get(key);
  if (record != null) {
    if (record.isExpired(maxIdleMillis,timeToLiveMillis)) {
      cache.remove(key);
      updateSizeEstimator(-calculateCost(record));
      stats.incrementMisses();
      stats.incrementExpirations();
      return null;
    }
    stats.incrementHits();
    record.access();
    return record.getValue();
  }
 else {
    stats.incrementMisses();
    return null;
  }
}
