{
  Operation op=(Operation)backupAwareOp;
  Backup backup=new Backup(backupOpData,op.getCallerAddress(),replicaVersions,respondBack);
  backup.setPartitionId(op.getPartitionId()).setReplicaIndex(replicaIndex).setCallerUuid(nodeEngine.getLocalMember().getUuid());
  setCallId(backup,op.getCallId());
  return backup;
}
