{
  if (file == null) {
    throw new NullPointerException();
  }
  PrintWriter out=null;
  try {
    out=new PrintWriter(new BufferedWriter(new FileWriter(file,true)));
    if (s == null) {
      out.println("null");
    }
 else {
      out.println(s);
    }
    out.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    IOUtil.closeResource(out);
  }
}
