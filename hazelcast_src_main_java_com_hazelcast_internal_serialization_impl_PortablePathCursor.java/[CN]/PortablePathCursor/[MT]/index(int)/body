{
  this.index=0;
  this.offset=0;
  this.nextSplit=ExtractorHelper.indexOf(path,'.',0);
  this.token=null;
  for (int i=1; i <= indexToNavigateTo; i++) {
    if (!advanceToNextToken()) {
      throw new IndexOutOfBoundsException("Index out of bound " + indexToNavigateTo + " in "+ path);
    }
  }
}
