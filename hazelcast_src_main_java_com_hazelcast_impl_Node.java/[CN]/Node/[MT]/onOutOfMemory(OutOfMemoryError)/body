{
  try {
    if (serverSocketChannel != null) {
      try {
        serverSocketChannel.close();
        connectionManager.shutdown();
        shutdown(true,false);
      }
 catch (      Throwable ignored) {
      }
    }
  }
  finally {
    active=false;
    outOfMemory=true;
    logger.log(Level.SEVERE,e.getMessage(),e);
  }
}
