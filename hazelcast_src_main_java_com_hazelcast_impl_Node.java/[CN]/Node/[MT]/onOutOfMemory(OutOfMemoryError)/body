{
  try {
    if (connectionManager != null) {
      connectionManager.shutdown();
      shutdown(true,false);
    }
  }
 catch (  Throwable ignored) {
    logger.log(Level.FINEST,ignored.getMessage(),ignored);
  }
 finally {
    outOfMemory=true;
    logger.log(Level.SEVERE,e.getMessage(),e);
  }
}
