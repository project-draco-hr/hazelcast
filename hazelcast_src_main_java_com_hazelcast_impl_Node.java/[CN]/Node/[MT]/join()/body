{
  try {
    Joiner joiner=getJoiner();
    if (joiner == null) {
      logger.log(Level.WARNING,"No join method is enabled! Starting standalone.");
      setAsMaster();
    }
 else {
      joiner.join(joined);
    }
  }
 catch (  Exception e) {
    logger.log(Level.WARNING,e.getMessage());
    factory.lifecycleService.restart();
  }
}
