{
  if (active && !completelyShutdown) {
    try {
      completelyShutdown=true;
      if (logger != null) {
        logger.log(Level.INFO,"Hazelcast ShutdownHook is shutting down!");
      }
      shutdown();
    }
 catch (    Exception e) {
      if (logger != null) {
        logger.log(Level.WARNING,"Hazelcast shutdownhook exception:",e);
      }
    }
  }
}
