{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  Vertex vertex=(Vertex)o;
  if (owner != null ? !(owner.getLockAddress().equals(vertex.owner.getLockAddress()) && owner.getLockThreadId() == vertex.owner.getLockThreadId()) : vertex.owner != null)   return false;
  return true;
}
