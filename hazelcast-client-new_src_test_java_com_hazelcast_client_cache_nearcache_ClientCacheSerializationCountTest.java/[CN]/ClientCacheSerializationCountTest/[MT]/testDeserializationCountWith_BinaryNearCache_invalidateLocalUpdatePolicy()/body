{
  NearCacheConfig nearCacheConfig=createNearCacheConfig(InMemoryFormat.BINARY,NearCacheConfig.LocalUpdatePolicy.INVALIDATE);
  prepareCache(nearCacheConfig);
  String key=randomString();
  SerializationCountingData value=new SerializationCountingData();
  cache.put(key,value);
  assertAndReset(1,0);
  cache.get(key);
  assertAndReset(0,1);
  cache.get(key);
  assertAndReset(0,1);
}
