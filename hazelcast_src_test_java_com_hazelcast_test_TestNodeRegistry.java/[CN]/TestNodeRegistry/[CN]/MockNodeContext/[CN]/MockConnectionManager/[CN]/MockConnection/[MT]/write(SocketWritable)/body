{
  final Packet packet=(Packet)socketWritable;
  if (nodeEngine.getNode().isActive()) {
    packet.setConn(thisConnection);
    MemberImpl member=nodeEngine.getClusterService().getMember(thisAddress);
    if (member != null) {
      member.didRead();
    }
    nodeEngine.handlePacket(packet);
    return true;
  }
  return false;
}
