{
  T service=serviceManager.getService(serviceName);
  if (service == null) {
    if (isRunning()) {
      throw new HazelcastException("Service with name '" + serviceName + "' not found!");
    }
 else {
      throw new RetryableHazelcastException("HazelcastInstance[" + getThisAddress() + "] is not active!");
    }
  }
  return service;
}
