{
  ILogger logger=nodeEngine.getLogger(getClass());
  for (  Future future : futures) {
    try {
      future.get(AWAIT_COMPLETION_TIMEOUT_SECONDS,TimeUnit.SECONDS);
    }
 catch (    TimeoutException e) {
      logger.finest(e);
    }
catch (    Exception e) {
      logger.warning(e);
    }
  }
}
