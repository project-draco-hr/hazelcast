{
  final CountDownLatch latch=new CountDownLatch(1);
  ioSelector.addTask(new Runnable(){
    public void run(){
      createWriter(protocol);
      latch.countDown();
    }
  }
);
  ioSelector.wakeup();
  try {
    latch.await(3,TimeUnit.SECONDS);
  }
 catch (  InterruptedException ignored) {
  }
}
