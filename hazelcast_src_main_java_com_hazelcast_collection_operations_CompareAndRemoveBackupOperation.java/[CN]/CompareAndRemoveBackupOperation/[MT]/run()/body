{
  CollectionWrapper wrapper=getCollectionWrapper();
  if (wrapper == null) {
    response=false;
    return;
  }
  Iterator<CollectionRecord> iter=wrapper.getCollection().iterator();
  while (iter.hasNext()) {
    CollectionRecord record=iter.next();
    if (idSet.contains(record.getRecordId())) {
      iter.remove();
    }
  }
  response=true;
}
