{
  Collection<CollectionRecord> coll=getCollection();
  if (coll == null) {
    response=false;
    return;
  }
  Iterator<CollectionRecord> iter=coll.iterator();
  while (iter.hasNext()) {
    CollectionRecord record=iter.next();
    if (idSet.contains(record.getRecordId())) {
      iter.remove();
    }
  }
  response=true;
}
