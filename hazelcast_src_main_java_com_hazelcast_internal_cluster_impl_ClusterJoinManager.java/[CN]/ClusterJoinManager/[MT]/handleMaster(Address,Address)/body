{
  if (node.joined()) {
    if (logger.isFinestEnabled()) {
      logger.finest(format("Ignoring master response %s from %s, this node is already joined",masterAddress,callerAddress));
    }
    return;
  }
  if (node.getThisAddress().equals(masterAddress)) {
    if (node.isMaster()) {
      logger.finest(format("Ignoring master response %s from %s, this node is already master",masterAddress,callerAddress));
    }
 else {
      node.setAsMaster();
    }
    return;
  }
  handleMasterResponse(masterAddress,callerAddress);
}
