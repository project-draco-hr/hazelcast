{
  checkNotNull(task,"task should not be null");
  final String uuid=getUUID();
  int partitionId=randomPartitionId();
  final PartitionTargetCallableRequest request=new PartitionTargetCallableRequest(name,uuid,task,partitionId);
  final ICompletableFuture<T> f=invokeOnPartitionOwner(request,partitionId);
  f.andThen(callback);
}
