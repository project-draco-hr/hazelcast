{
  if (data == null || data.size() == 0)   return null;
  Object result=null;
  try {
    bbis.reset();
    bufferProvider.setData(data);
    byte typeId=bbis.readByte();
    result=typeSerizalizers[typeId].read(bbis,data);
    if (purgeData)     data.setNoData();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return result;
}
