{
  if (data == null || data.size() == 0)   return null;
  try {
    bbis.reset();
    bufferProvider.setData(data);
    byte typeId=bbis.readByte();
    Object result=typeSerizalizers[typeId].read(bbis);
    if (purgeData)     data.setNoData();
    return result;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
