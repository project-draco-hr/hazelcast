{
  String className=bbis.readShortUTF();
  try {
    DataSerializable ds=(DataSerializable)Class.forName(className).newInstance();
    ds.readData(bbis);
    return ds;
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new IOException("Problem reading DataSerializable class : " + className + ", exception: "+ e);
  }
}
