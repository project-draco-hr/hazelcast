{
  int charCount=readInt();
  char[] charBuffer=new char[charCount];
  byte b;
  for (int i=0; i < charCount; i++) {
    b=dataInput.readByte();
    if (b < 0) {
      charBuffer[i]=Bits.readUtf8Char(dataInput,b);
    }
 else {
      charBuffer[i]=(char)b;
    }
  }
  return new String(charBuffer,0,charCount);
}
