{
  Session session=null;
  Transaction txn=null;
  try {
    session=sf.openSession();
    txn=session.beginTransaction();
    DummyEntity entityToDelete=(DummyEntity)session.get(DummyEntity.class,id);
    session.delete(entityToDelete);
    txn.commit();
  }
 catch (  Exception e) {
    txn.rollback();
    e.printStackTrace();
    throw e;
  }
 finally {
    session.close();
  }
}
