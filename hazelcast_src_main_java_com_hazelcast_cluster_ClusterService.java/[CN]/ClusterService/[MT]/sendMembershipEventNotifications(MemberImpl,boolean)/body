{
  MembershipEvent membershipEvent=new MembershipEvent(getClusterProxy(),member,(added ? MembershipEvent.MEMBER_ADDED : MembershipEvent.MEMBER_REMOVED));
  final EventService eventService=nodeEngine.getEventService();
  Collection<EventRegistration> registrations=eventService.getRegistrations(SERVICE_NAME,SERVICE_NAME);
  eventService.publishEvent(SERVICE_NAME,registrations,membershipEvent);
}
