{
  packetQueue.clear();
  processableQueue.clear();
  try {
    final CountDownLatch l=new CountDownLatch(1);
    processableQueue.offer(new Processable(){
      public void process(){
        running=false;
        l.countDown();
      }
    }
);
    l.await();
  }
 catch (  InterruptedException ignored) {
  }
}
