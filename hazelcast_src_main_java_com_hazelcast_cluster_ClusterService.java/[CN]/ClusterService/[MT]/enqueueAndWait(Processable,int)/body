{
  try {
    final CountDownLatch l=new CountDownLatch(1);
    enqueueAndReturn(new Processable(){
      public void process(){
        processable.process();
        l.countDown();
      }
    }
);
    return l.await(seconds,TimeUnit.SECONDS);
  }
 catch (  InterruptedException ignored) {
  }
  return false;
}
