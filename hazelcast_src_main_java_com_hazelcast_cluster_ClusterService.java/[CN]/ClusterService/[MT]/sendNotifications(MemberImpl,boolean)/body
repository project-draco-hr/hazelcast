{
  if (listeners.size() > 0) {
    MembershipEvent membershipEvent=new MembershipEvent(this,member,(added ? MembershipEvent.MEMBER_ADDED : MembershipEvent.MEMBER_REMOVED));
    for (    MembershipListener listener : listeners) {
      if (added) {
        listener.memberAdded(membershipEvent);
      }
 else {
        listener.memberRemoved(membershipEvent);
      }
    }
  }
}
