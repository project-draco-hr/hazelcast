{
  if (isDone() || cancelled) {
    return false;
  }
  waitForRequestToBeSend();
  final Future f=invokeCancelRequest(mayInterruptIfRunning);
  try {
    final Boolean b=context.getSerializationService().toObject(f.get());
    if (b != null && b) {
      setError(new CancellationException());
      cancelled=true;
      return true;
    }
    return false;
  }
 catch (  Exception e) {
    throw rethrow(e);
  }
 finally {
    setDone();
  }
}
