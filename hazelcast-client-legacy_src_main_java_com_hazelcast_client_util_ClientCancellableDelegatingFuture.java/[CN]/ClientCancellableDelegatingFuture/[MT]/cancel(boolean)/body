{
  if (isDone() || cancelled) {
    return false;
  }
  Boolean b=false;
  try {
    b=invokeCancelRequest(mayInterruptIfRunning);
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
  }
  try {
    if (b != null && b) {
      setError(new CancellationException());
      cancelled=true;
      return true;
    }
    return false;
  }
 catch (  Exception e) {
    throw rethrow(e);
  }
 finally {
    setDone();
  }
}
