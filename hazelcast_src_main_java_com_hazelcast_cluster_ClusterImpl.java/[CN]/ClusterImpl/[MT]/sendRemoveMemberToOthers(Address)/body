{
  for (  MemberImpl member : getMemberList()) {
    Address address=member.getAddress();
    if (!thisAddress.equals(address) && !address.equals(deadAddress)) {
      node.nodeService.createSingleInvocation(SERVICE_NAME,new MemberRemover(deadAddress),-1).setTarget(address).setTryCount(1).build().invoke();
    }
  }
}
