{
  Record record=recordStore.getRecords().get(dataKey);
  if (record != null) {
    if (record.getState().isDirty()) {
      MapStore store=recordStore.getMapInfo().getStore();
      if (store != null) {
        store.store(dataKey,record.getValue());
      }
      record.getState().resetStoreTime();
    }
  }
}
