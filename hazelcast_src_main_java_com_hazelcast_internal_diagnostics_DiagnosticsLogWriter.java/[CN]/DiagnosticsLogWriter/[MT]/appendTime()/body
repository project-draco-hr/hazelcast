{
  int hour=calendar.get(HOUR_OF_DAY);
  if (hour < 10) {
    write('0');
  }
  write(hour);
  write(':');
  int minute=calendar.get(MINUTE);
  if (minute < 10) {
    write('0');
  }
  write(minute);
  write(':');
  int second=calendar.get(SECOND);
  if (second < 10) {
    write('0');
  }
  write(second);
}
