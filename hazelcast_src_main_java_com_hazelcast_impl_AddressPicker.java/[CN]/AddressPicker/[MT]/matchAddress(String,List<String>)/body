{
  final int[] ip=new int[4];
  int i=0;
  final StringTokenizer st=new StringTokenizer(address,".");
  while (st.hasMoreTokens()) {
    ip[i++]=Integer.parseInt(st.nextToken());
  }
  for (  final String ipmask : interfaces) {
    if (matchAddress(ipmask,ip)) {
      return true;
    }
  }
  return false;
}
