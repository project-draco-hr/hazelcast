{
  MultiMapWrapper wrapper=getCollectionWrapper();
  response=false;
  if (wrapper == null) {
    return;
  }
  Collection<MultiMapRecord> coll=wrapper.getCollection();
  Iterator<MultiMapRecord> iter=coll.iterator();
  while (iter.hasNext()) {
    if (iter.next().getRecordId() == recordId) {
      iter.remove();
      response=true;
      if (coll.isEmpty()) {
        remove();
      }
      break;
    }
  }
}
