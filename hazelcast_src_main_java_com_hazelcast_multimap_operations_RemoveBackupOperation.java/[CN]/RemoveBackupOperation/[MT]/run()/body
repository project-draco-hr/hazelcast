{
  CollectionWrapper wrapper=getCollectionWrapper();
  response=false;
  if (wrapper == null) {
    return;
  }
  Collection<CollectionRecord> coll=wrapper.getCollection();
  Iterator<CollectionRecord> iter=coll.iterator();
  while (iter.hasNext()) {
    if (iter.next().getRecordId() == recordId) {
      iter.remove();
      response=true;
      if (coll.isEmpty()) {
        removeCollection();
      }
      break;
    }
  }
}
