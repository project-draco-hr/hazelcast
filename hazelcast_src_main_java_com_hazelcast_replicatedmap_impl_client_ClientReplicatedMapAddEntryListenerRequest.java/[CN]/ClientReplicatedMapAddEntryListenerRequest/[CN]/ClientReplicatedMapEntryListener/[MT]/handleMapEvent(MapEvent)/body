{
  if (endpoint.isAlive()) {
    final EntryEventType type=event.getEventType();
    final String uuid=event.getMember().getUuid();
    ReplicatedMapPortableEntryEvent portableEntryEvent=new ReplicatedMapPortableEntryEvent(null,null,null,type,uuid,event.getNumberOfEntriesAffected());
    endpoint.sendEvent(null,portableEntryEvent,getCallId());
  }
}
