{
  final String name="testPoll";
  int count=100;
  HazelcastInstance instance=createHazelcastInstance();
  IQueue<String> queue=instance.getQueue(name);
  for (int i=0; i < count; i++) {
    queue.offer("item" + i);
  }
  assertEquals(100,queue.size());
  queue.poll();
  queue.poll();
  queue.poll();
  queue.poll();
  assertEquals(96,queue.size());
}
