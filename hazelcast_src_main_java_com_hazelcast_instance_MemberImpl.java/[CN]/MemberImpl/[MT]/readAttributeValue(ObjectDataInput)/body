{
  byte type=in.readByte();
switch (type) {
case TYPE_BOOLEAN:
    return in.readBoolean();
case TYPE_BYTE:
  return in.readByte();
case TYPE_SHORT:
return in.readShort();
case TYPE_INTEGER:
return in.readInt();
case TYPE_LONG:
return in.readLong();
case TYPE_FLOAT:
return in.readFloat();
case TYPE_DOUBLE:
return in.readDouble();
case TYPE_UTF:
return in.readUTF();
}
throw new IllegalStateException("Illegal type id found");
}
