{
  Class<?> type=value.getClass();
  if (type.equals(Boolean.class) || type.equals(Byte.class) || type.equals(Short.class)|| type.equals(Integer.class)|| type.equals(Long.class)|| type.equals(Float.class)|| type.equals(Double.class)|| type.equals(String.class)) {
    return;
  }
  throw new IllegalArgumentException("Attribute values can only have a value of types [" + "boolean, byte, short, int, long, float, double, String");
}
