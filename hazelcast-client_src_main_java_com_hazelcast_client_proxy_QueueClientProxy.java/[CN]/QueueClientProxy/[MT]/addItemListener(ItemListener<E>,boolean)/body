{
  check(listener);
  Protocol request=proxyHelper.createProtocol(Command.QLISTEN,new String[]{getName(),String.valueOf(includeValue)},null);
  ListenerThread thread=proxyHelper.createAListenerThread("hz.client.qListener." + threadCounter.incrementAndGet(),client,request,new ItemEventLRH<E>(listener,includeValue,this));
  listenerMap.put(listener,thread);
  thread.start();
}
