{
  if (target == null) {
    return;
  }
  try {
    node.nodeEngine.getOperationService().send(new HeartbeatOperation(),target);
  }
 catch (  Exception e) {
    if (logger.isFinestEnabled()) {
      logger.finest("Error while sending heartbeat -> " + e.getClass().getName() + "["+ e.getMessage()+ "]");
    }
  }
}
