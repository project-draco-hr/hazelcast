{
  if (size < maxPoolSize) {
    try {
      if (lock.tryLock(250,TimeUnit.MILLISECONDS)) {
        try {
          if (size < maxPoolSize && queueSize() > 0) {
            size++;
            cachedExecutor.execute(new Worker());
          }
        }
  finally {
          lock.unlock();
        }
      }
    }
 catch (    InterruptedException ignored) {
    }
  }
}
