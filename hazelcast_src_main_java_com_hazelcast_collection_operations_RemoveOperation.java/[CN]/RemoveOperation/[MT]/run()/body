{
  begin=Clock.currentTimeMillis();
  Collection<CollectionRecord> coll=getCollection();
  if (coll == null) {
    response=false;
    return;
  }
  CollectionRecord record=new CollectionRecord(isBinary() ? value : toObject(value));
  response=coll.remove(record);
  if (coll.isEmpty()) {
    removeCollection();
  }
}
