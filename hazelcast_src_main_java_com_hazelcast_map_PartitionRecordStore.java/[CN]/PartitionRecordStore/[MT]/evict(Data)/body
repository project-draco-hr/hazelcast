{
  Record record=records.get(dataKey);
  Object oldValue=null;
  try {
    if (record != null) {
      flush(dataKey);
      mapService.interceptRemove(name,record.getValue());
      oldValue=record.getValue();
      records.remove(dataKey);
      removeIndex(dataKey);
    }
  }
 catch (  Exception e) {
    ExceptionUtil.rethrow(e);
  }
  return oldValue;
}
