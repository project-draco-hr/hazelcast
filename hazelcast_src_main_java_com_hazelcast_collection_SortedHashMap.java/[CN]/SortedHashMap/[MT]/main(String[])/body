{
  SortedHashMap m=new SortedHashMap(100,SortedHashMap.OrderingType.LRU);
  for (int i=0; i < 10; i++) {
    m.put(i,"value" + i);
  }
  m.get(0);
  m.get(4);
  SortedHashMap.moveToTop(m,7);
  Object[] values=m.values().toArray();
  for (  Object o : values) {
    System.out.println("vv " + o);
  }
  m.clear();
  values=m.values().toArray();
  for (  Object o : values) {
    System.out.println("vv now " + o);
  }
}
