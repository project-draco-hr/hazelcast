{
  Config config=getConfig(backupCount);
  HazelcastInstance hz=factory.newHazelcastInstance(config);
  fill(hz);
  assertSizeAndData();
  int size=1;
  while (size < (nodeCount + 1)) {
    startNodes(config,backupCount + 1);
    size+=(backupCount + 1);
    assertSizeAndData();
    terminateNodes(backupCount);
    size-=backupCount;
    assertSizeAndData();
  }
}
