{
  final int max=100;
  final IMap map=client.getMap(randomString());
  final Map expected=new HashMap();
  for (int i=0; i < max; i++) {
    expected.put(i,i);
  }
  map.putAll(expected);
  for (  Object key : expected.keySet()) {
    Object value=map.get(key);
    Object expectedValue=expected.get(key);
    assertEquals(expectedValue,value);
  }
}
