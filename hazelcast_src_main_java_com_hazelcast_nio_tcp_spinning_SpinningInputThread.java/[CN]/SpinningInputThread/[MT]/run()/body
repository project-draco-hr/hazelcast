{
  for (; ; ) {
    ConnectionHandlers handlers=connectionHandlers;
    if (handlers == SHUTDOWN) {
      return;
    }
    for (    SpinningReadHandler handler : handlers.readHandlers) {
      try {
        handler.read();
      }
 catch (      Throwable t) {
        handler.onFailure(t);
      }
    }
  }
}
