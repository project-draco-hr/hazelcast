{
  if (isInsideAws) {
    return address;
  }
  if (address == null) {
    return null;
  }
  String publicAddress=getLookupTable().get(address.getHost());
  if (publicAddress != null) {
    return constructPrivateAddress(publicAddress,address);
  }
  refresh();
  publicAddress=getLookupTable().get(address.getHost());
  if (publicAddress != null) {
    return constructPrivateAddress(publicAddress,address);
  }
  return null;
}
