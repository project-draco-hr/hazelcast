{
  StringBuilder sb=new StringBuilder();
  char[] chars=element.toCharArray();
  boolean upper=true;
  for (int i=0; i < chars.length; i++) {
    char c=chars[i];
    if (c == '_' || c == '-' || c == '.') {
      upper=true;
    }
 else {
      if (upper) {
        sb.append(Character.toUpperCase(c));
        upper=false;
      }
 else {
        sb.append(c);
      }
    }
  }
  return sb.toString();
}
