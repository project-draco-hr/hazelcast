{
  String getterString=type + " " + name+ ";\n";
  if (!isPrimitive) {
    getterString+=name + " = null ;\n";
  }
  if (isNullable) {
    getterString+=" boolean " + name + "_isNull = clientMessage.getBoolean();\n";
    getterString+=" if(!" + name + "_isNull) { \n";
  }
  getterString+=innerGetterString;
  if (isNullable) {
    getterString+=" \n}\n";
  }
  return getterString;
}
