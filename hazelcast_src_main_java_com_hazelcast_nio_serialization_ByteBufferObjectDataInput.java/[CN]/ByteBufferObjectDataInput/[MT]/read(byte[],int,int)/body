{
  if (b == null) {
    throw new NullPointerException();
  }
  if (len <= 0) {
    return 0;
  }
  final int pos=buffer.position();
  final int size=buffer.capacity();
  if (pos >= size) {
    return -1;
  }
  if (pos + len > size) {
    len=size - pos;
  }
  buffer.get(b,off,len);
  return len;
}
