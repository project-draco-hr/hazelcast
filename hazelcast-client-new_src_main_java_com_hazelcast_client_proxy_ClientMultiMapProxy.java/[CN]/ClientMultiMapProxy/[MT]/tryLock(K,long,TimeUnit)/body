{
  checkNotNull(key,NULL_KEY_IS_NOT_ALLOWED);
  final Data keyData=toData(key);
  ClientMessage request=MultiMapTryLockCodec.encodeRequest(name,keyData,ThreadUtil.getThreadId(),timeunit.toMillis(time));
  ClientMessage response=invoke(request,keyData);
  MultiMapTryLockCodec.ResponseParameters resultParameters=MultiMapTryLockCodec.decodeResponse(response);
  return resultParameters.response;
}
