{
  V value=null;
  V oldValue=null;
  V mergingValue=null;
  if (includeValue) {
    value=toObject(valueData);
    oldValue=toObject(oldValueData);
    mergingValue=toObject(mergingValueData);
  }
  K key=toObject(keyData);
  return new EntryEvent<K,V>(name,member,eventType,key,oldValue,value,mergingValue);
}
