{
  beforeProcess();
  ClientEndpoint endpoint=getEndpoint();
  Operation op=prepareOperation();
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=operationService.createInvocationBuilder(getServiceName(),op,getPartition()).setReplicaIndex(getReplicaIndex()).setTryCount(TRY_COUNT).setResultDeserialized(false);
  ICompletableFuture future=builder.invoke();
  future.andThen(this);
}
