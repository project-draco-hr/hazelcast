{
  beforeProcess();
  ClientEndpoint endpoint=getEndpoint();
  Operation op=prepareOperation();
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=operationService.createInvocationBuilder(getServiceName(),op,getPartition()).setReplicaIndex(getReplicaIndex()).setExecutionCallback(this).setResultDeserialized(false);
  builder.invoke();
}
