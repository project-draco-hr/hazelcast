{
  fireTtlCleanup();
  CacheRecord record=cache.get(key);
  if (record != null) {
    record.access();
    if (record.expired()) {
      cache.remove(key);
      updateSizeEstimator(-calculateCost(record));
      return null;
    }
    return record.value;
  }
 else {
    stats.incrementMisses();
    return null;
  }
}
