{
  Iterator<Map.Entry<String,Permit>> iter=permitMap.entrySet().iterator();
  while (iter.hasNext()) {
    Permit permit=iter.next().getValue();
    if (permit.getPartitionId() == partitionId && (copyBack == -1 || permit.getConfig().getTotalBackupCount() < copyBack)) {
      iter.remove();
    }
  }
}
