{
  int entityCount=10;
  insertDummyEntities(entityCount,0);
  sleep(1);
  executeQuery(sf);
  executeUpdateQuery(sf,"delete from DummyEntity");
  executeQuery(sf);
  executeQuery(sf);
  assertEquals(1,sf.getStatistics().getQueryCacheHitCount());
  assertEquals(2,sf.getStatistics().getQueryCacheMissCount());
}
