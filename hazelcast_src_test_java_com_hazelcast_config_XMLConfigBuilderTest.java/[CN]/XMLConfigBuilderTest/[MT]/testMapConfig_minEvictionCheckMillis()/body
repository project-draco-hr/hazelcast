{
  String xml="<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "<map name=\"mymap\">" + "<min-eviction-check-millis>123456789</min-eviction-check-millis>"+ "</map>"+ "</hazelcast>";
  final Config config=buildConfig(xml);
  final MapConfig mapConfig=config.getMapConfig("mymap");
  assertEquals(123456789L,mapConfig.getMinEvictionCheckMillis());
}
