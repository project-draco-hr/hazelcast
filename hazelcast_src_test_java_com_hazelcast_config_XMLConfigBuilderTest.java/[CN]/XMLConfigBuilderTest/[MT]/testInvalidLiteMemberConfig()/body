{
  String xml="<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "    <lite-member enabled=\"dummytext\"/>\n" + "</hazelcast>";
  ByteArrayInputStream bis=new ByteArrayInputStream(xml.getBytes());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder(bis);
  configBuilder.build();
}
