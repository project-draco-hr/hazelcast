{
  Config config=buildConfig("<hazelcast>\n" + "    <network>\n" + "        <port port-count=\"200\">5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertEquals(200,config.getNetworkConfig().getPortCount());
  config=buildConfig("<hazelcast>\n" + "    <network>\n" + "        <port>5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertEquals(100,config.getNetworkConfig().getPortCount());
}
