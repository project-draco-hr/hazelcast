{
  Config config=buildConfig(HAZELCAST_START_TAG + "    <network>\n" + "        <port auto-increment=\"false\">5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertFalse(config.getNetworkConfig().isPortAutoIncrement());
  config=buildConfig(HAZELCAST_START_TAG + "    <network>\n" + "        <port>5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertTrue(config.getNetworkConfig().isPortAutoIncrement());
}
