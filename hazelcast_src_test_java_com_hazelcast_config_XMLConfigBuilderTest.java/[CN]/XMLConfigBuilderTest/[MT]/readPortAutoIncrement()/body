{
  Config config=buildConfig("<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "    <network>\n" + "        <port auto-increment=\"false\">5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertFalse(config.getNetworkConfig().isPortAutoIncrement());
  config=buildConfig("<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "    <network>\n" + "        <port>5701</port>\n"+ "    </network>\n"+ "</hazelcast>");
  assertTrue(config.getNetworkConfig().isPortAutoIncrement());
}
