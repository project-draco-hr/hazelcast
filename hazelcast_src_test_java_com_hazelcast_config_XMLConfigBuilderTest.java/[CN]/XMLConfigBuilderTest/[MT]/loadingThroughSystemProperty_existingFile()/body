{
  String xml="<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "    <group>\n" + "        <name>foobar</name>\n"+ "        <password>dev-pass</password>\n"+ "    </group>"+ "</hazelcast>";
  File file=File.createTempFile("foo","bar");
  file.deleteOnExit();
  PrintWriter writer=new PrintWriter(file,"UTF-8");
  writer.println(xml);
  writer.close();
  System.setProperty("hazelcast.config",file.getAbsolutePath());
  XmlConfigBuilder configBuilder=new XmlConfigBuilder();
  Config config=configBuilder.build();
  assertEquals("foobar",config.getGroupConfig().getName());
}
