{
  String xml="<hazelcast>\n" + "<map name=\"mymap\">" + "<check-if-evictable-after-millis>123456789</check-if-evictable-after-millis>"+ "</map>"+ "</hazelcast>";
  final Config config=buildConfig(xml);
  final MapConfig mapConfig=config.getMapConfig("mymap");
  assertEquals(123456789L,mapConfig.getCheckIfEvictableAfterMillis());
}
