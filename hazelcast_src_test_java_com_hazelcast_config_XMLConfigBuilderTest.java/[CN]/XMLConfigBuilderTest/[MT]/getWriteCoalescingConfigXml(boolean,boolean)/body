{
  String writeCoalescingConfigPart=useDefault ? "" : "<write-coalescing>" + String.valueOf(value) + "</write-coalescing>";
  return "<hazelcast xmlns=\"http://www.hazelcast.com/schema/config\">\n" + "<map name=\"mymap\">" + "<map-store >" + writeCoalescingConfigPart + "</map-store>"+ "</map>"+ "</hazelcast>";
}
