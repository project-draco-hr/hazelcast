{
  if (DEBUG) {
    log("Processing ScheduledActionController");
  }
  dirty=false;
  if (setScheduledActions.size() > 0) {
    Iterator<ScheduledAction> it=setScheduledActions.iterator();
    while (it.hasNext()) {
      ScheduledAction sa=it.next();
      if (sa.expired()) {
        sa.onExpire();
        it.remove();
      }
    }
    if (setScheduledActions.size() > 0) {
      dirty=true;
    }
  }
}
