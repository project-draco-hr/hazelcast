{
  FieldDefinition fd=cd.get(fieldName);
  if (fd == null) {
    return 0;
  }
switch (fd.getType()) {
case FieldDefinition.TYPE_SHORT:
    return super.readShort(fieldName);
case FieldDefinition.TYPE_BYTE:
  return super.readByte(fieldName);
default :
throw new IncompatibleClassChangeError();
}
}
