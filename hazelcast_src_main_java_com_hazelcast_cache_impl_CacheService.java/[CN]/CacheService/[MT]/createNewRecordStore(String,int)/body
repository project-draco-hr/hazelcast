{
  CacheConfig cacheConfig=configs.get(name);
  if (cacheConfig == null) {
    throw new IllegalArgumentException("CacheConfig is null! " + name);
  }
  return new CacheRecordStore(name,partitionId,nodeEngine,CacheService.this,cacheConfig.getEvictionPolicy(),cacheConfig.getEvictionPercentage(),cacheConfig.getEvictionThresholdPercentage());
}
