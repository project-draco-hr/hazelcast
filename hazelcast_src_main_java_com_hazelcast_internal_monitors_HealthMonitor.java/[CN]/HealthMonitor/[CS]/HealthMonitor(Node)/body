{
  this.node=node;
  this.logger=node.getLogger(HealthMonitor.class);
  this.metricRegistry=node.nodeEngine.getMetricsRegistry();
  this.monitorLevel=getHealthMonitorLevel();
  this.thresholdMemoryPercentage=node.getGroupProperties().getInteger(GroupProperty.HEALTH_MONITORING_THRESHOLD_MEMORY_PERCENTAGE);
  this.thresholdCPUPercentage=node.getGroupProperties().getInteger(GroupProperty.HEALTH_MONITORING_THRESHOLD_CPU_PERCENTAGE);
  this.monitorThread=initMonitorThread();
  this.healthMetrics=new HealthMetrics();
}
