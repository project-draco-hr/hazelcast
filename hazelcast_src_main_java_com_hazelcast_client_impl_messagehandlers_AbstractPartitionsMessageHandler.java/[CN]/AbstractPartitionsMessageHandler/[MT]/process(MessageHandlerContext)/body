{
  final OperationService opService=context.getOperationService();
  beforeProcess(context);
  ClientEndpoint endpoint=context.getClientEndpoint();
  Operation op=prepareOperation(context);
  op.setCallerUuid(endpoint.getUuid());
  InvocationBuilder builder=opService.createInvocationBuilder(getServiceName(),op,getPartition(context)).setReplicaIndex(getReplicaIndex()).setTryCount(TRY_COUNT).setResultDeserialized(false).setCallback(new CallbackImpl(context));
  builder.invoke();
}
