{
  Config config=new Config();
  config.getServicesConfig().addServiceConfig(new ServiceConfig().setServiceImpl(new TestInitializingObjectService()).setEnabled(true).setName(TestInitializingObjectService.NAME));
  HazelcastInstance instance=createHazelcastInstanceFactory(1).newHazelcastInstance(config);
  TestInitializingObject object=instance.getDistributedObject(TestInitializingObjectService.NAME,"test-object");
  test(instance,object);
}
