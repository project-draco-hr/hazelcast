{
  try {
    List<String> list=aws.getPrivateIpAddresses();
    if (list.isEmpty()) {
      logger.warning("No EC2 instances found!");
    }
 else {
      logger.finest("The list of possible members are: " + list);
    }
    return list;
  }
 catch (  Exception e) {
    logger.warning(e);
    throw ExceptionUtil.rethrow(e);
  }
}
