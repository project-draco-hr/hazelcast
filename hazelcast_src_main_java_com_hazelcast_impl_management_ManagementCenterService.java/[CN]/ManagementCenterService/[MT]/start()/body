{
  if (running) {
    return;
  }
  running=true;
  if (webServerUrl != null) {
    taskPoller.start();
    stateSender.start();
    logger.log(Level.INFO,"Hazelcast Management Center is listening from " + webServerUrl);
  }
 else {
    logger.log(Level.WARNING,"Hazelcast Management Center Web server url is null!");
  }
}
