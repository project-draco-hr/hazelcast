{
  to.setNoData();
  int len=from.lsData.size();
  for (int i=0; i < len; i++) {
    ByteBuffer bb=from.lsData.get(i);
    ByteBuffer bbTo=obtainEmptyBuffer();
    bbTo.put(bb.array(),0,bb.limit());
    to.add(bbTo);
    bbTo.flip();
  }
  if (from.size != to.size)   throw new RuntimeException("size doesn't match: src " + from.size + " dest: "+ to.size);
}
