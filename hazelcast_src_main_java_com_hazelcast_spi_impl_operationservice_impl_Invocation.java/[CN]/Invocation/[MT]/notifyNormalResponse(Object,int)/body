{
  if (value == null) {
    value=NULL_RESPONSE;
  }
  if (expectedBackups > backupsCompleted) {
    this.pendingResponseReceivedMillis=Clock.currentTimeMillis();
    this.backupsExpected=expectedBackups;
    this.pendingResponse=value;
    if (backupsCompleted != expectedBackups) {
      return;
    }
  }
  future.set(value);
}
