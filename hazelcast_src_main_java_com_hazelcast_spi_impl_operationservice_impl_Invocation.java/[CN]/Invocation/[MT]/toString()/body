{
  String connectionStr=null;
  Address invTarget=this.invTarget;
  if (invTarget != null) {
    ConnectionManager connectionManager=operationService.nodeEngine.getNode().getConnectionManager();
    Connection connection=connectionManager.getConnection(invTarget);
    connectionStr=connection == null ? null : connection.toString();
  }
  return "Invocation{" + ", op=" + op + ", tryCount="+ tryCount+ ", tryPauseMillis="+ tryPauseMillis+ ", invokeCount="+ invokeCount+ ", callTimeout="+ callTimeout+ ", target="+ invTarget+ ", backupsExpected="+ backupsExpected+ ", backupsCompleted="+ backupsCompleted+ ", connection="+ connectionStr+ '}';
}
