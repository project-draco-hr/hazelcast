{
  if (!engineActive()) {
    return;
  }
  invokeCount++;
  if (!initInvocationTarget()) {
    return;
  }
  setInvocationTime(op,nodeEngine.getClusterService().getClusterClock().getClusterTime());
  operationService.invocationsRegistry.register(this);
  if (remote) {
    doInvokeRemote();
  }
 else {
    doInvokeLocal();
  }
}
