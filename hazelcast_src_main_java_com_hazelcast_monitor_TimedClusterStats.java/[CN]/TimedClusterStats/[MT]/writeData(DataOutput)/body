{
  out.writeLong(time);
  out.writeInt(memberStats.size());
  Set<Map.Entry<Member,MemberStatsImpl>> memberStatEntries=memberStats.entrySet();
  for (  Map.Entry<Member,MemberStatsImpl> memberStatEntry : memberStatEntries) {
    memberStatEntry.getKey().writeData(out);
    memberStatEntry.getValue().writeData(out);
  }
}
