{
  MigrationStrategy strategy;
  if (Boolean.getBoolean("hazelcast.io.balancer.monkey")) {
    log.warning("Using Monkey IO Balancer Strategy. This is for stress tests only. Do not user in production! " + "Disable by not setting the property '" + PROP_MONKEY_BALANCER + "' to true.");
    strategy=new MonkeyMigrationStrategy();
  }
 else {
    log.finest("Using normal IO Balancer Strategy.");
    strategy=new StaticMigrationStrategy();
  }
  return strategy;
}
