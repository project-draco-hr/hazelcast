{
  while (true) {
    if (cancelled.get()) {
      return;
    }
    Integer partitionId=findNewPartitionProcessing();
    if (partitionId == null) {
      return;
    }
    if (partitionId == -1) {
      continue;
    }
    try {
      ((PartitionIdAware)keyValueSource).setPartitionId(partitionId);
      processPartitionMapping(delegate,partitionId);
    }
 catch (    Throwable t) {
      handleProcessorThrowable(t);
    }
  }
}
