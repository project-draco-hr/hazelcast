{
  try {
    RequestPartitionResult result=mapReduceService.processRequest(supervisor.getJobOwner(),new RequestPartitionMapping(name,jobId),name);
    if (result.getResultState() == NO_SUPERVISOR) {
      return null;
    }
 else     if (result.getResultState() == CHECK_STATE_FAILED) {
      return -1;
    }
 else     if (result.getResultState() == NO_MORE_PARTITIONS) {
      return null;
    }
 else {
      return result.getPartitionId();
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
