{
  if (state == State.REMOVE) {
    return null;
  }
  if (value != null) {
    return value;
  }
  if (record != null) {
    state=State.ACCESS;
    return getRecordValue(record);
  }
  if (recordLoaded == null) {
    recordLoaded=cacheRecordStore.readThroughRecord(keyData,now);
  }
  if (recordLoaded != null) {
    state=State.LOAD;
    return getRecordValue(recordLoaded);
  }
  return null;
}
