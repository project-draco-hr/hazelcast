{
  if (data == null) {
    if (hash == null) {
      return false;
    }
 else {
      hash=null;
      return true;
    }
  }
  byte[] newHash=hash(data);
  if (hash == null) {
    hash=newHash;
    return true;
  }
  boolean same=Arrays.equals(hash,newHash);
  if (!same) {
    hash=newHash;
    return true;
  }
  return false;
}
