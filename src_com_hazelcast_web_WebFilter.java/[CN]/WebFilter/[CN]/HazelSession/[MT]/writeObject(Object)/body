{
  if (obj == null)   return null;
  try {
    final Data data=ThreadContext.get().toData(obj);
    final int size=data.size();
    if (minSize == -1 || minSize > size)     minSize=size;
    if (maxSize == -1 || maxSize < size)     maxSize=size;
    return data;
  }
 catch (  final Exception e) {
    e.printStackTrace();
  }
  return null;
}
