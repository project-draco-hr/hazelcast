{
  flush();
  long minReqT=(minRequestTime.get() == Long.MAX_VALUE) ? 0 : minRequestTime.get();
  long maxReqT=(maxRequestTime.get() == Long.MIN_VALUE) ? 0 : maxRequestTime.get();
  return new SnapshotEvent(context.getOriginalServletContext(),createdSessions.get(),destroyedSessions.get(),minReqT,maxReqT,aveRequestTime.get(),numberOfRequests.get());
}
