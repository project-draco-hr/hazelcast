{
  if (nano < minRequestTime.get())   minRequestTime.set(nano);
  if (nano > maxRequestTime.get())   maxRequestTime.set(nano);
  final long tempCount=tempNumberOfRequests.incrementAndGet();
  tempTotalReqTime.addAndGet(nano);
  if (tempCount > 10000) {
synchronized (averageLock) {
      if (tempCount > 10000) {
        flush();
      }
    }
  }
}
