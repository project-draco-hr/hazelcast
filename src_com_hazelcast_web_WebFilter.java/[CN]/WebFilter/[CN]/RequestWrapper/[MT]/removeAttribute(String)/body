{
  if (DEBUG) {
    logger.log(Level.FINEST,"removeAttribute " + name);
  }
  if (HAZELCAST_REQUEST.equals(name))   return;
  final Object oldValue=atts.remove(name);
  if (oldValue == null)   return;
  if (context.lsRequestAttListeners.size() > 0) {
    executor.execute(new Runnable(){
      public void run(){
        final ServletRequestAttributeEvent event=new ServletRequestAttributeEvent(context.getOriginalServletContext(),RequestWrapper.this,name,oldValue);
        for (        final ServletRequestAttributeListener listener : context.lsRequestAttListeners) {
          listener.attributeRemoved(event);
        }
      }
    }
);
  }
}
