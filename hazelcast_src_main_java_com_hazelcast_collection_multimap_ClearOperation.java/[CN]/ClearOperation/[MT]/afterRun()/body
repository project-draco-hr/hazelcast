{
  if (entries != null) {
    for (    Map.Entry<Data,Collection> entry : entries.entrySet()) {
      Data key=entry.getKey();
      Collection coll=entry.getValue();
      for (      Object obj : coll) {
        publishEvent(EntryEventType.REMOVED,key,obj);
      }
    }
    entries.clear();
    entries=null;
  }
}
