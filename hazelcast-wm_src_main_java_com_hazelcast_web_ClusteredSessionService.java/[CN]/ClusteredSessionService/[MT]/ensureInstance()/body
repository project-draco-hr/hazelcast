{
  if (failedConnection && System.currentTimeMillis() > lastConnectionTry + RETRY_MILLIS) {
synchronized (this) {
      try {
        if (failedConnection && System.currentTimeMillis() > lastConnectionTry + RETRY_MILLIS) {
          reconnectHZInstance();
          clearOrphanSessionQueue();
        }
      }
 catch (      Exception e) {
        setFailedConnection(true);
        if (LOGGER.isFinestEnabled()) {
          LOGGER.finest("Cannot connect hazelcast server",e);
        }
      }
    }
  }
}
