{
  if (count != null) {
    count.inc();
  }
  executedOperationsCount.incrementAndGet();
  boolean publishCurrentTask=publishCurrentTask();
  if (publishCurrentTask) {
    currentTask=op;
  }
  try {
    if (timeout(op)) {
      return;
    }
    ensureNoPartitionProblems(op);
    ensureQuorumPresent(op);
    op.beforeRun();
    if (waitingNeeded(op)) {
      return;
    }
    op.run();
    handleResponse(op);
    afterRun(op);
  }
 catch (  Throwable e) {
    handleOperationError(op,e);
  }
 finally {
    if (publishCurrentTask) {
      currentTask=null;
    }
  }
}
