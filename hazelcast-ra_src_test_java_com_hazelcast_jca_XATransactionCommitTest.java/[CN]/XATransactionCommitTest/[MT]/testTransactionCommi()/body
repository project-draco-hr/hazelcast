{
  Object o=context.lookup("java:/UserTransaction");
  assertNotNull(o);
  UserTransaction tx=(UserTransaction)o;
  HazelcastConnection c=connectionFactory.getConnection();
  Map<String,String> m=c.getMap("testmap");
  tx.begin();
  m.put("key","value");
  assertEquals("value",m.get("key"));
  doSql();
  tx.commit();
  assertEquals("value",m.get("key"));
}
