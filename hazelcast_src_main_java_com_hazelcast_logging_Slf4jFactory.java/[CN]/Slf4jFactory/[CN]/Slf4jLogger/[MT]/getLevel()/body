{
  if (logger.isDebugEnabled()) {
    return Level.FINEST;
  }
 else   if (logger.isInfoEnabled()) {
    return Level.INFO;
  }
 else   if (logger.isWarnEnabled()) {
    return Level.WARNING;
  }
 else   if (logger.isErrorEnabled()) {
    return Level.SEVERE;
  }
 else {
    return Level.OFF;
  }
}
