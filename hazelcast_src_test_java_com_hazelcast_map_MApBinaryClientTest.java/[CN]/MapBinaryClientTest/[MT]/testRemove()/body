{
  final IQueue q=hz.getQueue(mapName);
  q.offer("item1");
  q.offer("item2");
  q.offer("item3");
  c.send(new RemoveRequest(mapName,ss.toData("item2")));
  Boolean result=(Boolean)c.receive();
  assertTrue(result);
  assertEquals(2,q.size());
  c.send(new RemoveRequest(mapName,ss.toData("item2")));
  result=(Boolean)c.receive();
  assertFalse(result);
  assertEquals(2,q.size());
}
