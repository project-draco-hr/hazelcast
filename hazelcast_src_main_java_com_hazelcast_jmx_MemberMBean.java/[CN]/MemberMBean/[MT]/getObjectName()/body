{
  if (name == null) {
    String memberName="Local";
    if (!getManagedObject().localMember()) {
      memberName='"' + getManagedObject().getInetAddress().getHostAddress() + ":"+ getManagedObject().getPort()+ '"';
    }
    name=MBeanBuilder.buildObjectName("type","Cluster","name",memberName);
  }
  return name;
}
