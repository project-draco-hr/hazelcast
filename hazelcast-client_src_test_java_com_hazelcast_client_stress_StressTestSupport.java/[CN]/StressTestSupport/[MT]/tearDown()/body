{
  stopTest=true;
  if (killMemberThread != null) {
    try {
      killMemberThread.join(TimeUnit.SECONDS.toMillis(KILL_DELAY_SECONDS * 4));
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
  for (  HazelcastInstance hz : instances) {
    try {
      hz.shutdown();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
