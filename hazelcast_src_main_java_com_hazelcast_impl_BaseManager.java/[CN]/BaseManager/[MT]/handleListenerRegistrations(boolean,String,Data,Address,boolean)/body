{
  if (name.startsWith("q:")) {
    node.blockingQueueManager.handleListenerRegistrations(add,name,key,address,includeValue);
  }
 else   if (name.startsWith("t:")) {
    node.topicManager.handleListenerRegistrations(add,name,key,address,includeValue);
  }
 else {
    node.concurrentMapManager.handleListenerRegistrations(add,name,key,address,includeValue);
  }
}
