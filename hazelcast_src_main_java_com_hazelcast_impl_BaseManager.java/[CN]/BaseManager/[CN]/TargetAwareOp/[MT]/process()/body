{
  request.caller=thisAddress;
  setTarget();
  if (target == null) {
    setResult(OBJECT_REDO);
  }
 else {
    if (target.equals(thisAddress)) {
      long callId=localIdGen.incrementAndGet();
      request.callId=callId;
      setCallId(callId);
      doLocalOp();
    }
 else {
      invoke();
    }
  }
}
