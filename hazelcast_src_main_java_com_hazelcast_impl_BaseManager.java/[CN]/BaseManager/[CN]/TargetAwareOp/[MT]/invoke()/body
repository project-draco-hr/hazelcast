{
  addCall(TargetAwareOp.this);
  final Packet packet=obtainPacket();
  request.setPacket(packet);
  packet.callId=getId();
  final boolean sent=send(packet,target);
  if (!sent) {
    if (DEBUG) {
      log(TargetAwareOp.this + " Packet cannot be sent to " + target);
    }
    packet.returnToContainer();
    redo();
  }
}
