{
  Object result=getResult();
  if (result == OBJECT_NULL || result == null) {
    result=null;
  }
 else {
    if (result instanceof Data) {
      final Data data=(Data)result;
      if (ThreadContext.get().isClient() && force) {
        result=data;
      }
 else {
        if (data.size() == 0) {
          result=null;
        }
 else {
          result=toObject(data);
        }
      }
    }
  }
  afterGettingResult(request);
  return result;
}
