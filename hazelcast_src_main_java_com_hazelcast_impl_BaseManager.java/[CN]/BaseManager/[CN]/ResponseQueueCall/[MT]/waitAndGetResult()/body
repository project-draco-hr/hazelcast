{
  while (true) {
    try {
      Object obj=responses.poll(5,TimeUnit.SECONDS);
      if (obj != null) {
        return obj;
      }
      logger.log(Level.WARNING,"Still no response! reason:" + getCallState(request,getTarget()));
      node.checkNodeState();
    }
 catch (    InterruptedException e) {
      handleInterruptedException();
    }
  }
}
