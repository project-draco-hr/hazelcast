{
  while (true) {
    try {
      Object obj=responses.poll(5,TimeUnit.SECONDS);
      if (obj != null) {
        return obj;
      }
      node.checkNodeState();
    }
 catch (    InterruptedException e) {
      if (node.factory.restarted) {
        throw new RuntimeException();
      }
 else {
        throw new RuntimeInterruptedException();
      }
    }
  }
}
