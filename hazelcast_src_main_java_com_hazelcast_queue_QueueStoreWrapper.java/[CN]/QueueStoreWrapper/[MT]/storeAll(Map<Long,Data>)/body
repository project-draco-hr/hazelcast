{
  if (enabled) {
    if (binary) {
      store.storeAll(map);
    }
 else {
      Map<Long,Object> objectMap=new HashMap<Long,Object>(map.size());
      for (      Map.Entry<Long,Data> entry : map.entrySet()) {
        objectMap.put(entry.getKey(),serializationService.toObject(entry.getValue()));
      }
      store.storeAll(objectMap);
    }
  }
}
