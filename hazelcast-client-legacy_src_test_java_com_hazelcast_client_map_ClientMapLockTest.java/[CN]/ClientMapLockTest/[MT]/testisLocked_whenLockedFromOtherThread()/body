{
  final IMap map=client.getMap(randomString());
  final String key="key";
  final CountDownLatch lockedLatch=new CountDownLatch(1);
  new Thread(){
    public void run(){
      map.lock(key);
      lockedLatch.countDown();
    }
  }
.start();
  assertOpenEventually(lockedLatch);
  assertTrue(map.isLocked(key));
}
