{
  if (isInsideAws) {
    return address;
  }
  if (address == null) {
    return null;
  }
  String privateAddress=getLookupTable().get(address.getHost());
  if (privateAddress != null) {
    return constructPrivateAddress(privateAddress,address);
  }
  refresh();
  privateAddress=getLookupTable().get(address.getHost());
  if (privateAddress != null) {
    return constructPrivateAddress(privateAddress,address);
  }
  return null;
}
