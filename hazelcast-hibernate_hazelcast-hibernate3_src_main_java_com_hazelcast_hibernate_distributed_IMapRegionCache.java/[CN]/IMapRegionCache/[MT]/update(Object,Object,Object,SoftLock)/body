{
  if (lock instanceof MarkerWrapper) {
    final ExpiryMarker unwrappedMarker=((MarkerWrapper)lock).getMarker();
    return (Boolean)map.executeOnKey(key,new UpdateEntryProcessor(unwrappedMarker,newValue,newVersion,nextMarkerId(),nextTimestamp(hazelcastInstance)));
  }
 else {
    return false;
  }
}
