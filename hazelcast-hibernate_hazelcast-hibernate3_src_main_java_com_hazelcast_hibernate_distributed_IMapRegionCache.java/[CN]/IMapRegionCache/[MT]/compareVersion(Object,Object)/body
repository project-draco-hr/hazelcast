{
  final CacheEntry currentEntry=(CacheEntry)value;
  try {
    if (map.tryLock(key,tryLockAndGetTimeout,TimeUnit.MILLISECONDS)) {
      return compareVersionUnderRowLock(key,value,currentEntry);
    }
 else {
      return false;
    }
  }
 catch (  InterruptedException e) {
    return false;
  }
}
