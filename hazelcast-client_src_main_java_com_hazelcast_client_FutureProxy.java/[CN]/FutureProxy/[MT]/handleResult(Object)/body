{
  if (exception != null) {
    throw this.exception;
  }
  if (isDone) {
    return this.result;
  }
  if (result instanceof ExecutionException) {
    this.exception=(ExecutionException)result;
    throw this.exception;
  }
 else {
    isDone=true;
    this.result=(T)result;
    return this.result;
  }
}
