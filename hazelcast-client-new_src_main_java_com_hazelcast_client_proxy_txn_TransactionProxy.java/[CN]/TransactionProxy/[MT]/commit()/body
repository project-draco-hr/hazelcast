{
  try {
    if (state != ACTIVE) {
      throw new TransactionNotActiveException("Transaction is not active");
    }
    checkThread();
    checkTimeout();
    ClientMessage request=TransactionCommitCodec.encodeRequest(txnId,threadId,true);
    invoke(request);
    state=COMMITTED;
  }
 catch (  Exception e) {
    state=ROLLING_BACK;
    throw ExceptionUtil.rethrow(e);
  }
 finally {
    TRANSACTION_EXISTS.set(null);
  }
}
