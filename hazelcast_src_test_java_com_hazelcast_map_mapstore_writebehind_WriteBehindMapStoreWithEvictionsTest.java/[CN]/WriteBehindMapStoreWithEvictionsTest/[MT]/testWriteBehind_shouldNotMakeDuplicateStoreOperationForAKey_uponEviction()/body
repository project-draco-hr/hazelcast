{
  final AtomicInteger storeCount=new AtomicInteger(0);
  MapStore store=createSlowMapStore(storeCount);
  IMap<Object,Object> map=TestMapUsingMapStoreBuilder.create().withMapStore(store).withNodeCount(1).withNodeFactory(createHazelcastInstanceFactory(1)).withBackupCount(0).withWriteDelaySeconds(1).build();
  map.put(1,1);
  map.evict(1);
  sleepSeconds(2);
  assertStoreCount(1,storeCount);
}
