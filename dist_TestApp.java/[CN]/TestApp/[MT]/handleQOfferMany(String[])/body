{
  int count=1;
  if (args.length > 1)   count=Integer.parseInt(args[1]);
  Object value=null;
  if (args.length > 2)   value=new byte[Integer.parseInt(args[2])];
  long t0=System.currentTimeMillis();
  for (int i=0; i < count; i++) {
    if (value == null)     queue.offer("obj");
 else     queue.offer(value);
  }
  long t1=System.currentTimeMillis();
  System.out.print("size = " + queue.size() + ", "+ count * 1000 / (t1 - t0) + " evt/s");
  if (value == null) {
    System.out.println("");
  }
 else {
    int b=Integer.parseInt(args[2]);
    System.out.println(", " + (count * 1000 / (t1 - t0)) * (b * 8) / 1024 + " Kbit/s, " + count * b / 1024 + " KB added");
  }
}
