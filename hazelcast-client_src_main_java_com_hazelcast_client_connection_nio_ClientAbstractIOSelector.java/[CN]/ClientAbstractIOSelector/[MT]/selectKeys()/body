{
  final Set<SelectionKey> setSelectedKeys=selector.selectedKeys();
  final Iterator<SelectionKey> it=setSelectedKeys.iterator();
  while (it.hasNext()) {
    final SelectionKey sk=it.next();
    try {
      it.remove();
      handleSelectionKey(sk);
    }
 catch (    Throwable e) {
      handleSelectorException(e);
    }
  }
}
