{
  final SerializableXID sXid=new SerializableXID(xid.getFormatId(),xid.getGlobalTransactionId(),xid.getBranchQualifier());
  final ClientConnection connection=recoveredTransactions.remove(sXid);
  if (connection == null) {
    return false;
  }
  ClientMessage request=TransactionRecoverParameters.encode(sXid,commit);
  try {
    final ClientInvocation clientInvocation=new ClientInvocation(client,request,connection);
    final Future<ClientMessage> future=clientInvocation.invoke();
    future.get();
  }
 catch (  Exception e) {
    ExceptionUtil.rethrow(e);
  }
  return true;
}
