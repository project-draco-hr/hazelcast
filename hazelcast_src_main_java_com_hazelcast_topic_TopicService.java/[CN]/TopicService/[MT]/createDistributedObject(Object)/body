{
  final String name=String.valueOf(objectId);
  TopicProxy proxy;
  TopicConfig topicConfig=nodeEngine.getConfig().getTopicConfig(name);
  if (topicConfig.isGlobalOrderingEnabled())   proxy=new TotalOrderedTopicProxy(name,nodeEngine,this);
 else   proxy=new TopicProxy(name,nodeEngine,this);
  return proxy;
}
