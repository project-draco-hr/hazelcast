{
  TopicEvent topicEvent=(TopicEvent)event;
  Message message=new DataAwareMessage(topicEvent.name,topicEvent.data,topicEvent.publishTime,topicEvent.publishingMember,nodeEngine.getSerializationService());
  incrementReceivedMessages(topicEvent.name);
  MessageListener messageListener=(MessageListener)listener;
  messageListener.onMessage(message);
}
