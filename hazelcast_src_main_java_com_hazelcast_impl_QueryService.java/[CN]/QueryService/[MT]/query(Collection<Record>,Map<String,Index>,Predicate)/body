{
  try {
    final BlockingQueue<Collection<Record>> resultQ=new ArrayBlockingQueue(1);
    queryQ.put(new Runnable(){
      public void run(){
        resultQ.offer(doQuery(allRecords,namedIndexes,predicate));
      }
    }
);
    return resultQ.take();
  }
 catch (  InterruptedException e) {
  }
  return new ArrayList();
}
