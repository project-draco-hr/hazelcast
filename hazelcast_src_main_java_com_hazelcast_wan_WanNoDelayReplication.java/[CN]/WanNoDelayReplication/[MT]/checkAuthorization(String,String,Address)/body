{
  Operation authorizationCall=new AuthorizationOperation(groupName,groupPassword);
  Future<Boolean> future=node.nodeService.getInvocationService().createInvocationBuilder(WanReplicationService.SERVICE_NAME,authorizationCall,target).setTryCount(1).build().invoke();
  try {
    return future.get();
  }
 catch (  Exception ignored) {
  }
  return false;
}
