{
  if (isBindToSingleConnection()) {
    return false;
  }
  if (!shouldRetry()) {
    return false;
  }
  beforeRetry();
  try {
    sleep();
    executionService.execute(this);
  }
 catch (  RejectedExecutionException e) {
    if (LOGGER.isFinestEnabled()) {
      LOGGER.finest("Retry could not be scheduled ",e);
    }
    clientInvocationFuture.setResponse(e);
  }
  return true;
}
