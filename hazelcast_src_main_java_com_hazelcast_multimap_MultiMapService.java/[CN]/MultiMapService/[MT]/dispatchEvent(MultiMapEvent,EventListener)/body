{
  EntryListener entryListener=(EntryListener)listener;
  EntryEvent entryEvent=new EntryEvent(event.getName(),nodeEngine.getClusterService().getMember(event.getCaller()),event.getEventType().getType(),nodeEngine.toObject(event.getKey()),nodeEngine.toObject(event.getValue()));
  if (event.getEventType().equals(EntryEventType.ADDED)) {
    entryListener.entryAdded(entryEvent);
  }
 else   if (event.getEventType().equals(EntryEventType.REMOVED)) {
    entryListener.entryRemoved(entryEvent);
  }
  getLocalMultiMapStatsImpl(event.getName()).incrementReceivedEvents();
}
