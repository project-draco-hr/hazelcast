{
  int hash=hash(k.hashCode());
  int i=indexFor(hash,table.length);
  Entry<K,V> prev=table[i];
  Entry<K,V> e=prev;
  while (e != null) {
    Entry<K,V> next=e.next;
    if ((e.hash == hash) && (k == e.key || k.equals(e.key))) {
      modCount++;
      size--;
      if (prev == e)       table[i]=next;
 else       prev.next=next;
      e.recordRemoval(this);
      return e;
    }
    prev=e;
    e=next;
  }
  return e;
}
