{
  HazelcastMBean bean=createHazelcastBean(distributedObject);
  if (bean != null) {
    MBeanServer mbs=ManagementFactory.getPlatformMBeanServer();
    if (!mbs.isRegistered(bean.objectName)) {
      try {
        mbs.registerMBean(bean,bean.objectName);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
 else {
  }
}
