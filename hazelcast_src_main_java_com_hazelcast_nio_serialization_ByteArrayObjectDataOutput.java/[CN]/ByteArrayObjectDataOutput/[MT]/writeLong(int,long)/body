{
  write(position,(int)(v >>> 56));
  write(position + 1,(int)(v >>> 48));
  write(position + 2,(int)(v >>> 40));
  write(position + 3,(int)(v >>> 32));
  write(position + 4,(int)(v >>> 24));
  write(position + 5,(int)(v >>> 16));
  write(position + 6,(int)(v >>> 8));
  write(position + 7,(int)(v));
}
