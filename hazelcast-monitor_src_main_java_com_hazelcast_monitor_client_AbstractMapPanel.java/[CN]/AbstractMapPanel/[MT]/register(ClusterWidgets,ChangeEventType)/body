{
  List<MonitoringPanel> list=clusterWidgets.getPanels().get(changeEventType);
  if (list == null) {
    list=new ArrayList<MonitoringPanel>();
    clusterWidgets.getPanels().put(changeEventType,list);
  }
  if (!list.contains(this)) {
    list.add(this);
    return true;
  }
 else {
    return false;
  }
}
