{
  ensureOpen();
  if (key == null) {
    throw new NullPointerException(NULL_KEY_IS_NOT_ALLOWED);
  }
  final SerializationService serializationService=nodeEngine.getSerializationService();
  final Operation op=new CacheGetOperation(getDistributedObjectName(),key,null);
  final InternalCompletableFuture<Object> f=nodeEngine.getOperationService().invokeOnPartition(getServiceName(),op,getPartitionId(nodeEngine,key));
  Object result=f.getSafely();
  if (result instanceof Data) {
    result=serializationService.toObject((Data)result);
  }
  return (V)result;
}
