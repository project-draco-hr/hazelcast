{
  ensureOpen();
  validateNotNull(key);
  final Data k=serializationService.toData(key);
  final Operation op=new CacheContainsKeyOperation(getDistributedObjectName(),k);
  final InternalCompletableFuture<Boolean> f=getNodeEngine().getOperationService().invokeOnPartition(getServiceName(),op,getPartitionId(getNodeEngine(),k));
  return f.getSafely();
}
