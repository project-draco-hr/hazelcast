{
  ensureOpen();
  checkNotNull(cacheEntryListenerConfiguration,"CacheEntryListenerConfiguration can't be null");
  final ICacheService service=getService();
  final CacheEventListenerAdaptor<K,V> entryListener=new CacheEventListenerAdaptor<K,V>(this,cacheEntryListenerConfiguration,getNodeEngine().getSerializationService());
  final String regId=service.registerListener(getDistributedObjectName(),entryListener);
  if (regId != null) {
    cacheConfig.addCacheEntryListenerConfiguration(cacheEntryListenerConfiguration);
    addListenerLocally(regId,cacheEntryListenerConfiguration);
    updateCacheListenerConfigOnOtherNodes(cacheEntryListenerConfiguration,true);
  }
}
