{
  if (record == null) {
    return null;
  }
  final Record result=mapContainer.getReachabilityHandlerChain().isReachable(record,criteria,System.nanoTime());
  if (result != null) {
    return result;
  }
  final Data key=record.getKey();
  final Object value=record.getValue();
  evict(key);
  doPostEvictionOperations(key,value);
  return null;
}
