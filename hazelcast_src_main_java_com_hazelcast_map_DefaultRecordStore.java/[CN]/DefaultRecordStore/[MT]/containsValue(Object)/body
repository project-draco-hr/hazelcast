{
  checkIfLoaded();
  final long now=getNow();
  for (  Record record : records.values()) {
    if (nullIfExpired(record,false) == null) {
      continue;
    }
    if (mapServiceContext.compare(name,value,record.getValue())) {
      return true;
    }
  }
  postReadCleanUp(now,false);
  return false;
}
