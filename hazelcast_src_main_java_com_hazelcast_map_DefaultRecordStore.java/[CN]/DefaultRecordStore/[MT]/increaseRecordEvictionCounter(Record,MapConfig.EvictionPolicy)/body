{
switch (evictionPolicy) {
case LRU:
    record.setAccessCounter(++lruAccessCounter);
  break;
case LFU:
record.setAccessCounter(record.getAccessCounter() + 1L);
break;
case NONE:
break;
default :
throw new IllegalArgumentException("Not an appropriate eviction policy [" + evictionPolicy + ']');
}
}
