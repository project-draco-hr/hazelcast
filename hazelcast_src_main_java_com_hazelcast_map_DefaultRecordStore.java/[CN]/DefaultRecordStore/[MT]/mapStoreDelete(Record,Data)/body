{
  final MapStoreWrapper store=mapContainer.getStore();
  if (store == null) {
    return;
  }
  final long writeDelayMillis=mapContainer.getWriteDelayMillis();
  if (writeDelayMillis < 1L) {
    store.delete(mapService.toObject(key));
    if (record != null) {
      record.onStore();
    }
    return;
  }
  addToDelayedStore(key,null);
}
