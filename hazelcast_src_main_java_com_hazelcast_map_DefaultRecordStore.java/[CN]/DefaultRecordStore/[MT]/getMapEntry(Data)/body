{
  checkIfLoaded();
  Record record=records.get(dataKey);
  if (record == null) {
    record=getRecordInternal(dataKey,true);
  }
 else {
    accessRecord(record);
  }
  final Object value=record != null ? record.getValue() : null;
  return new AbstractMap.SimpleImmutableEntry<Data,Object>(dataKey,value);
}
