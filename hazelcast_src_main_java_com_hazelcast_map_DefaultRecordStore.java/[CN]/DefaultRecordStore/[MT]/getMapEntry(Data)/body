{
  checkIfLoaded();
  Record record=getRecord(key,false);
  if (record == null) {
    record=loadAndCreateRecordOrNull(key,true);
  }
 else {
    accessRecord(record);
  }
  final Object value=record != null ? record.getValue() : null;
  return new AbstractMap.SimpleImmutableEntry<Data,Object>(key,value);
}
