{
  final MapStoreWrapper store=mapContainer.getStore();
  if (store == null) {
    return;
  }
  final long writeDelay=getWriteDelayTime();
  if (writeDelay < 1L) {
    store.delete(mapService.toObject(key));
    if (record != null) {
      record.onStore();
    }
    return;
  }
  addToDelayedStore(key,null,now);
}
