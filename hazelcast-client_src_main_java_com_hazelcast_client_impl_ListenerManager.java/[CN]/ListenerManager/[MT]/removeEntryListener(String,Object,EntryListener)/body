{
  Map<Object,List<EntryListener>> m=mapOfListeners.get(name);
  if (m != null) {
    List<EntryListener> list=m.get(key);
    if (list != null) {
      list.remove(entryListener);
      if (m.get(key).size() == 0) {
        m.remove(key);
      }
    }
    if (m.size() == 0) {
      mapOfListeners.remove(name);
    }
  }
}
