{
  while (true) {
    Packet packet=null;
    try {
      packet=queue.take();
      EntryEvent event=new EntryEvent(packet.getName(),(int)packet.getLongValue(),Serializer.toObject(packet.getKey()),Serializer.toObject(packet.getValue()));
      fireEvent(event);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
