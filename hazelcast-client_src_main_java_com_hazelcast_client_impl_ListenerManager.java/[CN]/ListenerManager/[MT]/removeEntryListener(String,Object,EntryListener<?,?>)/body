{
  Map<Object,List<EntryListener<?,?>>> m=entryListeners.get(name);
  if (m != null) {
    List<EntryListener<?,?>> list=m.get(key);
    if (list != null) {
      list.remove(entryListener);
      if (m.get(key).size() == 0) {
        m.remove(key);
      }
    }
    if (m.size() == 0) {
      entryListeners.remove(name);
    }
  }
}
