{
  final LoginContext lc=loginContext;
  if (lc != null) {
    lc.logout();
  }
  final TransactionContext context=transactionContext;
  if (context != null) {
    try {
      context.rollbackTransaction();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    transactionContext=null;
  }
  authenticated=false;
}
