{
  this.clientEngine=clientEngine;
  this.conn=conn;
  if (conn instanceof TcpIpConnection) {
    TcpIpConnection tcpIpConnection=(TcpIpConnection)conn;
    socketAddress=tcpIpConnection.getSocketChannelWrapper().socket().getRemoteSocketAddress();
  }
 else {
    socketAddress=null;
  }
  this.uuid=uuid;
}
