{
  SerializationConfig config=new SerializationConfig();
  TypeSerializerConfig tsc=new TypeSerializerConfig().setImplementation(new FooXmlSerializer()).setTypeClass(Foo.class);
  config.addTypeSerializer(tsc);
  SerializationService ss=new SerializationServiceBuilder().setUseNativeByteOrder(false).setByteOrder(order).setConfig(config).build();
  Foo foo=new Foo();
  foo.setFoo("f");
  Data d=ss.toData(foo);
  Foo newFoo=(Foo)ss.toObject(d);
  assertEquals(newFoo.getFoo(),foo.getFoo());
}
