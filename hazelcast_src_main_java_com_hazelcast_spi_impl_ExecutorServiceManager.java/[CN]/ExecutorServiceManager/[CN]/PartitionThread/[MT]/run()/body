{
  ThreadContext.get().setCurrentInstance(nodeService.getNode().hazelcastInstance);
  try {
    super.run();
  }
 catch (  OutOfMemoryError e) {
    OutOfMemoryErrorDispatcher.onOutOfMemory(e);
  }
 finally {
    try {
      ThreadContext.shutdown(this);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
