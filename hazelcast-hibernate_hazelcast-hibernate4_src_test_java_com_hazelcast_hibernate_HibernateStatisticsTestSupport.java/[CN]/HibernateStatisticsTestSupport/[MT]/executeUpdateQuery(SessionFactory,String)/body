{
  Session session=null;
  Transaction txn=null;
  try {
    session=sf.openSession();
    txn=session.beginTransaction();
    Query query=session.createQuery(queryString);
    query.setCacheable(true);
    query.executeUpdate();
    txn.commit();
  }
 catch (  RuntimeException e) {
    txn.rollback();
    e.printStackTrace();
    throw e;
  }
 finally {
    session.close();
  }
}
