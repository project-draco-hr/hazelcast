{
  if (itemMap == null) {
    if (itemSet != null && !itemSet.isEmpty()) {
      itemMap=new HashMap<Long,CollectionItem>(itemSet.size());
      for (      CollectionItem item : itemSet) {
        itemMap.put(item.getItemId(),item);
      }
      itemSet.clear();
    }
 else {
      itemMap=new HashMap<Long,CollectionItem>(1000);
    }
    itemSet=null;
  }
  return itemMap;
}
