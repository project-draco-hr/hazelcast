{
  if (!started)   return;
  executionIds.clear();
  if (executor != null) {
    try {
      executor.shutdown();
      executor.awaitTermination(5,TimeUnit.SECONDS);
    }
 catch (    InterruptedException ignored) {
    }
  }
  if (executorForMigrations != null) {
    try {
      executorForMigrations.shutdown();
      executorForMigrations.awaitTermination(5,TimeUnit.SECONDS);
    }
 catch (    InterruptedException ignored) {
    }
  }
  started=false;
}
