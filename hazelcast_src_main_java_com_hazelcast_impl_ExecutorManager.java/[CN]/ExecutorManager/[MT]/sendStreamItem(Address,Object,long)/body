{
  try {
    final Packet packet=obtainPacket("exe",null,value,ClusterOperation.STREAM,DEFAULT_TIMEOUT);
    packet.longValue=streamId;
    enqueueAndReturn(new Processable(){
      public void process(){
        send(packet,address);
      }
    }
);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
