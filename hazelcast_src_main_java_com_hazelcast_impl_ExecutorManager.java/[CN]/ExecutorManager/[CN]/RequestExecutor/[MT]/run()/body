{
  Object result=null;
  try {
    Callable callable=(Callable)toObject(request.value);
    result=callable.call();
    result=toData(result);
  }
 catch (  Throwable e) {
    result=toData(e);
  }
 finally {
    request.clearForResponse();
    request.response=result;
    enqueueAndReturn(new Processable(){
      public void process(){
        returnResponse(request);
      }
    }
);
  }
}
