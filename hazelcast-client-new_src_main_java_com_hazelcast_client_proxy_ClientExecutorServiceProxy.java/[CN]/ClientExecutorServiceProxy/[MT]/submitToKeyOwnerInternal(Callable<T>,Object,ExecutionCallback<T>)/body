{
  checkNotNull(task,"task should not be null");
  final String uuid=getUUID();
  final int partitionId=getPartitionId(key);
  final PartitionOwnerTargetCallableRequest request=new PartitionOwnerTargetCallableRequest(name,uuid,task,partitionId);
  final ICompletableFuture<T> f=invokeFuture(request,partitionId);
  f.andThen(callback);
}
