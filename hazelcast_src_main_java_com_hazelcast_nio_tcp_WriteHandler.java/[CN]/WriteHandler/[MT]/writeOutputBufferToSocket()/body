{
  outputBuffer.flip();
  try {
    int result=socketChannel.write(outputBuffer);
    this.bytesWritten.inc(result);
  }
 catch (  Exception e) {
    currentPacket=null;
    handleSocketException(e);
    return;
  }
  if (!outputBuffer.hasRemaining()) {
    outputBuffer.clear();
    return;
  }
  outputBuffer.compact();
}
