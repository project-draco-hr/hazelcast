{
  int evictedCount;
switch (policy) {
case RANDOM:
    evictedCount=evictRecordsRandom(percentage);
  break;
case LRU:
evictedCount=evictRecordsLRU(percentage);
break;
case LFU:
evictedCount=evictRecordsLFU(percentage);
break;
default :
throw new IllegalArgumentException("Unsupported eviction policy: " + policy);
}
return evictedCount;
}
