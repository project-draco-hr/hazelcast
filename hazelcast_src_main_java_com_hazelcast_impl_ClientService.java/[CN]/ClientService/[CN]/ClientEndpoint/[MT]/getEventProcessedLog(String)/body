{
  Map<Object,EntryEvent> eventProcessedLog=listeneds.get(name);
  if (eventProcessedLog == null) {
synchronized (name) {
      if (eventProcessedLog == null) {
        eventProcessedLog=new HashMap<Object,EntryEvent>();
        listeneds.put(name,eventProcessedLog);
      }
    }
  }
  return eventProcessedLog;
}
