{
  try {
    long millis=(Long)toObject(value);
    if (millis == -1) {
      return (Data)queue.take();
    }
 else     if (millis == 0) {
      return (Data)queue.poll();
    }
 else {
      return (Data)queue.poll((Long)millis,TimeUnit.MILLISECONDS);
    }
  }
 catch (  InterruptedException e) {
    throw new RuntimeException(e);
  }
}
