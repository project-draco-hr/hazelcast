{
  Session session=sf.openSession();
  Transaction tx=session.beginTransaction();
  try {
    for (int i=0; i < count; i++) {
      session.save(new DummyEntity(new Long(i),"dummy:" + i,i * 123456d,new Date()));
    }
    tx.commit();
  }
 catch (  Exception e) {
    tx.rollback();
    e.printStackTrace();
  }
 finally {
    session.close();
  }
}
