{
  EntryEvent entryEvent=new EntryEvent(event.getName(),nodeEngine.getCluster().getMember(event.getCaller()),event.getEventType().getType(),nodeEngine.toObject(event.getKey()),nodeEngine.toObject(event.getValue()));
  if (event.eventType.equals(EntryEventType.ADDED)) {
    listener.entryAdded(entryEvent);
  }
 else   if (event.eventType.equals(EntryEventType.REMOVED)) {
    listener.entryRemoved(entryEvent);
  }
}
