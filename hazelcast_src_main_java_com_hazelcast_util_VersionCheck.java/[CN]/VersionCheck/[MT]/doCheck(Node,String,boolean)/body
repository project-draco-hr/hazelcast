{
  String downloadId="source";
  InputStream is=null;
  try {
    is=getClass().getClassLoader().getResourceAsStream("hazelcast-download.properties");
    if (is != null) {
      final Properties properties=new Properties();
      properties.load(is);
      downloadId=properties.getProperty("hazelcastDownloadId");
    }
  }
 catch (  IOException ignored) {
    EmptyStatement.ignore(ignored);
  }
 finally {
    IOUtil.closeResource(is);
  }
  String urlStr="http://versioncheck.hazelcast.com/version.jsp?version=" + version + "&m="+ hazelcastNode.getLocalMember().getUuid()+ "&e="+ isEnterprise+ "&l="+ toMD5String(hazelcastNode.getConfig().getLicenseKey())+ "&p="+ downloadId+ "&c="+ toMD5String(hazelcastNode.getConfig().getGroupConfig().getName())+ "&crsz="+ convertToLetter(hazelcastNode.getClusterService().getMembers().size())+ "&cssz="+ convertToLetter(hazelcastNode.clientEngine.getClientEndpointCount());
  fetchWebService(urlStr);
}
