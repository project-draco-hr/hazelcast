{
  for (int i=0; i < 1000; i++) {
    Callable<String> task1=new BasicTestTask(), task2=new BasicTestTask();
    Future<String> future1=executor.submit(task1), future2=executor.submit(task2);
    assertEquals(future2.get(),BasicTestTask.RESULT);
    assertTrue(future2.isDone());
    assertEquals(future1.get(),BasicTestTask.RESULT);
    assertTrue(future1.isDone());
  }
}
