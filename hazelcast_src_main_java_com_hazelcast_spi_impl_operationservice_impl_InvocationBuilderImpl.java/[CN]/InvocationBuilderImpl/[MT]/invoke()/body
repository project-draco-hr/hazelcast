{
  op.setServiceName(serviceName);
  if (target == null) {
    op.setPartitionId(partitionId).setReplicaIndex(replicaIndex);
    return new PartitionInvocation(operationService,op,tryCount,tryPauseMillis,callTimeout,getTargetExecutionCallback(),resultDeserialized).invoke();
  }
 else {
    return new TargetInvocation(operationService,op,target,tryCount,tryPauseMillis,callTimeout,getTargetExecutionCallback(),resultDeserialized).invoke();
  }
}
