{
  int migration=0;
  for (  MigrationRequestTask t : qm) {
    if (t.getReplicaIndex() == index) {
      migration++;
    }
  }
  int replication=0;
  for (  MigrationRequestTask t : qr) {
    if (t.getReplicaIndex() == index) {
      replication++;
    }
  }
  System.out.println("INDEX= " + index + ", MIGRATION QUEUE= "+ migration+ ", REPLICATION QUEUE= "+ replication);
}
