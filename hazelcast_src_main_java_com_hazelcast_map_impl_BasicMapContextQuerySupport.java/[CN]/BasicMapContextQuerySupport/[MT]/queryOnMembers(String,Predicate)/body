{
  OperationService operationService=nodeEngine.getOperationService();
  Collection<MemberImpl> members=nodeEngine.getClusterService().getMemberList();
  List<Future<QueryResult>> futures=new ArrayList<Future<QueryResult>>(members.size());
  for (  MemberImpl member : members) {
    Future<QueryResult> future=operationService.invokeOnTarget(MapService.SERVICE_NAME,new QueryOperation(mapName,predicate),member.getAddress());
    futures.add(future);
  }
  return futures;
}
