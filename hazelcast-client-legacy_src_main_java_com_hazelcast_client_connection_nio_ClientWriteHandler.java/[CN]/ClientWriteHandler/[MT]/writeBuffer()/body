{
  while (buffer.hasRemaining() && lastPacket != null) {
    boolean complete=lastPacket.writeTo(buffer);
    if (complete) {
      lastPacket=poll();
    }
 else {
      break;
    }
  }
  if (buffer.position() > 0) {
    buffer.flip();
    socketChannel.write(buffer);
    if (buffer.hasRemaining()) {
      buffer.compact();
    }
 else {
      buffer.clear();
    }
  }
}
