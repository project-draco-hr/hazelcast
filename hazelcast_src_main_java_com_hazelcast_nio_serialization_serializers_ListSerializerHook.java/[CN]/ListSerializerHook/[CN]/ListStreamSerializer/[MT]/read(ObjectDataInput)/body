{
  if (in.readBoolean()) {
    int size=in.readInt();
    int type=in.readInt();
    List result;
    if (type == 0) {
      result=new ArrayList(size);
    }
 else {
      result=new LinkedList();
    }
    for (int i=0; i < size; i++) {
      result.add(i,in.readObject());
    }
    return result;
  }
  return null;
}
