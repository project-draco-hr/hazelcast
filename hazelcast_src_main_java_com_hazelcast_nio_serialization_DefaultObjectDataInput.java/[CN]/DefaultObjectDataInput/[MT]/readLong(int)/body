{
  read(position,longBuffer,0,8);
  return (((long)longBuffer[0] << 56) + ((long)(longBuffer[1] & 255) << 48) + ((long)(longBuffer[2] & 255) << 40)+ ((long)(longBuffer[3] & 255) << 32)+ ((long)(longBuffer[4] & 255) << 24)+ ((longBuffer[5] & 255) << 16)+ ((longBuffer[6] & 255) << 8)+ ((longBuffer[7] & 255) << 0));
}
