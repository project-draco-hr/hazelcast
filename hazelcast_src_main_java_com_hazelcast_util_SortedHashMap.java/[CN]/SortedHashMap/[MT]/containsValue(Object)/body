{
  if (value == null) {
    for (Entry<K,V> e=header.after; e != header; e=e.after) {
      if (e.value == null) {
        return true;
      }
    }
  }
 else {
    for (Entry<K,V> e=header.after; e != header; e=e.after) {
      if (value.equals(e.value)) {
        return true;
      }
    }
  }
  return false;
}
