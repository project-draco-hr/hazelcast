{
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(CountDownLatchService.SERVICE_NAME,new SetCountOperation(name,count),partitionId).build();
  try {
    return (Boolean)nodeEngine.toObject(inv.invoke().get());
  }
 catch (  Exception e) {
    throw new HazelcastException(e);
  }
}
