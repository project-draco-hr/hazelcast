{
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(CountDownLatchService.SERVICE_NAME,new AwaitOperation(name,timeout),partitionId).build();
  try {
    return (Boolean)nodeEngine.toObject(inv.invoke().get());
  }
 catch (  ExecutionException e) {
    throw new HazelcastException(e);
  }
}
