{
  Record record=removedRecord == null ? storage.get(key) : removedRecord;
  Object value=null;
  if (record != null) {
    value=record.getValue();
    mapDataStore.flush(key,value,Clock.currentTimeMillis(),backup);
    removeIndex(record);
    value=deleteRecord(key);
    if (!backup) {
      mapServiceContext.interceptRemove(name,value);
    }
  }
  return value;
}
