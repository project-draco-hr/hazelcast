{
  Record record=storage.get(key);
  Object value=null;
  if (record != null) {
    value=record.getValue();
    mapDataStore.flush(key,value,backup);
    removeIndex(record);
    storage.removeRecord(record);
    if (!backup) {
      mapServiceContext.interceptRemove(name,value);
    }
  }
  return value;
}
