{
  Object oldValue=record.getValue();
  oldValue=mapServiceContext.interceptRemove(name,oldValue);
  if (oldValue != null) {
    removeIndex(record);
    mapDataStore.remove(key,now);
    onStore(record);
  }
  updateSizeEstimator(-calculateRecordHeapCost(record));
  deleteRecord(key);
  return oldValue;
}
