{
  checkIfLoaded();
  final long now=getNow();
  final Record record=getRecordOrNull(key,now,false);
  Object oldValue;
  if (record == null) {
    oldValue=mapDataStore.load(key);
    if (oldValue != null) {
      removeIndex(key);
      mapDataStore.remove(key,now);
    }
  }
 else {
    oldValue=removeRecord(key,record,now);
  }
  return oldValue;
}
