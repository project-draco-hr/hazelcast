{
  Record record=null;
  final Object value=mapDataStore.load(key);
  if (value != null) {
    record=createRecord(key,value,getNow());
    records.put(key,record);
    if (!backup) {
      saveIndex(record,null);
    }
    updateSizeEstimator(calculateRecordHeapCost(record));
  }
  return record;
}
