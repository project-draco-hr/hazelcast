{
  if (CollectionUtil.isEmpty(recordsToRemove)) {
    return 0;
  }
  int removalSize=recordsToRemove.size();
  Iterator<Record> iterator=recordsToRemove.iterator();
  while (iterator.hasNext()) {
    Record record=iterator.next();
    deleteRecord(record);
    iterator.remove();
  }
  return removalSize;
}
