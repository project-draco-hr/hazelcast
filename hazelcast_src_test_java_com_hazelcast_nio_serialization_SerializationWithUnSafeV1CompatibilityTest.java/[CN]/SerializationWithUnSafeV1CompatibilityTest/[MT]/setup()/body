{
  DefaultSerializationServiceBuilder defaultSerializationServiceBuilder=new DefaultSerializationServiceBuilder();
  serializationService=defaultSerializationServiceBuilder.setVersion(SerializationService.VERSION_1).addPortableFactory(TestSerializationConstants.PORTABLE_FACTORY_ID,new PortableTest.TestPortableFactory()).setAllowUnsafe(true).setUseNativeByteOrder(true).build();
}
