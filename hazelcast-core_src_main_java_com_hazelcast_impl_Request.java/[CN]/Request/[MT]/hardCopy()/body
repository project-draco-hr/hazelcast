{
  final Request copy=new Request();
  Data newKey=doHardCopy(key);
  Data newValue=doHardCopy(value);
  copy.set(local,operation,name,newKey,newValue,blockId,timeout,txnId,eventId,lockThreadId,lockAddress,lockCount,caller,longValue,recordId,version);
  copy.attachment=attachment;
  copy.response=response;
  copy.scheduled=scheduled;
  return copy;
}
