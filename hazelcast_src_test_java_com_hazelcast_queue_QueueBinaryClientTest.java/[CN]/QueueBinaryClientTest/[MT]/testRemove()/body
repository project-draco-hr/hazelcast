{
  final IQueue q=hz.getQueue(queueName);
  q.offer("item1");
  q.offer("item2");
  q.offer("item3");
  client().send(new RemoveRequest(queueName,ss.toData("item2")));
  Boolean result=(Boolean)client().receive();
  assertTrue(result);
  assertEquals(2,q.size());
  client().send(new RemoveRequest(queueName,ss.toData("item2")));
  result=(Boolean)client().receive();
  assertFalse(result);
  assertEquals(2,q.size());
}
