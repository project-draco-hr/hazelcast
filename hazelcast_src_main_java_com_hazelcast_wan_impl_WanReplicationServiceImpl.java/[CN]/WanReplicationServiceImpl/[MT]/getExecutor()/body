{
  StripedExecutor ex=executor;
  if (ex == null) {
synchronized (mutex) {
      if (executor == null) {
        executor=new StripedExecutor(node.getLogger(WanReplicationServiceImpl.class),node.getThreadNamePrefix("wan"),node.threadGroup,ExecutorConfig.DEFAULT_POOL_SIZE,ExecutorConfig.DEFAULT_QUEUE_CAPACITY);
      }
      ex=executor;
    }
  }
  return ex;
}
