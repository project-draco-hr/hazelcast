{
  members.remove(member);
  logger.info(membersString());
  final Connection connection=connectionManager.getConnection(member.getAddress());
  if (connection != null) {
    connectionManager.destroyConnection(connection);
  }
  MembershipEvent event=new MembershipEvent(client.getCluster(),member,MembershipEvent.MEMBER_REMOVED,Collections.unmodifiableSet(new LinkedHashSet<Member>(members)));
  clusterService.handleMembershipEvent(event);
}
