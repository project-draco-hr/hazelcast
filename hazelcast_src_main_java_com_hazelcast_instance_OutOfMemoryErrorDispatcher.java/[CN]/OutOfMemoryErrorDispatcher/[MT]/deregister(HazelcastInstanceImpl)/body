{
  for (int index=0; index < instances.length; index++) {
    HazelcastInstance hz=instances[index];
    if (hz == instance) {
      try {
        int numMoved=size - index - 1;
        if (numMoved > 0) {
          System.arraycopy(instances,index + 1,instances,index,numMoved);
        }
        instances[--size]=null;
        return true;
      }
 catch (      Throwable ignored) {
      }
    }
  }
  return false;
}
