{
  if (logger.isFinestEnabled()) {
    logger.finest("PerformanceMonitor failed to output to file:" + logFile.getAbsolutePath() + " cause:"+ e.getMessage(),e);
  }
 else {
    logger.warning("PerformanceMonitor failed to output to file:" + logFile.getAbsolutePath() + " cause:"+ e.getMessage());
  }
}
