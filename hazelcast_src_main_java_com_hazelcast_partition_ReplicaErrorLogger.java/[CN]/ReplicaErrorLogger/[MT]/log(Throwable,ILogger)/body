{
  if (e instanceof RetryableException) {
    Level level=Level.INFO;
    if (e instanceof CallerNotMemberException || e instanceof WrongTargetException || e instanceof TargetNotMemberException) {
      level=Level.FINEST;
    }
    logger.log(level,e.toString());
  }
 else {
    logger.log(Level.WARNING,e.getMessage(),e);
  }
}
