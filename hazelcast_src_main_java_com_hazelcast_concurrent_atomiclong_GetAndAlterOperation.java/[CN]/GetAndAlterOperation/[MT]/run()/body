{
  NodeEngine nodeEngine=getNodeEngine();
  Function<Long,Long> f=nodeEngine.toObject(function);
  AtomicLongWrapper number=getNumber();
  long input=number.get();
  response=input;
  long output=f.apply(input);
  shouldBackup=input != output;
  if (shouldBackup) {
    backup=output;
    number.set(output);
  }
}
