{
  final long now=this.now;
  final boolean checkExpiration=this.checkExpiration;
  final Iterator<Record> iterator=this.iterator;
  while (iterator.hasNext()) {
    nextRecord=iterator.next();
    if (nextRecord != null) {
      if (!checkExpiration) {
        return;
      }
      if (!isExpired(nextRecord,now)) {
        return;
      }
    }
  }
  nextRecord=null;
}
