{
  MapTxnOperation txnOp=(MapTxnOperation)op;
  txnOp.setThreadId(threadId);
  txnOp.setVersion(version);
  try {
    int partitionId=nodeEngine.getPartitionService().getPartitionId(key);
    Invocation invocation=nodeEngine.getOperationService().createInvocationBuilder(MapService.SERVICE_NAME,op,partitionId).build();
    return invocation.invoke();
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
