{
  address=new Address();
  address.readData(in);
  if (in.readBoolean()) {
    uuid=in.readUTF();
  }
  int size=in.readInt();
  if (size > 0)   attributes=new HashMap<String,Object>();
  for (int i=0; i < size; i++) {
    String key=in.readUTF();
    Object value=in.readObject();
    attributes.put(key,value);
  }
}
