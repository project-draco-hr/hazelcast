{
  ClientInvocation clientInvocation;
  final HazelcastClientInstanceImpl client=(HazelcastClientInstanceImpl)context.getHazelcastInstance();
  if (target != null) {
    ClientMessage request=ExecutorServiceCancelOnAddressParameters.encode(uuid,target.getHost(),target.getPort(),mayInterruptIfRunning);
    clientInvocation=new ClientInvocation(client,request,target);
  }
 else {
    ClientMessage request=ExecutorServiceCancelOnPartitionParameters.encode(uuid,partitionId,mayInterruptIfRunning);
    clientInvocation=new ClientInvocation(client,request,partitionId);
  }
  try {
    return clientInvocation.invoke();
  }
 catch (  Exception e) {
    throw rethrow(e);
  }
}
