{
  try {
    ExecutorService executorService=hazelcast.getExecutorService();
    MultiTask<String> echoTask=new MultiTask(new LongTask(args[1]),hazelcast.getCluster().getMembers()){
      @Override public void setMemberLeft(      Member member){
        println("Member Left " + member);
      }
      @Override public void done(){
        println("Done!");
      }
    }
;
    executorService.execute(echoTask);
    Collection<String> results=echoTask.get();
    for (    String result : results) {
      println(result);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
