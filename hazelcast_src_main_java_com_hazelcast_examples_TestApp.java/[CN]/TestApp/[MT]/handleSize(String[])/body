{
  int size=0;
  String iteratorStr=args[0];
  if (iteratorStr.startsWith("s.")) {
    size=set.size();
  }
 else   if (iteratorStr.startsWith("m.")) {
    size=map.size();
  }
 else   if (iteratorStr.startsWith("q.")) {
    size=queue.size();
  }
 else   if (iteratorStr.startsWith("l.")) {
    size=list.size();
  }
  println("Size = " + size);
}
