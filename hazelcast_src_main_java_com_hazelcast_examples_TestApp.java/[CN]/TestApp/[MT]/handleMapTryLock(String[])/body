{
  String key=args[1];
  long time=(args.length > 2) ? Long.valueOf(args[2]) : 0;
  boolean locked=false;
  if (time == 0)   locked=getMap().tryLock(key);
 else   try {
    locked=getMap().tryLock(key,time,TimeUnit.SECONDS);
  }
 catch (  InterruptedException e) {
    locked=false;
  }
  println(locked);
}
