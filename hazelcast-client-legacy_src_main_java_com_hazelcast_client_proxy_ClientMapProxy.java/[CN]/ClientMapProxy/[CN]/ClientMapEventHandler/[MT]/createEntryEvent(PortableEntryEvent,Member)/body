{
  V value=null;
  V oldValue=null;
  V mergingValue=null;
  if (includeValue) {
    value=toObject(event.getValue());
    oldValue=toObject(event.getOldValue());
    mergingValue=toObject(event.getMergingValue());
  }
  K key=toObject(event.getKey());
  return new EntryEvent<K,V>(name,member,event.getEventType().getType(),key,oldValue,value,mergingValue);
}
