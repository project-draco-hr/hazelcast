{
  if (average <= 1) {
    return;
  }
  final float r=3f;
  Assert.assertTrue("Too low partition count! Owned: " + count + ", Avg: "+ average+ ", Replica: "+ replica,count >= average / r);
  Assert.assertTrue("Too high partition count! Owned: " + count + ", Avg: "+ average+ ", Replica: "+ replica,count <= average * r);
}
