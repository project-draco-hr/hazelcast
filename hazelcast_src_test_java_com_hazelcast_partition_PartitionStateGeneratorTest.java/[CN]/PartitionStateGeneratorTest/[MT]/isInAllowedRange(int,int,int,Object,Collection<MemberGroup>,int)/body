{
  if (average <= 10) {
    return;
  }
  final float r=2f;
  Assert.assertTrue("Too low partition count! \nOwned: " + count + ", Avg: "+ average+ ", \nPartitionCount: "+ partitionCount+ ", Replica: "+ replica+ ", \nOwner: "+ owner,count >= (float)(average) / r);
  Assert.assertTrue("Too high partition count! \nOwned: " + count + ", Avg: "+ average+ ", \nPartitionCount: "+ partitionCount+ ", Replica: "+ replica+ ", \nOwner: "+ owner,count <= (float)(average) * r);
}
