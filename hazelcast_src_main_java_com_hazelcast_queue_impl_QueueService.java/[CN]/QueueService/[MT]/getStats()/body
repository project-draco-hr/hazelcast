{
  Map<String,LocalQueueStats> queueStats=MapUtil.createHashMap(containerMap.size());
  for (  Entry<String,QueueContainer> queueStat : containerMap.entrySet()) {
    queueStats.put(queueStat.getKey(),createLocalQueueStats(queueStat.getKey(),nodeEngine.getPartitionService().getPartitionId(nodeEngine.getSerializationService().toData(queueStat.getKey(),StringPartitioningStrategy.INSTANCE))));
  }
  return queueStats;
}
