{
  this.nodeEngine=nodeEngine;
  ScheduledExecutorService defaultScheduledExecutor=nodeEngine.getExecutionService().getDefaultScheduledExecutor();
  QueueEvictionProcessor entryProcessor=new QueueEvictionProcessor(nodeEngine,this);
  this.queueEvictionScheduler=EntryTaskSchedulerFactory.newScheduler(defaultScheduledExecutor,entryProcessor,ScheduleType.POSTPONE);
  this.logger=nodeEngine.getLogger(QueueService.class);
}
