{
  final Predicate predicate=query != null && query.trim().length() > 0 ? new SqlPredicate(query) : null;
  final Collection<Map.Entry> values=predicate != null ? getManagedObject().entrySet(predicate) : getManagedObject().entrySet();
  final StringBuilder sb=new StringBuilder().append("{");
  for (final Iterator<Map.Entry> it=values.iterator(); it.hasNext(); ) {
    final Map.Entry next=it.next();
    if (sb.length() > 1) {
      sb.append(", ");
    }
    sb.append("key:").append(next.getKey()).append(", value:").append(next.getValue());
  }
  return sb.append("}").toString();
}
