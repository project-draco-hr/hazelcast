{
  boolean[] b=ByteUtil.fromByte(in.readByte());
  enabled=b[0];
  if (enabled) {
    salt=in.readUTF();
    password=in.readUTF();
    iterationCount=in.readInt();
    algorithm=in.readUTF();
    boolean hasKey=b[1];
    if (hasKey) {
      key=new byte[in.readInt()];
      in.readFully(key);
    }
  }
}
