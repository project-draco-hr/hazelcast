{
  log(Level.FINEST,"Shutting down IO selectors... Total: " + selectorThreadCount);
  for (int i=0; i < selectorThreadCount; i++) {
    IOSelector ioSelector=inSelectors[i];
    if (ioSelector != null) {
      ioSelector.shutdown();
    }
    inSelectors[i]=null;
    ioSelector=outSelectors[i];
    if (ioSelector != null) {
      ioSelector.shutdown();
    }
    outSelectors[i]=null;
  }
}
