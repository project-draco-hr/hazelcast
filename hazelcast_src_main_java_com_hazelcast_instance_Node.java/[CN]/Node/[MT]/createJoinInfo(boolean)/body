{
  final JoinInfo jr=new JoinInfo(this.getLogger(JoinInfo.class.getName()),true,address,config,getLocalNodeType(),Packet.PACKET_VERSION,buildNumber,clusterService.getSize(),0,localMember.getUuid());
  if (withCredentials && securityContext != null) {
    Credentials c=securityContext.getCredentialsFactory().newCredentials();
    jr.setCredentials(c);
  }
  return jr;
}
