{
  Map<Integer,Employee> employees=new HashMap<Integer,Employee>();
  for (int i=0; i < employeeCount; i++) {
    Employee val;
    if (i % 2 == 0) {
      val=new Employee(i,null,null,0,true,i);
    }
 else {
      val=new Employee(i,"name" + i,"city" + i,0,true,i);
    }
    employees.put(i,Mockito.spy(val));
  }
  return employees;
}
