{
  final AtomicReference<MapConfig> ref=new AtomicReference<MapConfig>();
  getClusterService().enqueueAndWait(new Processable(){
    public void process(){
      final CMap cmap=getCMap(mapName);
      MapConfig cfg=cmap.getRuntimeConfig();
      ref.set(cfg);
    }
  }
,5);
  return ref.get();
}
