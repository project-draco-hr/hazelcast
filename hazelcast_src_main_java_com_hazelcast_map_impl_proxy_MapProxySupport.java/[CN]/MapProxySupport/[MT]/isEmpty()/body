{
  try {
    Map<Integer,Object> results=operationService.invokeOnAllPartitions(SERVICE_NAME,new IsEmptyOperationFactory(name));
    for (    Object result : results.values()) {
      if (!(Boolean)toObject(result)) {
        return false;
      }
    }
    return true;
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
