{
  int partitionId=partitionService.getPartitionId(key);
  MapOperation operation=operationProvider.createEntryOperation(name,key,entryProcessor);
  operation.setThreadId(ThreadUtil.getThreadId());
  try {
    if (callback == null) {
      return operationService.invokeOnPartition(SERVICE_NAME,operation,partitionId);
    }
 else {
      ICompletableFuture future=operationService.createInvocationBuilder(SERVICE_NAME,operation,partitionId).setExecutionCallback(new MapExecutionCallbackAdapter(callback)).invoke();
      invalidateNearCache(key);
      return future;
    }
  }
 catch (  Throwable t) {
    throw ExceptionUtil.rethrow(t);
  }
}
