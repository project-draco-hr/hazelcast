{
  List<Block> lsBlocks=q.lsBlocks;
  boolean exist=false;
  for (  Block block : lsBlocks) {
    if (block.blockId == newBlockId) {
      exist=true;
    }
  }
  Block newBlock=null;
  if (!exist) {
    newBlock=q.createBlock(newBlockOwner,newBlockId);
    q.addBlock(newBlock);
  }
  if (fullBlockId != -1) {
    q.setBlockFull(fullBlockId);
  }
  return newBlock;
}
