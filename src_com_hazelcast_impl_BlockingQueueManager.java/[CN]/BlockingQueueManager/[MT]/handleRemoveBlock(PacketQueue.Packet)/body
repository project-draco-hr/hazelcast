{
  try {
    BlockUpdate addRemove=(BlockUpdate)ThreadContext.get().toObject(packet.value);
    String name=packet.name;
    Q q=getQ(name);
    doRemoveBlock(q,packet.conn.getEndPoint(),addRemove.removeBlockId);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    packet.returnToContainer();
  }
}
