{
  try {
    BlockUpdate addRemove=(BlockUpdate)ThreadContext.get().toObject(inv.value);
    String name=inv.name;
    Q q=getQ(name);
    doRemoveBlock(q,inv.conn.getEndPoint(),addRemove.removeBlockId);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    inv.returnToContainer();
  }
}
