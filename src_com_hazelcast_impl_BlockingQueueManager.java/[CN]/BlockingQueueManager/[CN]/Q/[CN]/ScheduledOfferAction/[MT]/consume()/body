{
  Q q=getQ(request.name);
  Block currentPutBlock=q.getCurrentPutBlock();
  valid=(currentPutBlock.blockId == request.blockId) && thisAddress.equals(currentPutBlock.address) && !currentPutBlock.isFull();
  if (valid) {
    offer(request);
    request.response=Boolean.TRUE;
    request.key=null;
    request.value=null;
    returnScheduledAsBoolean(request);
    return true;
  }
 else {
    onExpire();
    return false;
  }
}
