{
  Q q=getQ(request.name);
  Block currentTakeBlock=q.getCurrentTakeBlock();
  valid=(currentTakeBlock.blockId == request.blockId) && thisAddress.equals(currentTakeBlock.address) && currentTakeBlock.size() > 0;
  if (valid) {
    Data value=poll(request);
    request.response=value;
    request.key=null;
    request.value=null;
    returnScheduledAsSuccess(request);
    return true;
  }
 else {
    onExpire();
    return false;
  }
}
