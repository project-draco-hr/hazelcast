{
  if (zeroBackup)   return true;
  Block block=getBlock(blockId);
  if (block == null)   return false;
  if (block.address.equals(thisAddress)) {
    return false;
  }
  if (add) {
    boolean added=block.add(addIndex,data);
    return true;
  }
 else {
    if (block.size() > 0) {
      block.remove();
    }
 else {
      return false;
    }
  }
  return true;
}
