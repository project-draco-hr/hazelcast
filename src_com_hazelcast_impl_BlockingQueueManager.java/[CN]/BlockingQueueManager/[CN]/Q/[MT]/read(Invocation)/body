{
  int index=(int)inv.longValue;
  int blockId=inv.blockId;
  Block block=getBlock(blockId);
  inv.longValue=-1;
  if (block != null) {
    blockLoop:     for (int i=index; i < Block.BLOCK_SIZE; i++) {
      Data data=block.get(i);
      if (data != null) {
        inv.doHardCopy(data,inv.data);
        inv.longValue=i;
        break blockLoop;
      }
    }
  }
  sendResponse(inv);
}
