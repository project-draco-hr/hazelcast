{
  addCall(this);
  Q q=getQ(name);
  Address target=q.getBlockOwner(blockId);
  if (target == null) {
    responses.add(OBJECT_NULL);
  }
 else   if (target.equals(thisAddress)) {
    q.read(this);
  }
 else {
    Invocation inv=obtainServiceInvocation();
    inv.name=name;
    inv.operation=getOperation();
    inv.eventId=getId();
    inv.blockId=blockId;
    inv.timeout=0;
    inv.longValue=index;
    boolean sent=send(inv,target);
    if (!sent) {
      inv.returnToContainer();
      onDisconnect(target);
    }
  }
}
