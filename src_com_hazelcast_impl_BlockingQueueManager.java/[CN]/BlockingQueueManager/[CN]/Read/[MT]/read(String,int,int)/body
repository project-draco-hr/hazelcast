{
  this.name=name;
  this.blockId=blockId;
  this.index=index;
  enqueueAndReturn(this);
  Object result;
  try {
    result=responses.take();
    if (result == OBJECT_NULL)     return null;
 else {
      return ThreadContext.get().toObject((Data)result);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return null;
}
