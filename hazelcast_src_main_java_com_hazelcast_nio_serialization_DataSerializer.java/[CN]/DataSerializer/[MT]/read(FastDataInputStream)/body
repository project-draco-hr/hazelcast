{
  final String className=in.readUTF();
  try {
    final DataSerializable ds=(DataSerializable)newInstance(className);
    ds.readData(in);
    return ds;
  }
 catch (  final Exception e) {
    throw new HazelcastSerializationException("Problem while reading DataSerializable class : " + className + ", exception: "+ e);
  }
}
