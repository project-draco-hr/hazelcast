{
  String urlString=webServerUrl + "getTask.do?member=" + address.getHost()+ ":"+ address.getPort()+ "&cluster="+ groupConfig.getName();
  if (projectId != null) {
    urlString+="&projectid=" + projectId;
  }
  if (securityToken != null) {
    urlString+="&securitytoken=" + securityToken;
  }
  return new URL(urlString);
}
