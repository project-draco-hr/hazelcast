{
  String url=cleanupUrl(managementCenterUrl) + "collector.do";
  if (clusterId != null) {
    url+="?clusterid=" + clusterId;
  }
  if (securityToken != null) {
    if (clusterId == null) {
      url+="?securitytoken=" + securityToken;
    }
 else {
      url+="&securitytoken=" + securityToken;
    }
  }
  return new URL(url);
}
