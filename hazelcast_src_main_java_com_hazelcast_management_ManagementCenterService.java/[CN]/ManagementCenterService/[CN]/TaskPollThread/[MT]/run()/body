{
  try {
    while (isRunning()) {
      processTask();
      sleep();
    }
  }
 catch (  Throwable throwable) {
    if (!(throwable instanceof InterruptedException)) {
      inspectOutputMemoryError(throwable);
      logger.warning("Problem on Hazelcast Management Center Service while polling for a task.",throwable);
    }
  }
}
