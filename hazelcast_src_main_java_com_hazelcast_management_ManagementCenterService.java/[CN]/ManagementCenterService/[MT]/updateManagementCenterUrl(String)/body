{
  if (newUrl == null) {
    return;
  }
  managementCenterUrl=newUrl;
  if (newUrl.equals(managementCenterUrl)) {
    return;
  }
  if (!isRunning()) {
    start();
  }
  urlChanged=true;
  logger.info("Management Center URL has changed. " + "Hazelcast will connect to Management Center on address: \n" + managementCenterUrl);
}
