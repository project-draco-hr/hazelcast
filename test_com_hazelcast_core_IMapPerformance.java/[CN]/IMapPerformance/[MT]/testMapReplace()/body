{
  IMap<Integer,String> map=Hazelcast.getMap("testMapReplace");
  long ops=100000;
  for (int i=0; i < ops; ++i) {
    map.put(i,"Hello World");
  }
  Timer t=new Timer(ops);
  for (int i=0; i < ops; ++i) {
    map.replace(i,"FooBar");
  }
  t.stop();
  t.printResult("testMapReplace");
  map.clear();
}
