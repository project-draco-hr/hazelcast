{
  IMap<Integer,String> map=Hazelcast.getMap("testMapRemove");
  long ops=100000;
  for (int i=0; i < ops; ++i) {
    map.put(i,"Hello World");
  }
  Timer t=new Timer(ops / 10);
  for (int i=0; i < ops / 10; ++i) {
    int index=(int)((int)ops * Math.random());
    map.remove(index);
  }
  t.stop();
  t.printResult("testMapRemove");
  map.clear();
}
