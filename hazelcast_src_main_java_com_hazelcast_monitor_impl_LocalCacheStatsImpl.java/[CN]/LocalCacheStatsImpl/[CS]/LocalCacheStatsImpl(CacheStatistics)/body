{
  creationTime=Clock.currentTimeMillis();
  cacheHits=cacheStatistics.getCacheHits();
  cacheHitPercentage=cacheStatistics.getCacheHitPercentage();
  cacheMisses=cacheStatistics.getCacheMisses();
