{
  if (!queue.offer(new WorkerTask(command))) {
    throw new RejectedExecutionException("Executor reached to max capacity!");
  }
  if (spinningThreads.get() < coreThreadSize) {
    lock.lock();
    try {
      signalWorker.signal();
    }
  finally {
      lock.unlock();
    }
  }
  return true;
}
