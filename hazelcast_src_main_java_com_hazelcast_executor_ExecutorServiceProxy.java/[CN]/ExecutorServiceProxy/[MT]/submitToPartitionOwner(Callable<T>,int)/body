{
  if (isShutdown()) {
    throw new RejectedExecutionException(getRejectionMessage());
  }
  final NodeEngine nodeEngine=getNodeEngine();
  Invocation inv=nodeEngine.getOperationService().createInvocationBuilder(DistributedExecutorService.SERVICE_NAME,new CallableTaskOperation<T>(name,task),partitionId).build();
  return new FutureProxy<T>(inv.invoke(),nodeEngine);
}
