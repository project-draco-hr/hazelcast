{
  int result=0;
  for (  OperationHandler handler : partitionOperationHandlers) {
    if (handler.currentTask() != null) {
      result++;
    }
  }
  for (  OperationHandler handler : genericOperationHandlers) {
    if (handler.currentTask() != null) {
      result++;
    }
  }
  return result;
}
