{
  validateXID(xid);
  final Transaction tx=getTransaction();
  if (onePhase) {
    validateTx(xid,Transaction.State.ACTIVE);
    tx.prepare();
  }
  validateTx(xid,Transaction.State.PREPARED);
  try {
    tx.commit();
  }
 catch (  TransactionException e) {
    throw new XAException(XAException.XAER_RMERR);
  }
}
