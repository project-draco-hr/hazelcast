{
  int size=in.readInt();
  for (int i=0; i < size; i++) {
    String className=in.readUTF();
    try {
      RemotelyProcessable rp=(RemotelyProcessable)Class.forName(className).newInstance();
      rp.readData(in);
      lsProcessables.add(rp);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
