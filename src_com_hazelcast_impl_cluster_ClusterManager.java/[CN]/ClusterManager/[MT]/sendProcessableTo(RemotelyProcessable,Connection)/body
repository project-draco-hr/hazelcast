{
  Data value=ThreadContext.get().toData(rp);
  Packet packet=obtainPacket();
  try {
    packet.set("remotelyProcess",OP_REMOTELY_PROCESS,null,value);
    boolean sent=send(packet,conn);
    if (!sent) {
      packet.returnToContainer();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
