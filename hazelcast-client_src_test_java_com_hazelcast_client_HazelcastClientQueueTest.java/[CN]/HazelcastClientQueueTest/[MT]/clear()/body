{
  HazelcastClient hClient=getHazelcastClient();
  IQueue<String> queue=hClient.getQueue("clear");
  List<String> list=new ArrayList<String>();
  list.add("a");
  list.add("b");
  assertTrue(queue.size() == 0);
  assertTrue(queue.addAll(list));
  assertTrue(queue.size() == 2);
  queue.clear();
  assertTrue(queue.size() == 0);
}
