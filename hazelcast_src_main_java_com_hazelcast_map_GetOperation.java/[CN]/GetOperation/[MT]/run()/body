{
  if (txnId != null) {
    final TransactionItem item=partitionContainer.getTransactionItem(new TransactionKey(txnId,name,dataKey));
    if (item != null) {
      if (item.isRemoved()) {
        result=null;
      }
 else {
        result=item.getValue();
      }
      return;
    }
  }
  result=mapService.toData(recordStore.get(dataKey));
}
