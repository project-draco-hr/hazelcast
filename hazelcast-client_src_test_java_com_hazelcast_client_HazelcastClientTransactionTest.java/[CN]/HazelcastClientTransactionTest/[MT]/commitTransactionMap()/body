{
  HazelcastInstance hClient=getHazelcastClient();
  Transaction transaction=hClient.getTransaction();
  transaction.begin();
  Map<String,String> map=hClient.getMap("commitTransactionMap");
  map.put("1","A");
  assertEquals("A",map.get("1"));
  transaction.commit();
  assertEquals("A",map.get("1"));
}
