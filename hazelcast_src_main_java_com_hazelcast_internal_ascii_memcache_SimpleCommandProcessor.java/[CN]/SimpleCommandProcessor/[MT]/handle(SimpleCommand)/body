{
  if (command.getType() == QUIT) {
    try {
      command.getReadHandler().closeConnection();
    }
 catch (    Exception e) {
      logger.warning(e);
    }
  }
 else   if (command.getType() == UNKNOWN) {
    command.setResponse(TextCommandConstants.ERROR);
    textCommandService.sendResponse(command);
  }
}
