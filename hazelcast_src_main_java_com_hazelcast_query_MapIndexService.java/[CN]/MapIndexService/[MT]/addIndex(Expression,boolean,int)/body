{
  Index index=mapIndexes.get(expression);
  if (index == null) {
    if (attributeIndex == -1) {
      attributeIndex=mapIndexes.size();
    }
    index=new Index(expression,ordered,attributeIndex);
    mapIndexes.put(expression,index);
    indexTypes=null;
    hasIndexedAttributes=true;
  }
  return index;
}
