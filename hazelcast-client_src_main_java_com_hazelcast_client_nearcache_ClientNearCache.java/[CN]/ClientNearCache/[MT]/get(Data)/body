{
  fireTtlCleanup();
  CacheRecord record=cache.get(key);
  if (record != null) {
    record.access();
    if (record.expired()) {
      cache.remove(key);
      return null;
    }
    return storageFormat.equals(StorageFormat.HEAP_BINARY) ? context.getSerializationService().toObject((Data)record.value) : record.value;
  }
 else {
    return null;
  }
}
