{
  long ownedEntryCount=0;
  long ownedEntryMemory=0;
  long hits=0;
  for (  CacheRecord record : cache.values()) {
    ownedEntryCount++;
    ownedEntryMemory+=record.getCost();
    hits+=record.hit.get();
  }
  stats.setOwnedEntryCount(ownedEntryCount);
  stats.setOwnedEntryMemoryCost(ownedEntryMemory);
  stats.setHits(hits);
  return stats;
}
