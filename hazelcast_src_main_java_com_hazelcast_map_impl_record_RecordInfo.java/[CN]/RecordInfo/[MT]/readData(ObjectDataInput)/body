{
  boolean statsEnabled=in.readBoolean();
  if (statsEnabled) {
    statistics=new RecordStatisticsImpl();
    statistics.readData(in);
  }
  version=in.readLong();
  hits=in.readLong();
  ttl=in.readLong();
  creationTime=in.readLong();
  lastAccessTime=in.readLong();
  lastUpdateTime=in.readLong();
}
