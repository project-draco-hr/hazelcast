{
  if (visited == BEING_VISITED) {
    return this;
  }
  if (visited == DONE_VISITED) {
    return null;
  }
  visited=BEING_VISITED;
  for (  Edge edge : outgoings) {
    Vertex v2=edge.to.visit(list);
    if (v2 != null) {
      list.add(edge);
      if (v2 == this) {
        throw new RuntimeException("Cycle is detected!");
      }
      return v2;
    }
  }
  visited=DONE_VISITED;
  return null;
}
