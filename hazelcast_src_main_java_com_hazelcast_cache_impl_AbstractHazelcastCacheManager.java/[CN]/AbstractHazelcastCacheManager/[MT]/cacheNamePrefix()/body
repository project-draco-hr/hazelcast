{
  final StringBuilder sb=new StringBuilder("/hz");
  if (!isDefaultURI) {
    sb.append("/").append(uri.toASCIIString());
  }
  final ClassLoader classLoader=getClassLoader();
  if (!isDefaultClassLoader && classLoader != null) {
    sb.append("/").append(classLoader.toString());
  }
  sb.append("/");
  return sb.toString();
}
