{
  Data value=ThreadContext.get().toData(rp);
  Invocation inv=obtainServiceInvocation();
  try {
    inv.set("remotelyProcess",OP_REMOTELY_PROCESS,null,value);
    boolean sent=send(inv,address);
    if (!sent) {
      inv.returnToContainer();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
