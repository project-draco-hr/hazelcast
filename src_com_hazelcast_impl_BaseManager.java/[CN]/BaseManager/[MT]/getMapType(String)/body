{
  byte mapType=MAP_TYPE_MAP;
  if (name.length() > 3) {
    final String typeStr=name.substring(2,4);
    if ("s:".equals(typeStr))     mapType=MAP_TYPE_SET;
 else     if ("l:".equals(typeStr))     mapType=MAP_TYPE_LIST;
 else     if ("u:".equals(typeStr))     mapType=MAP_TYPE_MULTI_MAP;
  }
  return mapType;
}
