{
  try {
    final PacketQueue.Packet packet=PacketQueue.get().obtainPacket();
    packet.set(name,operation,null,ds);
    final boolean sent=send(packet,address);
    if (!sent)     packet.returnToContainer();
    return sent;
  }
 catch (  final Exception e) {
    e.printStackTrace();
  }
  return true;
}
