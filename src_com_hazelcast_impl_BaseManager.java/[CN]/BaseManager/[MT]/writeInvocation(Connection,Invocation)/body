{
  if (!conn.live()) {
    inv.returnToContainer();
    return false;
  }
  final MemberImpl memberImpl=getMember(conn.getEndPoint());
  if (memberImpl != null) {
    memberImpl.didWrite();
  }
  inv.currentCallCount=mapCalls.size();
  inv.write();
  conn.getWriteHandler().enqueueInvocation(inv);
  return true;
}
