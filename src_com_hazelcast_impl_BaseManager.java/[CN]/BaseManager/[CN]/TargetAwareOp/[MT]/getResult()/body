{
  long timeout=request.timeout;
  Object result=null;
  try {
    if (timeout == -1) {
      result=responses.take();
    }
 else {
      result=responses.poll(timeout + TIMEOUT_ADDITION,TimeUnit.MILLISECONDS);
    }
    if (result == OBJECT_REDO) {
      Thread.sleep(2000);
      request.redoCount++;
      doOp();
      return getResult();
    }
  }
 catch (  Exception e) {
    e.printStackTrace(System.out);
  }
  return result;
}
