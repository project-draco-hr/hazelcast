{
  setTarget();
  if (DEBUG) {
  }
  if (target.equals(thisAddress)) {
    doLocalOp();
  }
 else {
    addCall(TargetAwareOp.this);
    Invocation inv=request.toInvocation();
    inv.eventId=getId();
    boolean sent=send(inv,target);
    if (!sent) {
      inv.returnToContainer();
      redo();
    }
  }
}
