{
  Object result=null;
  try {
    result=responses.take();
    if (result == OBJECT_REDO) {
      Thread.sleep(2000);
      request.redoCount++;
      doOp();
      return getResult();
    }
  }
 catch (  final Throwable e) {
    logger.log(Level.FINEST,"ResponseQueueCall.getResult()",e);
  }
  return result;
}
