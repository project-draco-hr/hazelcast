{
  try {
    final Object result=getResult();
    if (result == OBJECT_NULL || result == null) {
      return false;
    }
    if (result == Boolean.TRUE)     return true;
 else     return false;
  }
 catch (  final Exception e) {
    logger.log(Level.SEVERE,"getResultAsBoolean",e);
  }
 finally {
    request.reset();
  }
  return false;
}
