{
  try {
    Object result=doOp();
    if (result == OBJECT_NULL || result == null) {
      return null;
    }
    if (result instanceof Data) {
      Data data=(Data)result;
      if (data.size() == 0)       return null;
      return ThreadContext.get().toObject(data);
    }
    return result;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    request.reset();
  }
  return null;
}
