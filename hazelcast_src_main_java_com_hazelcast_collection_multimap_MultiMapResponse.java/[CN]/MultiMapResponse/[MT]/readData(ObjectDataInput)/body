{
  int size=in.readInt();
  map=new HashMap<Data,Collection>(size);
  for (int i=0; i < size; i++) {
    Data key=IOUtil.readData(in);
    int collSize=in.readInt();
    Collection coll=new ArrayList(collSize);
    for (int j=0; j < collSize; j++) {
      coll.add(IOUtil.readData(in));
    }
    map.put(key,coll);
  }
}
