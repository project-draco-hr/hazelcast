{
  if (key == null) {
    throw new NullPointerException(NULL_KEY_IS_NOT_ALLOWED);
  }
  final Data keyData=toData(key);
  MultiMapLockRequest request=new MultiMapLockRequest(keyData,ThreadUtil.getThreadId(),Long.MAX_VALUE,getTimeInMillis(time,timeunit),name);
  Boolean result=invoke(request,keyData);
  return result;
}
